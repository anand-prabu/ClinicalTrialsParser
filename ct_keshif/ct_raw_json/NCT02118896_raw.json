{
    "clinical_study": {
        "@rank": "163291", 
        "arm_group": {
            "arm_group_label": "1: FK506E (MR4)", 
            "arm_group_type": "Experimental", 
            "description": "Single open arm of FK506E (MR4)"
        }, 
        "brief_summary": {
            "textblock": "The purpose of this study was to offer patients who had participated in one of the phase II\n      PK or phase III studies on FK506E (MR4) the possibility to continue FK506E (MR4) until\n      commercial availability of the drug and to record long term efficacy and safety data."
        }, 
        "brief_title": "This Study is to Ascertain if Prolonged Release Tacrolimus (FK506E - MR4) is Safe and Effective When Used in the Long Term and in Combination With Other Immunosuppressive Drugs in Patients Who Have Received a Transplant", 
        "completion_date": {
            "#text": "October 2009", 
            "@type": "Actual"
        }, 
        "condition": [
            "Kidney Transplantation", 
            "Liver Transplantation", 
            "Heart Transplantation"
        ], 
        "detailed_description": {
            "textblock": "Only patients who have participated in one of the phase II PK or phase III studies on FK506E\n      (MR4) and have received at least one dose of study medication will be enrolled."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Patients who had already participated in the previous phase II pharmacokinetic or\n             phase III studies with FK506E (MR4).\n\n          -  Patients capable of understanding the purpose and risks of the study, who have been\n             fully informed and given written informed consent to participate in the study.\n\n        Exclusion Criteria:\n\n          -  Pregnant women or nursing mothers.\n\n          -  Women unwilling or unable to use adequate contraception during the study."
            }, 

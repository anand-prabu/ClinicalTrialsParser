    "clinical_study": {
        "@rank": "80689", 
        "acronym": "LoopKeta", 
        "arm_group": [
            {
                "arm_group_label": "Ketamine", 
                "arm_group_type": "Experimental", 
                "description": "Patients will receive a closed-loop administration of propofol and remifentanil according to bispectral level and Ketamine (bolus dose of Ketamine (1.5 mg.kg-1) followed by an infusion of Ketamine (2 mg.kg-1 h-1) until the end of anesthesia)"
            }, 
            {
                "arm_group_label": "Placebo", 
                "arm_group_type": "Experimental", 
                "description": "patients will receive a closed-loop administration of propofol and remifentanil according to bispectral level and a placebo (NaCl 9/00 (same volume as in the Ketamine group)"
            }
        ], 
        "brief_summary": {
            "textblock": "The objective is to evaluate the sparing effect of ketamine on the dose of propofol."
        }, 
        "brief_title": "Ketamine and Closed-Loop Anesthesia System", 
        "completion_date": {
            "#text": "July 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Anesthesia", 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  patients with an American Society of Anesthesiology score 1, 2 or 3\n\n          -  scheduled for a surgical procedure lasting more than 2 hours under general anesthesia\n\n        Exclusion Criteria:\n\n          -  pregnancy\n\n          -  chronic pain\n\n          -  simultaneous general and loco-regional anesthesia\n\n          -  contra-indication to to nonsteroidal anti-inflammatory drug\n\n          -  contra-indication to Ketamine\n\n          -  contra-indication to propofol, to remifentanil, to morphine\n\n          -  history of central nervous system disease\n\n          -  patients receiving a psychotropic treatment\n\n          -  patients with a pace-maker"
            }, 

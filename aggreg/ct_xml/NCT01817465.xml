  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Roman III criteria

          -  One or more conditions are applied: epigastralgia, burning feelings in the solar
             plexus  early satiety, uncomfortable fullness

          -  No organic lesion

        Exclusion Criteria:

          -  has been administered or was administered within a month

          -  had a surgery that might affect gastrointestinal motility

          -  Tegaserod
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>20 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>

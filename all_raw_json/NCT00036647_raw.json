{
    "clinical_study": {
        "@rank": "79195", 
        "brief_summary": {
            "textblock": "The purpose of this study is to determine if OSI-774 will improve overall survival of\n      patients with incurable stage IIIB/IV non-small cell lung cancer compared to standard of\n      care.  OSI-774 is a new type of drug under evaluation called an epidermal growth factor\n      receptor (EGFR).  OSI-774 is an investigational drug that has not yet been approved by the\n      U.S. Food and Drug Administration (FDA)."
        }, 
        "brief_title": "OSI-774 (Tarceva) in Treating Patients With Stage III or Stage IV Non-Small Cell Lung Cancer", 
        "completion_date": {
            "#text": "January 2003", 
            "@type": "Actual"
        }, 
        "condition": "Carcinoma, Non-small-cell Lung", 
        "condition_browse": {
            "mesh_term": [
                "Carcinoma", 
                "Carcinoma, Non-Small-Cell Lung", 
                "Lung Neoplasms"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "-  Clinical diagnosis of stage IIIB or IV non-small cell lung cancer.\n\n          -  Must have evidence of disease (clinical or radiological).\n\n          -  Have failed 1 but no more than 2 prior chemotherapy regimens, have recovered from any\n             side effects and have not had any chemotherapy for at least 21 days.\n\n          -  If the patient has had surgery, the surgery was at least 2 weeks ago.\n\n          -  Patients whose cancer has spread to their brain or central nervous system are\n             eligible, providing that they have been on stable dose of steroids for at least 4\n             weeks and are free of symptoms.\n\n          -  If the patient received radiation therapy, treatment was at least 4 weeks ago."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

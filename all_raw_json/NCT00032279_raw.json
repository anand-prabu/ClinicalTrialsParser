    "clinical_study": {
        "@rank": "133072", 
        "brief_summary": {
            "textblock": "The purpose of this phase II study is to evaluate an investigational monoclonal antibody for\n      the treatment of glucocorticoid-refractory Graft Versus Host Disease (GVHD).  Patients\n      diagnosed with GVHD who have not responded satisfactorily to, or are intolerant of,\n      treatment with standard agents will be considered for entry. Patients will be allowed to\n      continue on their other immunosuppressive drugs at stable doses during the trial. The\n      research is being conducted at up to 20 clinical research sites in the US."
        }, 
        "brief_title": "Research Study of Visilizumab for Treatment of Glucocorticoid- Refractory Graft Versus Host Disease", 
        "condition": "Graft-vs-Host Disease", 
        "condition_browse": {
            "mesh_term": "Graft vs Host Disease"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "-  Grade II to IV GVHD\n\n          -  Clinical findings: skin thickening, joint contraction, oral ulceration, diarrhea.\n\n          -  History of allogeneic hematopoietic cell transplant (HCT).\n\n          -  Patients must have failed to respond or clearly progress to previous therapy on or\n             before day +100 posttransplant.\n\n          -  Patients must have adequate renal, hepatic, cardiac function and hematologic values"
            }, 

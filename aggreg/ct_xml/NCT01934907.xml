  <brief_title>Transfusion Reaction by Washed Red Blood Cell (RBC)</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Bundang Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>

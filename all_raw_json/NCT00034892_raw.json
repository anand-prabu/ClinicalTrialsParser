    "clinical_study": {
        "@rank": "154974", 
        "brief_summary": {
            "textblock": "The purpose of this study is to compare the effectiveness, tolerability, and efficacy of the\n      currently available atypical antipsychotic drugs olanzapine (2.5-20 mg/day), quetiapine\n      (100-800 mg/day) and risperidone (0.5-4 mg/day) in patients with schizophrenia,\n      schizophreniform disorder, or schizoaffective disorder who are experiencing their first\n      psychotic episode."
        }, 
        "brief_title": "CAFE Comparison of Atypicals in First Episode of Psychosis", 
        "completion_date": {
            "#text": "March 2005", 
            "@type": "Actual"
        }, 

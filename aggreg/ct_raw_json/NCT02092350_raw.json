        "@rank": "85815", 
        "arm_group": [
            {
                "arm_group_label": "Immediate Treatment", 
                "arm_group_type": "Experimental", 
                "description": "Participants receive MK-5172 100 mg tablet, orally, once per day (QD) + MK-8742 50 mg tablet, orally, QD, for 12 weeks"
            }, 
            {
                "arm_group_label": "Deferred Treatment", 
                "arm_group_type": "Experimental", 
                "description": "Participants receive placebo to MK-5172 and MK-8742 for 12 weeks, and after a 4-week break, MK-5172 100 mg tablet, orally, QD + MK-8742 50 mg tablet, orally, QD for 12 weeks"
            }, 
            {
                "arm_group_label": "Immediate Treatment/PK Testing", 
                "arm_group_type": "Experimental", 
                "description": "Participants receive MK-5172 100 mg tablet, orally, once per day (QD) + MK-8742 50 mg tablet, orally, QD for 12 weeks with intensive pharmacokinetic testing"
            }
        ], 
        "brief_summary": {
            "textblock": "This study will evaluate the safety and efficacy of combination treatment with MK-5172 +\n      MK-8742 for non-cirrhotic participants with chronic Genotype 1 (GT1) hepatitis C virus (HCV)\n      infection and chronic kidney disease (CKD).  After a screening period, participants will\n      receive random assignment to one of two study arms (immediate treatment or deferred\n      treatment,) or assignment to open-label immediate treatment with intensive pharmacokinetic\n      sampling.  The primary study hypothesis is that the proportion of participants achieving a\n      sustained viral response 12 weeks after the end of all study treatment (SVR12) will be >45%."
        }, 
        "brief_title": "Safety and Efficacy of MK-5172 + MK-8742 in Participants With Chronic Hepatitis C and Chronic Kidney Disease (MK-5172-052/C-SURFER)", 
        "completion_date": {
            "#text": "August 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Hepatitis C Virus", 
        "condition_browse": {
            "mesh_term": [
                "Hepatitis", 

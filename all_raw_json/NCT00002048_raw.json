    "clinical_study": {
        "@rank": "80824", 
        "brief_summary": {
            "textblock": "To evaluate the safety and tolerance of chronic administration of Retrovir (AZT) in\n      HIV-infected adult patients without clinical manifestations of disease. To assess the\n      efficacy of AZT therapy in the treatment of HIV disease in these patients."
        }, 
        "brief_title": "The Efficacy and Safety of Retrovir in Patients Infected With HIV Who Are Asymptomatic (CDC Group II) or Have Persistent Generalized Lymphadenopathy (CDC Group III)", 
        "condition": [
            "Lymphatic Disease", 
            "HIV Infections"
        ], 
        "condition_browse": {

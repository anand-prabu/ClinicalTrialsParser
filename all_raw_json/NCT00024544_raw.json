    "clinical_study": {
        "@rank": "74138", 
        "brief_summary": {
            "textblock": "The purpose of the study is to evaluate an investigational medication to treat chronic\n      asthma.  The research is being conducted at 20 clinical research sites in the US and is open\n      to both men and women ages 18 to 70 years.  Participants in the study will have a number of\n      visits to a research site over an 8-month period. All study-related care and medication is\n      provided to qualified participants at no cost. This includes all visits, examinations and\n      laboratory work."
        }, 
        "brief_title": "Pilot Study in Patients With Symptomatic Steroid-Naive Asthma", 
        "completion_date": {
            "#text": "February 2003", 
            "@type": "Actual"
        }, 
        "condition": "Asthma", 
        "condition_browse": {
            "mesh_term": "Asthma"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  At least a 6-month history of asthma\n\n          -  No use of inhaled, oral or injectable steroids for at least 1 month prior to\n             enrollment\n\n        Exclusion Criteria:\n\n          -  Patients with > 10 pack years of smoking history or smoking within 12 months of study\n             enrollment\n\n          -  No significant disease other than asthma"
            }, 

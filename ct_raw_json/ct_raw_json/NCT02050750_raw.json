        "arm_group": {
            "arm_group_label": "TAILORx ICORG 06-31", 
            "description": "Patients must have participated in TAILORx ICORG 06-31, participated in trial arms, and have adequate tumour tissue available"
        }, 
        "brief_summary": {
            "textblock": "This is an exploratory, translational, non-interventional and multi-centre biobank.  The aim\n      of the development of such a biobank is to identify potential biomarkers that are indicative\n      of disease relapse."
        }, 
        "brief_title": "TAILORx Tissue Bank", 
        "condition": "Breast Cancer", 
        "condition_browse": {
            "mesh_term": "Breast Neoplasms"
        }, 
        "detailed_description": {
            "textblock": "The primary objective is to develop a biobank, tissue microarrays (TMAs) and full face\n      sections, which will be used to identify candidate/ novel biomarkers prognostic for disease\n      relapse and predictive for endocrine and/or chemotherapy resistance.\n\n      The secondary objectives are:\n\n        -  To validate emerging signatures/biomarkers identified.\n\n        -  To develop new assays for the breast cancer clinic."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Patients that were registered with the ICORG 06-31 TAILORx trial and participated in\n             trial arms\n\n          -  Patients with adequate tumor tissue available"
            }, 
            "gender": "Female", 
            "healthy_volunteers": "No", 
            "maximum_age": "75 Years", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "The study population will consist only of patients that participated in the TAILORx ICORG\n        06-31 trial.  These patients must have participated in trial arms relevant to ICORG 06-31\n        study and have adequate tumour tissue available."
            }
        }, 

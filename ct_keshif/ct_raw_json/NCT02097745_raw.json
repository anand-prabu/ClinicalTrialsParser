{
    "clinical_study": {
        "@rank": "77383", 
        "arm_group": {
            "arm_group_label": "MabThera/Rituxan", 
            "arm_group_type": "Experimental"
        }, 
        "brief_summary": {
            "textblock": "This study will assess the long-term safety and efficacy of repeating treatment with\n      MabThera, in combination with methotrexate and steroids, in patients who were previously\n      randomized into MabThera study WA17042. The anticipated time on study treatment is until\n      Mabthera is available on the local market and the target sample size is 100-500 individuals."
        }, 
        "brief_title": "A Study of the Efficacy and Safety of Re-treatments With Rituximab in Patients With Active Rheumatoid Arthritis Who Have Had an Inadequate Response to Anti-tnfa Therapies", 
        "completion_date": {
            "#text": "June 2013", 
            "@type": "Actual"
        }, 
        "condition": "Rheumatoid Arthritis", 
        "condition_browse": {
            "mesh_term": [
                "Arthritis", 
                "Arthritis, Rheumatoid"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  adult patients 18-80 years of age with active RA;\n\n          -  patients who participated in MabThera study WA17042, and completed to at least the\n             Week 24 visit;\n\n          -  eligible for re-treatment, based on clinical symptoms;\n\n          -  patients of reproductive potential must be using reliable contraceptive methods.\n\n        Exclusion Criteria:\n\n          -  patients who participated in MabThera study WA17042 but withdrew into the safety\n             follow-up;\n\n          -  current treatment with any other disease-modifying antirheumatic drug (apart from\n             methotrexate), or any anti-TNFalfa, anti-IL1, or other biologic therapies;\n\n          -  development of any new contraindications to receiving MabThera;\n\n          -  women who are pregnant or breast-feeding."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "80 Years", 
            "minimum_age": "18 Years"
        }, 
        "enrollment": {
            "#text": "341", 
            "@type": "Actual"
        }, 
        "firstreceived_date": "March 20, 2014", 
        "has_expanded_access": "No", 

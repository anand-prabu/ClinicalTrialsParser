        "arm_group": [
            {
                "arm_group_label": "NT-501 Implant", 
                "arm_group_type": "Experimental", 
                "description": "The investigational product is the NT-501 encapsulated cell system which consists of cells encapsulated within a semi-permeable polymer membrane and supportive matrices. NT-501 contains NTC-201 cells that were derived from the NTC-200 cell line by genetic modification so as to secrete recombinant human ciliary neurotrophic factor (CNTF)."
            }, 
            {
                "arm_group_label": "Sham procedure", 
                "arm_group_type": "Sham Comparator", 
                "description": "Non-penetrating sham procedure to mimic implant procedure"
            }
        ], 
        "brief_summary": {
            "textblock": "This study is a phase 2, randomized, multi-center, single-masked study to evaluate the\n      efficacy and safety of the NT-501 implants in participants with Mactel."
        }, 
        "brief_title": "A Phase 2 Multicenter Randomized Clinical Trial of Ciliary Neurotrophic Factor (CNTF) for Macular Telangiectasia Type 2 (MacTel)", 

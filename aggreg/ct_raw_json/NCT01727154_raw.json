{
    "clinical_study": {
        "@rank": "92273", 
        "acronym": "PRIME", 
        "arm_group": {
            "arm_group_label": "Sipuleucel-T"
        }, 
        "biospec_descr": {
            "textblock": "Whole blood, serum, manufacturing samples"
        }, 
        "biospec_retention": "Samples With DNA", 
        "brief_summary": {
            "textblock": "The purpose of this study is to evaluate the immune response induced by sipuleucel-T\n      (Provenge\u00ae)."
        }, 
        "brief_title": "Immune Monitoring Protocol in Men With Prostate Cancer Enrolled in a Clinical Trial of Sipuleucel-T", 
        "condition": "Prostate Cancer", 
        "condition_browse": {
            "mesh_term": "Prostatic Neoplasms"
        }, 
        "detailed_description": {
            "textblock": "Subjects will receive sipuleucel-T, and potentially other medications, as part of the\n      clinical trial in which they are concurrently enrolled. For this study, cellular and humoral\n      immune responses will be assessed. No additional study treatments will be conducted beyond\n      what is required for the subject's concurrent clinical trial."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Subjects must be at least 18 years of age\n\n          -  Subjects with prostate cancer who are enrolled in a clinical trial of sipuleucel-T\n             (including a Dendreon-sponsored clinical trial or registry, or an IIT)\n\n          -  Subjects have not yet undergone leukapheresis for their first dose of sipuleucel-T\n\n          -  Subjects must understand and sign an informed consent form prior to their first\n             leukapheresis\n\n        Exclusion Criteria:\n\n        \u2022None"
            }, 
            "gender": "Male", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Men at least 18 years of age with prostate cancer who are enrolled in a clinical trial of\n        sipuleucel-T (including a Dendreon-sponsored clinical trial or registry, or an IIT), and\n        have not yet undergone leukapheresis for their first dose of sipuleucel-T."
            }
        }, 
        "enrollment": {
            "#text": "350", 

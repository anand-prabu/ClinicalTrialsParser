  <study_design>Observational Model: Cohort</study_design>
  <target_duration>3 Years</target_duration>
  <primary_outcome>
    <measure>Number and severity of events reported for the Lenus Pro in a clinical environment.</measure>
    <time_frame>Up to 3,5 years</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <primary_outcome>
    <measure>Changes in baseline variables during follow-up on quality of life.</measure>
    <time_frame>Up to 3,5 years</time_frame>
    <safety_issue>No</safety_issue>
    <description>Patient reported outcome measures, mainly CAMPHOR and SF-36</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Description of baseline and follow-up of hemodynamic and functional capacity.</measure>
    <time_frame>Up to 3,5 years</time_frame>
    <safety_issue>No</safety_issue>
    <description>Mainly right hearth catheterization, echo and 6MWD data.</description>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">25</enrollment>
  <condition>Pulmonary Hypertension Group 1 and 4 According to Nice Classification.</condition>
  <arm_group>
    <arm_group_label>Patient with LenusPro pump</arm_group_label>
  </arm_group>
  <eligibility>
    <study_pop>
      <textblock>
        Patients in Scandinavia who received a LenusPro pump
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  The patient should have been diagnosed with Pulmonary Arterial Hypertension

          -  The patient has received or will receive treatment with the implanted pump LenusPro

        Exclusion Criteria:

          -  The patient is participating in another study which means treatment of the patient no
             longer follows the normal clinical practice
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>

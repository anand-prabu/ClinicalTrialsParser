  <primary_outcome>
    <measure>Change from baseline in 1mm central retinal thickness as measured by SD-OCT</measure>
    <time_frame>Baseline and 4 weeks</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in neovascular leakage by fluorescein angiography from baseline</measure>
    <time_frame>Baseline and 4 weeks</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>

        "brief_summary": {
            "textblock": "This is an open-label, multicenter, phase 2 clinical trial designed to evaluate the efficacy\n      and safety of brentuximab vedotin as a single-agent (Part A) and in combination with\n      dacarbazine (Part B) in front-line therapy of HL in adults age 60 and above."
        }, 
        "brief_title": "A Study of Brentuximab Vedotin in Adults Age 60 and Above With Newly Diagnosed Hodgkin Lymphoma (HL)", 
        "completion_date": {
            "#text": "December 2017", 
            "@type": "Anticipated"
        }, 
        "condition": "Hodgkin Disease", 
        "condition_browse": {
            "mesh_term": [
                "Hodgkin Disease", 
                "Lymphoma"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Histopathologically-confirmed diagnosis of classical Hodgkin lymphoma\n\n          -  Ineligible for or have declined initial conventional combination chemotherapy\n\n          -  Measurable disease of at least 1.5 cm as documented by radiographic technique\n\n          -  ECOG performance status less than or equal to 3\n\n        Exclusion Criteria:\n\n          -  Symptomatic neurologic disease compromising instrumental activities of daily living\n             or requiring medication\n\n          -  Concurrent use of other investigational agents\n\n          -  Chemotherapy, radiotherapy, biologics, and/or other treatment with immunotherapy not\n             completed 4 weeks prior to first dose of study drug"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "60 Years"
        }, 

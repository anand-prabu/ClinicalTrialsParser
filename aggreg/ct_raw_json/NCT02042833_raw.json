{
    "clinical_study": {
        "@rank": "57121", 
        "arm_group": {
            "arm_group_label": "Cohort"
        }, 
        "brief_summary": {
            "textblock": "This observational study will evaluate patients' perception of treatment with\n      erythropoiesis-stimulating agents in patients with chronic kidney disease not on dialysis.\n      Eligible patients initiated on treatment with Mircera (methoxy polyethylene glycol-epoetin\n      beta) will be followed for 6 months."
        }, 
        "brief_title": "An Observational Study of Patients' Perception of Treatment With Erythropoiesis-Stimulating Agents in Patients With Chronic Kidney Disease Not on Dialysis (PERCEPOLIS)", 
        "completion_date": {
            "#text": "June 2012", 
            "@type": "Actual"
        }, 
        "condition": "Anemia", 
        "condition_browse": {
            "mesh_term": [
                "Anemia", 
                "Renal Insufficiency, Chronic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Adult patients, >/= 18 years of age\n\n          -  Patients with chronic kidney disease not on dialysis (with or without previous renal\n             transplantation)\n\n          -  Treated or not with ESA at time of visit\n\n          -  Patients for whom the treating physician had decided to initiate Mircera for medical\n             reasons before the commencement of the study\n\n          -  Initiation of treatment with Mircera at the inclusion visit\n\n          -  Accepting and able of completing the ESA preference questionnaire\n\n        Exclusion Criteria:\n\n          -  Participation in a clinical trial on anemia\n\n          -  Treatment with Mircera during the 3 months before inclusion\n\n          -  Dialysis planned in the next 6 months"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Probability Sample", 
            "study_pop": {
                "textblock": "Patients with chronic kidney disease not on dialysis (with or without previous renal\n        transplantation), initiating treatment with Mircera, and erythropoiesis-stimulating agent\n        (ESA)-na\u00efve or not"
            }
        }, 

        "arm_group": [
            {
                "arm_group_label": "LDV/SOF 12 weeks", 
                "arm_group_type": "Experimental", 
                "description": "Participants will receive LDV/SOF for 12 weeks."
            }, 
            {
                "arm_group_label": "Retreatment substudy", 
                "arm_group_type": "Experimental", 
                "description": "Participants in the retreatment substudy will receive LDV/SOF plus RBV for 24 weeks."
            }
        ], 
        "brief_summary": {
            "textblock": "This study will evaluate the antiviral efficacy, safety, and tolerability of\n      ledipasvir/sofosbuvir (LDV/SOF) fixed-dose combination (FDC) administered for 12 weeks in\n      hepatitis C virus (HCV) treatment-naive and treatment-experienced (including treatment\n      intolerant) participants with chronic genotype 1 or 4 HCV infection who are co-infected with\n      HIV-1.\n\n      Participants who experience confirmed post-treatment virologic failure (relapse) at or\n      before posttreatment Week 24 may be eligible to be enrolled in the Retreatment Substudy."
        }, 
        "brief_title": "Efficacy and Safety of Ledipasvir/Sofosbuvir Fixed-Dose Combination for 12 Weeks in Subjects With Chronic Genotype 1 or 4 HCV and HIV-1 Co-infection", 
        "completion_date": {
            "#text": "June 2016", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Hepatitis C Virus", 
            "HIV"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Acquired Immunodeficiency Syndrome", 
                "HIV Infections", 
                "Hepatitis", 
                "Hepatitis A", 
                "Hepatitis C"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  HCV RNA \u2265 10,000 IU/mL at screening\n\n          -  HCV genotype 1 or 4\n\n          -  HIV-1 infection\n\n          -  Cirrhosis determination, a fibroscan or liver biopsy may be required\n\n          -  Screening laboratory values within defined thresholds\n\n          -  Use of protocol specified method(s) of contraception if female of childbearing\n             potential or sexually active male\n\n        Exclusion Criteria:\n\n          -  Clinically-significant illness (other than HCV or HIV) or any other major medical\n             disorder that may interfere with subject treatment, assessment or compliance with the\n             protocol\n\n          -  Current or prior history of Clinical hepatic decompensation Hepatocellular carcinoma\n             (HCC) or other Malignancy (with the exception of certain resolved skin cancers)\n\n          -  Hepatitis B virus (HBV) infection\n\n          -  Pregnant or nursing female\n\n          -  Chronic use of systemically administered immunosuppressive agents"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

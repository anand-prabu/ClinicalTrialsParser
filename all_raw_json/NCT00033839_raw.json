            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "-  At least 2 year  history of episodic cluster headache (meeting IHS criteria)\n\n          -  At least 2 previous episodes\n\n          -  Expected duration of cluster period is at least 6 weeks but not longer than 16 weeks\n\n          -  At least 1 but not more than 8 headaches on each of the 3 days immediately prior to\n             treatment"
            }, 

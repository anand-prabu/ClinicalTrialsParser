            "secondary_id": "EudraCT 2013-000423-14"
        }, 
        "intervention": [
            {
                "arm_group_label": "Selumetinib", 
                "description": "3 capsules of 25 mg strength orally twice a day for approximately 5 weeks treatment period", 
                "intervention_name": "Selumetinib", 
                "intervention_type": "Drug"
            }, 
            {
                "arm_group_label": "Placebo", 
                "description": "3 capsules ( to match Selumetinib capsules) orally twice a day for approximately 5 weeks treatment period", 
                "intervention_name": "Placebo", 
                "intervention_type": "Drug"
            }, 
            {
                "arm_group_label": [
                    "Selumetinib", 
                    "Placebo"
                ], 
                "description": "A single oral radioactive iodine dose of 100 mCI(3.7 GBq) 131I (+/-10% at the time of administration)to be administered 30 days after randomization. Additionaly, Thyrogen (Recombinant human TSH) will be used to stimulate iodine uptake according to the manufacturer's recommendation(0.9 mg intramuscular injection once a day for the 2 days prior to the dose of radioactive iodine)", 
                "intervention_name": "Radioactive Iodine Therapy", 
                "intervention_type": "Drug"
            }
        ], 

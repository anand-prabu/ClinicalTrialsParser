  <brief_title>Tornier Shoulder Outcomes Study to Examine Safety and Functional Outcomes in Shoulder Arthroplasty and Fracture Patients</brief_title>
  <official_title>Tornier Shoulder Outcomes Clinical Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Tornier</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Tornier</source>
  <oversight_info>
    <authority>United States: Federal Government</authority>

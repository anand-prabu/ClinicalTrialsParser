        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  age > 18\n\n          -  signed inform consent\n\n          -  first trimester\n\n          -  two early miscarriages(before week 10) or one later abortions (after week 10)\n\n        Exclusion Criteria:\n\n          -  Chronic treatment with aspirin or other anticoagulants"
            }, 

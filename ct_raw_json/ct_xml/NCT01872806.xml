  <brief_title>Electrophysiological Reactivity Due to Mobile Phone Radiation</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Maastricht University Medical Center</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>

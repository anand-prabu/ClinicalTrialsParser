{
    "clinical_study": {
        "@rank": "164743", 
        "acronym": "GESTURE", 
        "arm_group": [
            {
                "arm_group_label": "secukinumab 150mg", 
                "arm_group_type": "Experimental", 
                "description": "201 subjects with moderate or severe plaque type psoriasis and meeting all inclusion and exclusion criteria will be randomly assigned using a 1:1:1 ratio to one of the following treatment arms: secukinumab 150 mg, secukinumab 300 mg, or placebo. Subjects assigned to secukinumab will be dosed weekly for the first five weeks and then every four weeks up to and including Week 76. In order to maintain the blinding, subjects will receive additional placebo injections at Weeks 17, 18 and 19. All doses consist of sub-cutaneous injections."
            }, 
            {
                "arm_group_label": "secukinumab 300 mg", 
                "arm_group_type": "Experimental", 
                "description": "201 subjects with moderate or severe plaque type psoriasis and meeting all inclusion and exclusion criteria will be randomly assigned using a 1:1:1 ratio to one of the following treatment arms: secukinumab 150 mg, secukinumab 300 mg, or placebo. Subjects assigned to secukinumab will be dosed weekly for the first five weeks and then every four weeks up to and including Week 76. In order to maintain the blinding, subjects will receive additional placebo injections at Weeks 17, 18 and 19. All doses consist of sub-cutaneous injections."
            }, 
            {
                "arm_group_label": "Placebo", 
                "arm_group_type": "Placebo Comparator", 
                "description": "Subjects assigned to the placebo group will dose weekly for the first five weeks and then every four weeks up to Week 16. Prior to taking the Week 16 dose, the subject will be assessed for response to treatment using the palmoplantar Investigator Global Assessment scale. If the subject is a responder, the subject will continue on placebo for five weeks starting on Week 16 and then every four weeks up to and including Week 76. Subjects who are not responders will be randomized to either the secukinumab 150 mg or secukinumab 300mg treatment arms and dose weekly for five weeks starting on Week 16 and then every four weeks up to and including Week 76. All doses consist of sub-cutaneous injections."
            }
        ], 
        "brief_summary": {
            "textblock": "To demonstrate the efficacy of secukinumab versus placebo on palmoplantar psoriasis and to\n      assess long term efficacy, safety and tolerability of secukinumab."
        }, 
        "brief_title": "Study of Safety, Tolerability, and Efficacy of Secukinumab in Subjects With Moderate to Severe Palmoplantar Psoriasis", 
        "completion_date": {
            "#text": "November 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Moderate to Severe Palmoplantar Psoriasis", 
        "condition_browse": {
            "mesh_term": "Psoriasis"
        }, 
        "detailed_description": {
            "textblock": "It is a randomized, double-blind, placebo-controlled, multicenter study to demonstrate the\n      efficacy at 16 weeks of secukinumab 150 and 300 mg s.c. and to assess safety, tolerability\n      and long-term efficacy up to 80 weeks in subjects with moderate to severe palmoplantar\n      psoriasis"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Subjects should have chronic moderate to severe plaque type psoriasis for at least  6\n             months prior to entering the study, including  significant involvement of plaque type\n             psoriasis on the palms and soles defined by a palmoplantar Investigator Global\n             Assessment (ppIGA) score of \u2265 3 (on a 5-point scale) and at least one extra\n             palmoplantar plaque on the skin\n\n          -  Subjects should be candidates for systemic therapy, which means having psoriasis\n             considered inadequately controlled by topical treatment (including super potent\n             topical corticosteroid) and/or phototherapy and/or previous systemic therapy\n\n        Exclusion Criteria:\n\n          -  Forms of psoriasis other than chronic plaque type psoriasis (e.g., pustular\n             psoriasis, palmoplantar pustulosis, acrodermatitis of Hallopeau, erythrodermic and\n             guttate psoriasis)\n\n          -  Drug-induced psoriasis (e.g. new onset or current exacerbation from \u03b2-blockers,\n             calcium channel inhibitors or lithium)\n\n          -  Ongoing use of prohibited treatments (e.g. topical or systemic corticosteroids (CS),\n             UV therapy). Washout periods have to be adhered to Table 5-1\n\n          -  Prior exposure to secukinumab (AIN457) or any other biologic drug directly targeting\n             IL-17 or IL-17 Receptor\n\n          -  Receipt of any investigational drugs within 4 weeks of study drug initiation or\n             within a period of 5 half-lives of the investigational treatment, whichever is longer\n\n          -  Active ongoing inflammatory diseases other than psoriasis that might confound the\n             evaluation of the benefit of secukinumab therapy\n\n          -  History of hypersensitivity to constituents of the study treatment\n\n          -  Other protocol-defined inclusion/exclusion criteria may apply"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"
        }, 

  <brief_title>ACT01 in Combination With Comp01-04 in Patients With Actinic Keratosis</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Promius Pharma, LLC</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>

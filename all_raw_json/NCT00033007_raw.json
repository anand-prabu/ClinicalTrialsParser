    "clinical_study": {
        "@rank": "11854", 
        "brief_summary": {
            "textblock": "The purpose of this study is to test the utilization of incentive values (considerably lower\n      than those typically used in research clinics) to motivate clients to attend treatment and\n      initiate and sustain abstinence."
        }, 
        "brief_title": "Motivational Incentives for Enhanced Drug Abuse Recovery: Drug Free Clinics - 1", 
        "completion_date": {
            "#text": "February 2003", 
            "@type": "Actual"
        }, 
        "condition": "Substance-Related Disorders", 
        "condition_browse": {
            "mesh_term": "Substance-Related Disorders"
        }, 
        "detailed_description": {
            "textblock": "The purpose of the proposed research is to implement and systematically evaluate, in\n      community treatment settings, motivational incentive procedures that have been well\n      researched and have proven efficacious in a variety of treatment research clinics."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criterea:\n\n          -  New intake to a drug-free CTP\n\n          -  Evidence of cocaine or methamphetamine use, any reported use in past 2 weeks or\n             positive urine screen at study intake.  For those exiting a controlled environment,\n             any stimulant use within two weeks of entering the controlled environment\n\n        Exclusion Criteria:\n\n          -  Unable to give informed consent (fails simple consent quiz)\n\n          -  Answers yes to question: Are you in recovery from gambling?  That is, have you\n             stopped gambling because of previous problems with gambling?"
            }, 

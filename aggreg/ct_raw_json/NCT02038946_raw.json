        }, 
        "responsible_party": {
            "responsible_party_type": "Sponsor"
        }, 
        "secondary_outcome": [
            {
                "description": "DOR is defined as the time from first remission (CR or PR) to the date of initial objectively documented progression as determined using the revised International Working Group Criteria for non-Hodgkin Lymphoma or death due to any cause, whichever occurs first", 
                "measure": "The duration of response (DOR) based on IRRC assessments", 
                "safety_issue": "No", 
                "time_frame": "Approximately up to 24 months"
            }, 
            {
                "description": "CRR is defined as the number of subjects with a BOR of CR according to the revised International Working Group Criteria for non-Hodgkin Lymphoma, divided by the number of treated subjects", 
                "measure": "The complete remission rate (CRR) based on IRRC assessment", 
                "safety_issue": "No", 
                "time_frame": "Approximately up to 24 months"
            }, 
            {
                "description": "PFS is defined as the time from first dosing date to the date of the first documented progression, as determined by an IRRC, or death due to any cause, whichever occurs first", 
                "measure": "The progression free survival (PFS) based on IRRC assessment", 
                "safety_issue": "No", 
                "time_frame": "Approximately up to 24 months"
            }, 
            {
                "measure": "Overall response rate based on investigator assessments", 
                "safety_issue": "No", 
                "time_frame": "Approximately up to 24 months"
            }
        ], 

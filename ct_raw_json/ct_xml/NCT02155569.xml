  <brief_title>Extraperitoneal Versus Transperitoneal Cesarean Section</brief_title>
  <official_title>Extraperitoneal Versus Transperitoneal Cesarean Section: A Prospective Randomized Comparison</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Ataturk University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>

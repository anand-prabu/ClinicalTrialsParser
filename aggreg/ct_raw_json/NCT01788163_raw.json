    "clinical_study": {
        "@rank": "107493", 
        "acronym": "IGNITE", 
        "arm_group": {
            "arm_group_label": "Locally advanced/metastatic NSCLC pats.", 
            "arm_group_type": "Other", 
            "description": "Patients with locally advanced (stage IIIA/B) or metastatic NSCLC who have not received any local or systemic chemotherapy, and are not eligible for curative treatment (including surgery and chemoradiotherapy)"
        }, 
        "brief_summary": {
            "textblock": "Interventional diagnostic, international, multicenter and non-comparative study of EGFR\n      mutation status in aNSCLC patients (locally advanced and/or metastatic disease) with\n      adenocarcinoma and non-adenocarcinoma histologies. It will be conducted in Asia Pacific and\n      Russia and will assess the current status of EGFR mutation testing, and the concordance of\n      EGFR mutation status derived from tumour samples and blood based circulating free DNA."
        }, 
        "brief_title": "Asia Pacific and Russia Diagnostic Study for EGFR Testing", 
        "completion_date": {
            "#text": "July 2017", 
            "@type": "Anticipated"
        }, 
        "condition": "EGFR Mutation Status in aNSCLC Patients (Locally Advanced and/or Metastatic Disease) With Adenocarcinoma and Non-adenocarcinoma Histologies.", 
        "condition_browse": {
            "mesh_term": [
                "Adenocarcinoma", 
                "Neoplasm Metastasis"
            ]
        }, 
        "detailed_description": {
            "textblock": "A diagnostic study to determine the prevalence of EGFR mutations in Asian and Russian\n      patients with advanced NSCLC of Adenocarcinoma and Non-adenocarcinoma histologies"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Histological or cytological confirmed locally advanced NSCLC (stage IIIA/B) not\n             suitable for curative treatment or metastatic (stage IV) NSCLC\n\n          -  Newly diagnosed patients with locally advanced and/or metastatic NSCLC who are\n             systemic treatment Naive (i.e. no chemotherapy or EGFR-TKI) or patients with\n             recurrent disease who have previously received adjuvant chemotherapy (not including\n             EGFR-TKI)\n\n          -  Provision of diagnostic cancer tissue or cytology sample upon inclusion (surgical\n             specimen, biopsy sample, or cytology sample is acceptable) and Provision of a routine\n             blood (plasma) sample in China, Russia, Taiwan and Korea\n\n          -  Patients aged 18 years and older\n\n        Exclusion Criteria:\n\n          -  As judged by the investigator, any evidence of severe or uncontrolled systemic\n             disease (e.g. unstable or uncompensated respiratory, cardiac, hepatic or renal\n             disease)\n\n          -  Evidence of any other significant clinical disorder or laboratory finding that made\n             it undesirable for the patient to participate in the study\n\n          -  Pregnancy or breast-feeding"
            }, 

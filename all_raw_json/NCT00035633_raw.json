    "clinical_study": {
        "@rank": "55604", 
        "brief_summary": {
            "textblock": "The purpose of this clinical research study is to assess the safety effectiveness of\n      entecavir, as compared to lamivudine, in the treatment of adults with chronic hepatitis B\n      infection who are hepatitis B e antigen positive."
        }, 
        "brief_title": "A Phase III Study of Entecavir vs Lamivudine in Adults With Chronic Hepatitis B Infection and Positive for Hepatitis B E Antigen", 
        "completion_date": {
            "#text": "February 2005", 
            "@type": "Actual"
        }, 
        "condition": "Chronic Hepatitis B", 
        "condition_browse": {
            "mesh_term": [
                "Hepatitis", 
                "Hepatitis A", 
                "Hepatitis B", 
                "Hepatitis, Chronic", 
                "Hepatitis B, Chronic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "-  Male and female subjects =/> 16 years of age (or minimum age required in a given\n             country) with history of chronic hepatitis B infection;\n\n          -  Documentation of positive Hepatitis B e antigen (HBeAg) status.;\n\n          -  The absence of coinfection with immunodeficiency virus (HIV), hepatitis C virus (HCV)\n             or hepatitis D virus (HDV);\n\n          -  The absence of other forms of liver disease e.g., alcoholic, autoimmune, biliary\n             disease;\n\n          -  Less than 12 weeks prior therapy with nucleoside or nucleotide analogue antiviral\n             agents with activity against hepatitis B (e.g., adefovir, famciclovir and lamivudine)"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

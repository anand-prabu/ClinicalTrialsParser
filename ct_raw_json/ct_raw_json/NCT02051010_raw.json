        "arm_group": {
            "arm_group_label": "Metastatic Breast Cancer"
        }, 
        "brief_summary": {
            "textblock": "The study will follow a descriptive design involving a survey. Data collection will involve\n      paper based questionnaires. The questionnaires have been developed in conjunction with\n      breast cancer support groups associated with the Irish Cancer Society and patients."
        }, 
        "brief_title": "ABC Survey - do we Meet the Needs of Patients With Advanced Breast Cancer?", 
        "condition": "Metastatic Breast Cancer", 
        "condition_browse": {
            "mesh_term": "Breast Neoplasms"
        }, 
        "detailed_description": {
            "textblock": "The purpose of the study is to identify and categorise the need of Metastatic Breast Cancer\n      (MBC) patients in Ireland with particular focus on; quality of life, symptom burden and\n      access to and value of support available.\n\n      The study also aims to evaluate future service developments that will provide support to\n      these patients."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Signed written informed consent\n\n          -  Clinical diagnosis of MBC\n\n          -  Female aged 18 years or older\n\n          -  Able to complete the questionnaires independently\n\n        Exclusion Criteria:\n\n          -  Any patient who does not meet the above mentioned inclusion criteria."
            }, 
            "gender": "Female", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 

        "arm_group": {
            "arm_group_label": "Calcitriol 3 mcg/g ointment", 
            "arm_group_type": "Experimental", 
            "description": "Topical application every 12 hours for 14 consecutive days"
        }, 
        "brief_summary": {
            "textblock": "The purpose of this study is to determine if the calcitriol absorption in pediatric subjects\n      is comparable to that in adults and adolescents, with no significant impact on\n      calcium/phosphorus metabolism."
        }, 
        "brief_title": "PK and PD Study of Calcitriol 3 mcg/g Ointment in Pediatric Subjects With Plaque Psoriasis", 
        "condition": "Plaque Psoriasis", 
        "condition_browse": {
            "mesh_term": "Psoriasis"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Male or female pediatric subjects, 2 to 12 years of age inclusive at screening\n\n          -  Clinical diagnosis of plaque type psoriasis involving 5% through 35% body surface\n             area at screening and baseline (excluding face and scalp)\n\n        Exclusion Criteria:\n\n          -  Subjects with guttate or pustular psoriasis, erythrodermic psoriasis or active\n             infection\n\n          -  Subjects with hypercalcemia and out of range urinary calcium/creatinine ratio at\n             screening\n\n          -  Subjects with secondary hyperparathyroidism at screening"
            }, 

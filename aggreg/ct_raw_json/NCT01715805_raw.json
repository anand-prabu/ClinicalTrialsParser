        }, 
        "intervention": [
            {
                "arm_group_label": "Cariprazine", 
                "description": "Patients who meet eligibility will have 1-2 week screening period followed by blind placebo, followed by 8 week double blind treatment period on cariprazine with Antidepressant Therapy (ADT) or placebo with ADT for inadequate responders and ADT with single blind placebo for responders, followed by a 1-week safety follow-up period", 
                "intervention_name": "Cariprazine", 
                "intervention_type": "Drug"
            }, 
            {
                "arm_group_label": "Placebo", 
                "description": "Patients who meet eligibility will have 1-2 week screening period followed by blind placebo, followed by 8 week double blind treatment period on placebo with Antidepressant Therapy (ADT) for inadequate responders and ADT with single blind placebo for responders, followed by a 1-week safety follow-up period", 
                "intervention_name": "Placebo", 
                "intervention_type": "Drug"
            }

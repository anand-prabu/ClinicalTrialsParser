{
    "clinical_study": {
        "@rank": "38574", 
        "acronym": "OlympiAD", 
        "arm_group": [
            {
                "arm_group_label": "Olaparib", 
                "arm_group_type": "Experimental", 
                "description": "Olaparib tablet 300mg bd po"
            }, 
            {
                "arm_group_label": "Physician's choice chemotherapy", 
                "arm_group_type": "Active Comparator", 
                "description": "Capecitabine 2500 mg/m2 d1-14 q 21, or Vinorelbine 30 mg/m2 d1,8 q 21, or Eribulin 1.4 mg/m2 d1,8 q 21"
            }
        ], 
        "brief_summary": {
            "textblock": "This open label, randomised, controlled, multi-centre phase III study will assess the\n      efficacy and safety of single agent olaparib vs standard of care based on physician's choice\n      of capecitabine, vinorelbine or eribulin in metastatic breast cancer patients with gBRCA 1/2\n      mutations."
        }, 
        "brief_title": "Assessment of the Efficacy and Safety of Olaparib Monotherapy Versus Physicians Choice Chemotherapy in the Treatment of Metastatic Breast Cancer Patients With Germline BRCA1/2 Mutations.", 
        "completion_date": {
            "#text": "December 2018", 
            "@type": "Anticipated"
        }, 
        "condition": "Breast Cancer Metastatic", 
        "condition_browse": {
            "mesh_term": [
                "Breast Neoplasms", 
                "Neoplasms"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Germline mutation in BRCA1 or BRCA2 that is predicted to be deleterious or suspected\n             deleterious.\n\n          -  Histologically or cytologically confirmed breast cancer with evidence of metastatic\n             disease.\n\n          -  Prior therapy with an anthracycline and a taxane in either an adjuvant or metastatic\n             setting.\n\n          -  Prior platinum allowed as long as no breast cancer progression occurred on treatment\n             or if given in adjuvant/neoadjuvant setting at least 12 months from last dose to\n             study entry elapsed.\n\n          -  ER/PR breast cancer positive patients must have received and progressed on at least\n             one endocrine therapy (adjuvant or metastatic), or have disease that the treating\n             physician believes to be inappropriate for endocrine therapy.\n\n          -  ECOG performance status 0-1.\n\n          -  Adequate bone marrow, kidney and liver function.\n\n        Exclusion Criteria:\n\n          -  Prior treatment with PARP inhibitor.\n\n          -  Patients with HER2 positive disease.\n\n          -  More than 2 prior lines of chemotherapy for metastatic breast cancer.\n\n          -  Untreated and/or uncontrolled brain metastases.\n\n          -  Prior malignancy unless curatively treated and disease-free for > 5 years prior to\n             study entry. Prior adequately treated non-melanoma skin cancer, in situ cancer of the\n             cervix, DCIS or stage I grade 1 endometrial cancer allowed.\n\n          -  Known HIV (Human Immunodeficiency Virus) infection.\n\n          -  Pregnant or breast-feeding women."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"
        }, 
        "enrollment": {
            "#text": "2820", 
            "@type": "Anticipated"
        }, 
        "firstreceived_date": "November 18, 2013", 

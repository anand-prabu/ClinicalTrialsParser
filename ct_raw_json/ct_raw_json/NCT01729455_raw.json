    "clinical_study": {
        "@rank": "93954", 
        "acronym": "SABLE", 
        "arm_group": [
            {
                "arm_group_label": "With BENLYSTA", 
                "description": "SLE treatment including BENLYSTA at baseline"
            }, 
            {
                "arm_group_label": "Without BENLYSTA", 
                "description": "SLE treatment without BENLYSTA at baseline"
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this registry is to collect additional information regarding the side effects\n      and effectiveness of BENLYSTA (belimumab) when given with other lupus medicines to adults\n      with active systemic lupus erythematosus (SLE). Information will be collected on serious\n      events that are not that common or may only be seen with long-term treatment. These events\n      include death, cancers, serious infections and other infections of interest, and serious\n      mental health problems. Information on the effectiveness of BENLYSTA will also be collected."
        }, 
        "brief_title": "Safety and Effectiveness of Belimumab in Systemic Lupus Erythematosus Registry", 
        "completion_date": {
            "#text": "March 2022", 
            "@type": "Anticipated"
        }, 
        "condition": "Systemic Lupus Erythematosus", 
        "condition_browse": {
            "mesh_term": "Lupus Erythematosus, Systemic"
        }, 
        "detailed_description": {
            "textblock": "The registry will enroll 2 groups of patients. One group will include patients who are\n      currently taking lupus medicines along with intravenous BENLYSTA (With BENLYSTA). The other\n      group will include patients who are taking lupus medicines but do not take BENLYSTA (Without\n      BENLYSTA). For every 3 participants enrolled in the registry, 2 will be taking BENLYSTA and\n      1 will not. After enrollment, changes in lupus medications, including starting or stopping\n      BENLYSTA, are at the discretion of the physician, and all patients will continue to be\n      followed regardless of changes in their lupus medicines until study completion. Physicians\n      will manage the patient in accordance with their medical judgment and standard of care. Data\n      will be collected at enrollment and at 6 month intervals for 5 years."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Key Inclusion Criteria:\n\n          -  Clinical diagnosis of active SLE.\n\n          -  Autoantibody-positive.\n\n          -  Current SLE treatment includes BENLYSTA and/or immunosuppressants (for example,\n             azathioprine, methotrexate, cyclophosphamide, mycophenolate, and biologics).\n\n        Key Exclusion Criteria:\n\n          -  Have received treatment with an investigational agent within the past year.\n\n          -  Are currently enrolled in a placebo-controlled BENLYSTA (belimumab) clinical trial or\n             a continuation trial in which belimumab is used as an investigational agent.\n\n          -  Have a history of BENLYSTA exposure, but are not currently receiving BENLYSTA.\n\n          -  Current SLE treatment includes only an antimalarial (for example, hydroxychloroquine)\n             or only steroids."
            }, 
            "gender": "Both", 

{
    "clinical_study": {
        "@rank": "89309", 
        "brief_summary": {
            "textblock": "The purpose of this study is to evaluate the efficacy of the combination of tacrolimus +\n      methotrexate compared to methotrexate alone in the treatment of the signs and symptoms of\n      rheumatoid arthritis over 6 months in patients with partial response to methotrexate."
        }, 
        "brief_title": "Study to Assess Efficacy of Tacrolimus + Methotrexate Versus Placebo + Methotrexate in Treatment of Rheumatoid Arthritis", 
        "completion_date": {
            "#text": "January 2004", 
            "@type": "Actual"
        }, 
        "condition": "Rheumatoid Arthritis", 
        "condition_browse": {
            "mesh_term": [
                "Arthritis", 
                "Arthritis, Rheumatoid"
            ]
        }, 
        "detailed_description": {
            "textblock": "This is a Phase III, 6 month randomized, double-blind, placebo controlled multi-center study\n      with a 6 month open-label extension.  Patients will be randomized to the tacrolimus +\n      methotrexate arm or the placebo + methotrexate arm in a ratio of 2:1.  Patients who complete\n      the 6-month double-blind phase will be eligible to enroll in the open-label phase of the\n      study."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria\n\n          -  Willing to discontinue DMARDs, other than methotrexate\n\n          -  Has a diagnosis of Rheumatoid Arthritis using American College of Rheumatology\n             Criteria, of at least 6 months duration\n\n          -  Have been receiving oral or parenteral methotrexate for at least 3 months\n\n        Exclusion Criteria\n\n          -  Has received tacrolimus for any indication\n\n          -  Has moderate or severe liver disease\n\n          -  Has a known history of HIV infection\n\n          -  Has serum creatinine outside the normal range"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"
        }, 

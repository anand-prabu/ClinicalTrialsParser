        "arm_group": [
            {
                "arm_group_label": "LCM and concomitant sodium channel blocking AEDs", 
                "description": "Patients prescribed adjunctive lacosamide (LCM) added to one or more baseline Anti-Epileptic Drugs (AEDs) to include at least 1 sodium channel blocking AED."
            }, 
            {
                "arm_group_label": "LCM and concomitant non sodium channel blocking AEDs", 
                "description": "Patients prescribed adjunctive lacosamide (LCM) added to one or more baseline Anti-Epileptic Drugs (AEDs), none of which is a sodium channel blocking AED."
            }
        ], 
        "brief_summary": {
            "textblock": "This observational study aims to investigate how VIMPAT\u00ae is used as adjunctive therapy in\n      clinical practice and will also evaluate the subsequent change in the drug load of patients\n      after addition of VIMPAT\u00ae to their treatment regimen."
        }, 
        "brief_title": "Study Evaluating Changes in Total Drug Load and Seizure Frequency Using Vimpat\u00ae (Lacosamide) in Combination Therapy", 
        "completion_date": {
            "#text": "January 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Focal Epilepsy With and Without Secondary Generalization", 
        "condition_browse": {
            "mesh_term": [
                "Epilepsy", 
                "Epilepsies, Partial"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  The patient has not received Vimpat\u00ae more than 7 days prior to start of\n             Non-Interventional Study (NIS)\n\n          -  The patient must have a diagnosis of epilepsy with partial-onset seizures with or\n             without secondary generalization\n\n          -  Based on the physician's clinical judgment, it is in the patient's best interest to\n             be prescribed adjunctive Vimpat\u00ae (ie, the decision to prescribe Vimpat\u00ae is made by\n             the physician)\n\n          -  Patient must be at least 18 years of age\n\n          -  The patient must have had at least one seizure within the last 3 months prior to\n             enrolment"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

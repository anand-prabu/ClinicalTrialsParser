  <brief_title>Monovision vs Multifocal Lens Insertion in Cataract Patients</brief_title>
  <official_title>Monovision Correction Versus Multifocal Lens Insertion Following Emulsification Cataract Surgery</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Democritus University of Thrace</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

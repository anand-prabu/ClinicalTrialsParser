    "clinical_study": {
        "@rank": "19937", 
        "acronym": "CREHA", 
        "arm_group": [
            {
                "arm_group_label": "Bras A", 
                "arm_group_type": "Active Comparator", 
                "description": "Steroid 1mg/kg/d and Cyclophosphamide 2mg/kg/d"
            }, 
            {
                "arm_group_label": "Bras B", 
                "arm_group_type": "Experimental", 
                "description": "Steroid 1mg/kg/d and Rituximab 375 mg/m2 every week during four weeks"
            }
        ], 
        "brief_summary": {
            "textblock": "CREHA project is a study comparing steroid combined with cyclophosphamide versus steroid\n      combined with rituximab in patients with acquired haemophilia. The study will test the\n      hypothesis that steroid combined with cyclophosphamide is more effective than steroid plus\n      rituximab  for FVIII inhibitor eradication  in acquired haemophilia."
        }, 
        "brief_title": "Outcome of Acquired Haemophilia With Steroid Combined With Cyclophosphamide Versus Steroid Combined With Rituximab (CREHA Study)", 
        "completion_date": {
            "#text": "November 2018", 
            "@type": "Anticipated"
        }, 
        "condition": "Acquired Haemophilia", 
        "condition_browse": {
            "mesh_term": "Hemophilia A"
        }, 
        "detailed_description": {
            "textblock": "CREHA project is a multicenter, randomized, controlled efficacity and safety study comparing\n      steroid combined with cyclophosphamide versus steroid combined with rituximab in patients\n      with acquired haemophilia. The study will test the hypothesis that steroid combined with\n      cyclophosphamide is more effective than steroid plus rituximab  for FVIII inhibitor\n      eradication  in acquired haemophilia"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  men or women\n\n          -  women post-menpausal or with ongoing contraception\n\n          -  18 years old or more\n\n          -  diagnosis of acquired hemophilia\n\n          -  patient must be insured\n\n          -  patient has provided written informed consent prior to enrolment\n\n          -  patient compliant\n\n        Exclusion Criteria:\n\n          -  constitutional hemophilia\n\n          -  chemotherapy\n\n          -  ongoing treatment with prednisone > 20mg further more 1 month\n\n          -  ongoing treatment with prednisone >0.7 mg/d further more 10 days\n\n          -  thrombocytopenia\n\n          -  leukopenia\n\n          -  chronic disease"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"

{
    "clinical_study": {
        "@rank": "100864", 
        "arm_group": {
            "arm_group_label": "Cohort"
        }, 
        "brief_summary": {
            "textblock": "This non-interventional, cross-sectional survey and retrospective review will evaluate the\n      medical resource utilization and burden of illness in patients who have persistent symptoms\n      of schizophrenia despite receiving adequately dosed antipsychotic treatment and who have not\n      had an acute exacerbation in the 3 months prior to enrolment. Medical records will be\n      reviewed for a minimum of 3 months and up to 12 months prior to screening. Data collection\n      at a single visit will include rating scales and questionnaires that reflect the clinical\n      status and the quality of life of the patients and the economic impact of schizophrenia."
        }, 
        "brief_title": "A Non-Interventional Study of Patients With Persistent Symptoms of Schizophrenia to Describe Medical Resource Utilization and Burden of Illness", 
        "completion_date": {
            "#text": "April 2014", 
            "@type": "Actual"
        }, 
        "condition": "Schizophrenia", 
        "condition_browse": {
            "mesh_term": "Schizophrenia"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Adult patients, >/= 18 years of age\n\n          -  Patients who have a clinical diagnosis of schizophrenia as documented in their\n             medical records\n\n          -  Patients who, in the opinion of the investigator, have had on-going, persistent\n             symptoms of schizophrenia for at least the last 3 months prior to enrolment\n\n          -  Patients who, in the opinion of the investigator, have been prescribed adequate doses\n             of antipsychotic treatment for at least the last 3 months\n\n          -  Patient has adequate spoken local language\n\n        Exclusion Criteria:\n\n          -  Acute exacerbation of schizophrenia within the last 3 months prior to enrolment\n\n          -  Enrolment in an interventional study"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Probability Sample", 
            "study_pop": {

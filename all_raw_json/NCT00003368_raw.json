    "clinical_study": {
        "@rank": "114539", 
        "brief_summary": {
            "textblock": "RATIONALE: Drugs used in chemotherapy use different ways to stop tumor cells from dividing\n      so they stop growing or die. Irinotecan may be effective in treating patients with abnormal\n      liver or kidney function or who have received radiation therapy.\n\n      PURPOSE: Phase I trial to study the effectiveness of irinotecan in treating patients with\n      solid tumors or lymphoma who have abnormal liver or kidney function or who have had previous\n      radiation therapy to the pelvis."
        }, 
        "brief_title": "Irinotecan in Treating Patients With Solid Tumors or Lymphoma Who Have Abnormal Liver or Kidney Function or Who Have Received Radiation Therapy to the Pelvis", 
        "completion_date": {
            "#text": "July 2006", 
            "@type": "Actual"
        }, 
        "condition": [
            "Lymphoma", 
            "Small Intestine Cancer", 
            "Unspecified Adult Solid Tumor, Protocol Specific"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Lymphoma", 
                "Lymphoma, Non-Hodgkin", 
                "Lymphoma, Large-Cell, Immunoblastic", 

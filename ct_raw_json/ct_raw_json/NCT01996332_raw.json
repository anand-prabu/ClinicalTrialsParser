        }, 
        "brief_summary": {
            "textblock": "This study will evaluate the efficacy and safety of oral Tarceva in patients with advanced\n      NSCLC for whom Tarceva monotherapy is considered the best therapeutic option. The\n      anticipated time on study treatment is 3-12 months."
        }, 
        "brief_title": "A Study of Tarceva (Erlotinib) Monotherapy in Patients With Advanced Non-Small Cell Lung Cancer (NSCLC)", 
        "completion_date": {
            "#text": "October 2012", 
            "@type": "Actual"
        }, 
        "condition": "Non-Small Cell Lung Cancer", 
        "condition_browse": {
            "mesh_term": [
                "Carcinoma, Non-Small-Cell Lung", 
                "Lung Neoplasms"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  adult patients >=18 years of age;\n\n          -  locally advanced or metastatic NSCLC (Stage IIIB or IV);\n\n          -  not a candidate for curative surgery or radical chemotherapy;\n\n          -  no brain metastases, or clinically stable metastases for >=2 months.\n\n        Exclusion Criteria:\n\n          -  radiotherapy over the previous 2 weeks;\n\n          -  weight loss >10% in the previous 6 weeks."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

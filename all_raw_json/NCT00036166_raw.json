    "clinical_study": {
        "@rank": "111212", 
        "brief_summary": {
            "textblock": "The purpose of the study is to evaluate the safety and efficacy of FK463 in patients with\n      proven or probable invasive infections due to Aspergillus species."
        }, 
        "brief_title": "Study of FK463 for the Treatment of Invasive Aspergillosis", 
        "completion_date": {
            "#text": "December 2002", 
            "@type": "Actual"
        }, 
        "condition": "Aspergillosis", 
        "condition_browse": {
            "mesh_term": "Aspergillosis"
        }, 
        "detailed_description": {
            "textblock": "This will be an open-label, non-comparative study of intravenous FK463.  Enrollment will\n      include at least 100 patients evaluable for efficacy."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria\n\n          -  Has proven or probable systemic infection with Aspergillus species\n\n        Exclusion Criteria\n\n          -  Has abnormal liver test parameters, e.g., AST or ALT > 10 times upper limit of normal\n\n          -  Has bronchopulmonary aspergillosis, aspergillomas, sinus aspergillosis or external\n             otitis but does not have histologic evidence of tissue invasion\n\n          -  Has life-expectancy judged to be less than 5 days"
            }, 

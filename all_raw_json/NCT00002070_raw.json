    "clinical_study": {
        "@rank": "117884", 
        "brief_summary": {
            "textblock": "To determine whether co-administration of sargramostim (granulocyte-macrophage\n      colony-stimulating factor; GM-CSF) improves tolerance to ganciclovir in patients previously\n      intolerant because of neutropenia defined as an absolute neutrophil count less than 500\n      cells/mm3. To assess if improved tolerance of ganciclovir is associated with a favorable\n      outcome as defined by a delayed time to progression of retinitis; to confirm the safety and\n      co-administration of ganciclovir and GM-CSF; to assess the changes in the expression of HIV\n      p24 antigen in the serum and number of T4+ lymphocytes in the blood of patients receiving\n      ganciclovir with or without GM-CSF."
        }, 
        "brief_title": "Phase III Ganciclovir +/- rGM-CSF for AIDS-Related CMV Retinitis", 
        "condition": [
            "Cytomegalovirus Retinitis", 
            "HIV Infections"
        ], 
        "condition_browse": {

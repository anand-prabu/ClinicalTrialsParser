{
    "clinical_study": {
        "@rank": "105798", 
        "acronym": "EAS", 
        "brief_summary": {
            "textblock": "The aim of the study is to assess safety, tolerability and clinical effects of different\n      doses of riociguat in patients with inoperable  Chronic Thromboembolic Pulmonary\n      Hypertension (CTEPH) and who are not satisfactorily treated and cannot participate in any\n      other CTEPH trial. In the US the study runs as an Expanded Access program under 21 CFR\n      312.320."
        }, 
        "brief_title": "Riociguat in Patients With Chronic Thromboembolic Pulmonary Hypertension (CTEPH)", 
        "condition": "Hypertension, Pulmonary", 
        "condition_browse": {
            "mesh_term": [
                "Hypertension", 
                "Hypertension, Pulmonary"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n        - Male and female patients with CTEPH either inoperable or with persistent or recurrent PH\n        after surgery\n\n        Exclusion Criteria:\n\n          -  All types of pulmonary hypertension other than Dana Point Classification  Group 4\n\n          -  Operable patients listed for PEA (Pulmonary Endarterectomy)"
            }, 
            "gender": "Both", 
            "maximum_age": "80 Years", 
            "minimum_age": "18 Years"
        }, 
        "firstreceived_date": "February 4, 2013", 
        "id_info": {
            "nct_id": "NCT01784562", 
            "org_study_id": "16097", 
            "secondary_id": "2012-002104-40"
        }, 
        "intervention": {
            "description": "Individual dosing of riociguat between 0.5 and 2.5 mg three times daily based on patient's well being and blood pressure. (The individual optimal dose should be determined during the initial 8-week titration phase based on patient's monitoring of systolic blood pressure and well-being.)", 
            "intervention_name": "Adempas (Riociguat, BAY63-2521)", 
            "intervention_type": "Drug"
        }, 
        "is_fda_regulated": "Yes", 
        "is_section_801": "No", 

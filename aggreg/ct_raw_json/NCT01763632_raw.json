    "clinical_study": {
        "@rank": "55227", 
        "brief_summary": {
            "textblock": "Seventeen hospitals in China will participate in the study, which aims to provide data on\n      transcutaneous bilirubin (TcB) levels for the first 168 hr after birth in term and\n      late-preterm neonates, and  develop an hour-specific TcB nomogram.\n\n      The investigators hypothesize that the hour-specific TcB nomogram can predict neonatal\n      hyperbilirubinemia in term and late-preterm Chinese infants, and plan appropriate follow-up\n      for hyperbilirubinemia in newborns."
        }, 
        "brief_title": "Development of Transcutaneous Bilirubin Nomogram to Identify Neonatal Hyperbilirubinemia in Term and Late-preterm Infants: a Multicenter Study in China", 
        "completion_date": {
            "#text": "January 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Hyperbilirubinemia, Neonatal", 
        "condition_browse": {
            "mesh_term": [
                "Hyperbilirubinemia", 
                "Hyperbilirubinemia, Neonatal"
            ]
        }, 
        "detailed_description": {
            "textblock": "Neonatal hyperbilirubinemia is very common; it is not always a benign condition. If left\n      untreated, it can have devastating consequences including cerebral palsy and hearing loss.\n      It is therefore critical to be able to identify the newborns at risk for severe\n      hyperbilirubinemia."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n        Term or late-preterm newborn infants with GAs of \u226535 weeks and birth weights of \u22652,000 g\n        were included.\n\n        Exclusion Criteria:\n\n        All sick newborn infants who were admitted to the intensive care unit."
            }, 

  <phase>N/A</phase>
  <study_type>Observational</study_type>
  <study_design>Observational Model: Cohort, Time Perspective: Cross-Sectional</study_design>
  <primary_outcome>
    <measure>Difference of EPO level between control groups</measure>
    <time_frame>up to 4 years</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">120</enrollment>
  <condition>Healthy Men and Non- Pregnant Women</condition>
  <biospec_retention>Samples Without DNA</biospec_retention>

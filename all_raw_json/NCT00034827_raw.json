    "clinical_study": {
        "@rank": "45494", 
        "brief_summary": {
            "textblock": "CI-1040 is an experimental drug that is being tested in patients who have advanced\n      colorectal and lung cancer who failed no more than one prior chemotherapy regimen, breast\n      cancer who have failed no more than 2 prior regimens and in patients with pancreatic cancer\n      who have received no prior chemotherapy. CI-1040 is taken orally twice daily with meals.\n      Patients are required to have blood tests periodically while receiving treatment and will be\n      monitored closely throughout the trial for possible side effects and for response."
        }, 
        "brief_title": "A Multicenter Phase 2 Study of CI-1040 in Patients With Advanced Nonsmall-Cell Lung, Breast, Colon and Pancreatic Cancer", 
        "completion_date": {
            "#text": "May 2003", 
            "@type": "Actual"
        }, 

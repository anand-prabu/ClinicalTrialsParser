{
    "clinical_study": {
        "@rank": "53853", 
        "brief_summary": {
            "textblock": "The primary objectives of this trial are: To compare the safety of oral rifabutin versus\n      placebo in the treatment of Mycobacterium avium complex (MAC) bacteremia in AIDS patients\n      with CD4 counts less than or equal to 200 cells/mm3. To investigate the incidence of MAC in\n      these patients. A secondary objective is to compare clinical response, quality of life\n      (Karnofsky), and survival between these two groups."
        }, 
        "brief_title": "Rifabutin Therapy for the Prevention of Mycobacterium Avium Complex (MAC) Bacteremia in AIDS Patients With CD4 Counts = or < 200: A Double-Blind, Placebo-Controlled Trial", 
        "condition": [
            "Mycobacterium Avium-intracellulare Infection", 
            "HIV Infections"
        ], 
        "condition_browse": {
            "mesh_term": [

  <start_date>November 2012</start_date>
  <completion_date type="Anticipated">November 2016</completion_date>
  <primary_completion_date type="Anticipated">November 2016</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Observational</study_type>
  <study_design>Observational Model: Cohort, Time Perspective: Prospective</study_design>
  <primary_outcome>
    <measure>Incidence of sustained suppression of HBV DNA &lt;2000 IU/mL one year after treatment cessation</measure>
    <time_frame>approximately 4 years</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <secondary_outcome>
    <measure>Incidence of suppression of HBV DNA &lt;2000 IU/mL at the end of treatment and 6 months post-treatment</measure>
    <time_frame>approximately 4 years</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Incidence of HBV DNA undetectable (&lt;400 IU/mL) at the end of treatment and 1 year post-treatment</measure>
    <time_frame>approximately 4 years</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Incidence of HBsAg loss/seroconversion</measure>
    <time_frame>approximately 4 years</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>

  <start_date>January 2014</start_date>
  <completion_date type="Anticipated">November 2015</completion_date>
  <primary_completion_date type="Anticipated">November 2015</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Observational</study_type>
  <study_design>Observational Model: Cohort, Time Perspective: Prospective</study_design>

    "clinical_study": {
        "@rank": "75830", 
        "arm_group": {
            "arm_group_label": "1", 
            "description": "Pregnant, HIV-infected women"
        }, 
        "biospec_descr": {
            "textblock": "Blood and amniotic fluid"
        }, 
        "biospec_retention": "Samples With DNA", 
        "brief_summary": {
            "textblock": "The purpose of this study is to collect and study clinical and laboratory information about\n      a pregnant or new mother and her medical care that will increase our knowledge of the best\n      care for HIV-infected pregnant women and their children.\n\n      The rate of transmission of HIV from mothers to their infants has gone down. Specific U.S.\n      Public Health Service guidelines recommend that HIV-infected pregnant women be treated with\n      anti-HIV therapies; but the effectiveness of treatment and safety for the mother and her\n      infant have not been fully examined. This study will monitor the health of women and their\n      infants while they receive anti-HIV therapy. Also, this study will provide information that\n      may be used for future studies."
        }, 
        "brief_title": "Prenatal and Postnatal Studies of Interventions for Prevention of Mother-To-Child Transmission", 
        "completion_date": {
            "#text": "June 2015", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "HIV Infections", 
            "Pregnancy Complications, Infectious"
        ], 
        "condition_browse": {
            "mesh_term": [

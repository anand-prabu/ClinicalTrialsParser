        ], 
        "brief_summary": {
            "textblock": "This is a study to evaluate chronic hepatitis C infection in adults with genotype 1a\n      infection."
        }, 
        "brief_title": "A Study to Evaluate Chronic Hepatitis C Infection in Adults With Genotype 1a Infection", 
        "completion_date": {
            "#text": "August 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Chronic Hepatitis C Infection", 
        "condition_browse": {

  <brief_title>The First Jordanian PCI Registry: Events at 1 Year</brief_title>
  <acronym>JoPCR1</acronym>
  <official_title>The First Jordanian Percutaneous Coronary Intervention (PCI) Registry</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Jordan Collaborating Cardiology Group</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>AstraZeneca</agency>
      <agency_class>Industry</agency_class>
    </collaborator>
  </sponsors>

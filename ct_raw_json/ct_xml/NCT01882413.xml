  <eligibility>
    <study_pop>
      <textblock>
        Patients with planned cataract removal surgery
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Scheduled to undergo cataract removal surgery

        Exclusion Criteria:

          -  Current use of eye drops or oral medications for allergic conjunctivitis

          -  Serious ocular injury, intraocular surgery, or refractive surgery within 6 months in
             the study eye

          -  Use of a contact lens within 4 weeks
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>50 Years</minimum_age>

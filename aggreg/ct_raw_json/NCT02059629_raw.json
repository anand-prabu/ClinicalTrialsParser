{
    "clinical_study": {
        "@rank": "50392", 
        "arm_group": [
            {
                "arm_group_label": "Group A: Eluna pacemaker family", 
                "description": "Patients with standard indication for pacemaker therapy or Cardiac Resynchronization Therapy (CRT), who will be implanted with a pacemaker or CRT device of the Eluna pacemaker family. Single-, Dual- and Tripple-chamber pacemakers are applicable."
            }, 
            {
                "arm_group_label": "Group B: Sentus BP lead", 
                "description": "Heart failure patients with indication for Cardiac Resynchronization Therapy (CRT) therapy, who will be implanted with the left ventricular Sentus BP lead. Patients can receive either CRT pacemaker or CRT-D (CRT with defibrillator function)."
            }
        ], 
        "brief_summary": {
            "textblock": "The Eluna Family / Sentus BP Master Study is designed to confirm the safety of the new Eluna\n      pacemaker family and the Sentus BP (bipolar) left ventricular lead.\n\n      Furthermore the new wandless RF telemetry function \"SafeSync\" and the handling of the Sentus\n      lead during implantation will be assessed."
        }, 
        "brief_title": "Eluna Family / Sentus BP Master Study", 
        "completion_date": {
            "#text": "April 2016", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Bradycardia", 
            "Heart Failure"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Heart Failure", 
                "Bradycardia"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Patient is willing to participate in the study and provided written informed\n             consent\n\n          -  Patient meets standard indication for pacemaker or CRT-P or CRT-D therapy\n\n          -  Patient accepts Home Monitoring\u00ae concept\n\n          -  Patient has legal capacity and ability to consent\n\n        Exclusion Criteria:\n\n          -  Patient with any contraindication to pacemaker and CRT-P and CRT-D therapy\n\n          -  Patient under the age of 18\n\n          -  Pregnant or breast-feeding women\n\n          -  Cardiac surgery planned within the next 6 months\n\n          -  Life expectancy less than 12 months\n\n          -  Participation in another cardiac clinical investigation with active treatment arm\n\n          -  Group B only: Currently implanted with an endocardial or epicardial LV lead or had\n             prior attempt to place a LV lead (for CRT implants only)"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Patients will be recruited from available pacemaker or CRT patients directly by the\n        treating physician at the investigational site"
            }
        }, 

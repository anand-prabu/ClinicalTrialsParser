        "acronym": "ixCELL DCM", 
        "arm_group": [
            {
                "arm_group_label": "ixmyelocel-T", 
                "arm_group_type": "Experimental", 
                "description": "Ixmyelocel-T delivered by catheter-based intramyocardial injection procedure."
            }, 
            {
                "arm_group_label": "Vehicle Control", 

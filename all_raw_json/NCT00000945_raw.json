    "clinical_study": {
        "@rank": "75748", 
        "brief_summary": {
            "textblock": "The purpose of this study is to evaluate the safety, tolerance, and overall effectiveness of\n      cidovir to treat PML in AIDS patients.\n\n      PML is an opportunistic infection (HIV-associated, due to weak immune system) caused by a\n      virus that attacks the brain.  Cidovir has been used effectively to treat cytomegalovirus\n      (CMV) of the eye.  Cidovir could be an effective treatment for PML as well."
        }, 
        "brief_title": "A Study to Evaluate the Use of Cidofovir (an Experimental Drug) for the Treatment of Progressive Multifocal Leukoencephalopathy (PML) in AIDS Patients", 
        "completion_date": {
            "#text": "March 2001", 
            "@type": "Actual"
        }, 
        "condition": [
            "HIV Infections", 
            "Leukoencephalopathy, Progressive Multifocal"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Acquired Immunodeficiency Syndrome", 
                "HIV Infections", 
                "Immunologic Deficiency Syndromes", 

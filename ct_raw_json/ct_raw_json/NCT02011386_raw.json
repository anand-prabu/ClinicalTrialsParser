        "acronym": "MANGO", 
        "arm_group": {
            "arm_group_label": "Treatment", 
            "description": "Treatment: Injection Golimumab 50 mg every month on the same date"
        }, 
        "brief_summary": {
            "textblock": "The study investigates different criteria for remission based on MRI and circulating\n      biomarkers for inflammation, cartilage, connective tissue and bone turnover in patients with\n      axial spondyloarthritis treated with Golimumab. Furthermore, the study also investigates\n      factors that affect disease activity, function and participation by use of different\n      questionnaires."
        }, 
        "brief_title": "Novel MRI ANd Biomarkers in GOlimumab-treated Patients With Axial Spondyloarthritis", 
        "completion_date": {
            "#text": "July 2021", 
            "@type": "Anticipated"
        }, 
        "condition": "Axial Spondyloarthritis", 
        "condition_browse": {
            "mesh_term": [
                "Spondylarthritis", 
                "Spondylitis"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Axial spondyloarthritis according to the ASAS classification criteria\n\n          -  Sacroiliitis on conventional X-rays or MRI.\n\n          -  Disease activity assessed by BASDAI >40 mm despite treatment with NSAID.\n\n          -  Clinical indication for tumor-necrosis-factor-alpha inhibitor treatment by the\n             treating physician.\n\n        Exclusion Criteria:\n\n          -  No contraindications for tumor-necrosis-factor-alpha inhibitor\n\n          -  No contraindications for MRI\n\n          -  No contraindications in participation in a study"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "85 Years", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Patients with axial spondyloarthritis with moderate to high disease activity (BASDAI >= 40\n        mm), who are going to start treatment with tumor-necrosis-factor-alpha inhibitor."
            }
        }, 

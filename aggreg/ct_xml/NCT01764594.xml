  <brief_summary>
    <textblock>
      To evaluate safety, tolerability pharmacokinetics and immunogenicity of CDP7657.
    </textblock>
  </brief_summary>
  <overall_status>Active, not recruiting</overall_status>
  <start_date>January 2013</start_date>
  <completion_date type="Anticipated">August 2014</completion_date>
  <primary_completion_date type="Anticipated">August 2014</primary_completion_date>
  <phase>Phase 1</phase>
  <study_type>Interventional</study_type>
  <study_design>Allocation: Randomized, Endpoint Classification: Safety Study, Intervention Model: Parallel Assignment, Masking: Single Blind (Investigator)</study_design>
  <primary_outcome>
    <measure>Percentage of subjects with at least one Treatment-emergent Adverse Event (TEAE) during the study</measure>
    <time_frame>0 - 28 Weeks</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <primary_outcome>
    <measure>Percentage of subjects who withdrew due to an Treatment-emergent Adverse Event (TEAE) during the study</measure>
    <time_frame>0 - 28 Weeks</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <secondary_outcome>
    <measure>Maximum plasma concentration (Cmax)</measure>
    <time_frame>0 - 28 Weeks</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Predose plasma concentration (Ctrough)</measure>
    <time_frame>0 - 28 Weeks</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Area under the concentration-time curve over the dosing interval (AUCτ)</measure>
    <time_frame>0 - 28 Weeks</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>

        "acronym": "CaMriSS", 
        "brief_summary": {
            "textblock": "The purpose of this prospective single-center trial is to compare the safety and diagnostic\n      value of contrast medium-enhanced cardiac magnetic resonance imaging and transesophageal\n      echocardiography in patients with acute ischemic stroke."
        }, 
        "brief_title": "Cardiac MRI in Ischemic Stroke Study", 
        "completion_date": {
            "#text": "December 2016", 
            "@type": "Anticipated"
        }, 
        "condition": "Stroke", 
        "condition_browse": {
            "mesh_term": [
                "Stroke", 
                "Cerebral Infarction"
            ]
        }, 
        "detailed_description": {
            "textblock": "Primary safety objective: Contrast medium-enhanced cardiac magnetic resonance imaging is\n      safe in patients with acute ischemic stroke. Primary outcome: The diagnostic value of\n      contrast medium-enhanced cardiac magnetic resonance imaging is comparable to transesophageal\n      echocardiography in patients with acute ischemic stroke."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Patients with acute ischemic stroke detected by brain magnetic resonance imaging.\n\n        Exclusion Criteria:\n\n          -  Known atrial fibrillation or cardiac source of embolism\n\n          -  Severe aphasia\n\n          -  Acute infection or chronic inflammation\n\n          -  Severe arteriosclerosis or stenosis of brain-supplying arteries\n\n          -  Contraindications for magnetic resonance imaging or transesophageal echocardiography."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Overall, 103 patients with acute ischemic stroke detected by brain magnetic resonance\n        imaging will be enrolled."
            }
        }, 

        }, 
        "responsible_party": {
            "responsible_party_type": "Sponsor"
        }, 
        "secondary_outcome": {
            "description": "Recurrence was defined as a return of headache to moderate or severe intensity within 2-24 h if the subject had an initial response within 2 h of taking the study medication.", 
            "measure": "Sustained response was defined as achieving a headache response within 2 h post dose, with no recurrence or rescue medication needed", 
            "safety_issue": "No", 
            "time_frame": "18 weeks"
        }, 

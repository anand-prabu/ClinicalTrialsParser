{
    "clinical_study": {
        "@rank": "93941", 
        "acronym": "TURQUOISE-II", 
        "arm_group": [
            {
                "arm_group_label": "Arm A", 
                "arm_group_type": "Experimental", 
                "description": "ABT-450/r/ABT-267 and ABT-333 coadministered with ribavirin (RBV) for 12 weeks"
            }, 
            {
                "arm_group_label": "Arm B", 
                "arm_group_type": "Experimental", 
                "description": "ABT-450/r/ABT-267 and ABT-333 coadministered with ribavirin (RBV) for 24 weeks"
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to evaluate the safety and effect of ABT-450, ritonavir and\n      ABT-267 (ABT-450/r/ABT-267) and ABT-333 coadministered with ribavirin (RBV) in HCV genotype\n      1-infected adults with compensated cirrhosis."
        }, 
        "brief_title": "A Study to Evaluate the Safety and Effect of ABT-450, Ritonavir and ABT-267 (ABT-450/r/ABT-267) and ABT-333 Coadministered With Ribavirin (RBV) in Hepatitis C Virus (HCV) Genotype 1-infected Adults With Compensated Cirrhosis", 
        "completion_date": {
            "#text": "September 2014", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Chronic Hepatitis C", 
            "Compensated Cirrhosis", 
            "Hepatitis C Virus"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Hepatitis", 
                "Hepatitis A", 
                "Hepatitis, Chronic", 
                "Hepatitis C", 
                "Liver Cirrhosis", 
                "Fibrosis", 
                "Virus Diseases", 
                "Hepatitis C, Chronic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Male or female and age is between 18 and 70 years, inclusive, at time of Screening.\n\n          -  Chronic HCV-infection prior to study enrollment.\n\n          -  Screening laboratory result indicating HCV genotype 1-infection.\n\n          -  Compensated cirrhosis defined as a Child-Pugh Score of less than or equal to 6 at\n             Screening\n\n          -  Subject has plasma HCV RNA level greater than 10,000 IU/mL at Screening.\n\n        Exclusion Criteria:\n\n          -  Positive test result for Hepatitis B surface antigen (HBsAg) or anti-Human\n             Immunodeficiency virus antibody (HIV Ab) at screening.\n\n          -  Prior therapy with direct acting antiviral agents for the treatment of HCV, including\n             telaprevir and boceprevir.\n\n          -  Any current or past clinical evidence of Child-Pugh B or C Classification or clinical\n             history of liver decompensation including ascites (noted on physical exam), variceal\n             bleeding or hepatic encephalopathy.\n\n          -  A positive screening ultrasound for hepatocellular carcinoma (HCC) confirmed with a\n             subsequent CT Scan or MRI during the screening period.\n\n          -  Any cause of liver disease other than chronic HCV-infection, including but not\n             limited to the following:\n\n          -  Hemochromatosis\n\n          -  Alpha-1 antitrypsin deficiency\n\n          -  Wilson's disease\n\n          -  Autoimmune hepatitis\n\n          -  Alcoholic liver disease\n\n          -  Drug-related liver disease"
            }, 
            "gender": "Both", 

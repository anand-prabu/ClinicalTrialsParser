  <brief_title>Linking Former Inmates to Primary Care</brief_title>
  <official_title>Transitions Clinic Network: Linking High-Risk Medicaid Patients From Prison to Community Primary Care</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Yale University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

  <brief_title>Antipsychotic Discontinuation in High-risk Subjects</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>

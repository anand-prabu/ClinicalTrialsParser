        "arm_group": [
            {
                "arm_group_label": "Sequential icotinib plus chemotherapy", 
                "arm_group_type": "Experimental", 
                "description": "Sequential icotinib plus chemotherapy : pemetrexed 500mg/m2 iv d1, cisplatin 75mg/m2 d1, icotinib 125 mg is administered orally three times per day d 8-21, every 3 weeks for a cycle. After receiving a maximum of 4 cycles treatment, non-progressive patients continue to receive icotinib as maintenance treatment until disease progression or intolerable toxicity."
            }, 
            {
                "arm_group_label": "Icotinib", 
                "arm_group_type": "Active Comparator", 
                "description": "Icotinib 125 mg is administered orally three times per day until disease progression or intolerable toxicity."
            }
        ], 
        "brief_summary": {
            "textblock": "This randomised, controlled, multicentre trial is designed to assess the efficacy and safety\n      of sequential icotinib plus chemotherapy versus single icotinib as first-line treatment in\n      stage IIIB/IV lung adenocarcinoma patients with EGFR mutation."
        }, 
        "brief_title": "Sequential Icotinib Plus Chemotherapy Versus Icotinib Alone as First-line Treatment in Stage IIIB/IV Lung Adenocarcinoma", 
        "completion_date": {
            "#text": "September 2017", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "EGFR Positive Non-small Cell Lung Cancer", 
            "Adenocarcinoma"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Adenocarcinoma", 
                "Carcinoma, Non-Small-Cell Lung", 
                "Lung Neoplasms"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Pathologic confirmation of lung adenocarcinoma with measurable disease, defined as at\n             least one lesion that can be accurately measured in at least one dimension (longest\n             diameter to be recorded on CT)\n\n          -  Patients must have previously untreated locally advanced or metastatic NSCLC\n\n          -  EGFR activating mutation (exon 19 deletion, L858R) is required\n\n          -  Eastern Cooperative Oncology Group performance status (ECOG PS) score of 0 or 1\n\n        Exclusion Criteria:\n\n          -  Prior chemotherapy or treatment with gefitinib, erlotinib, or other drugs that target\n             EGFR\n\n          -  Patients with wild-type EGFR\n\n          -  Any other investigational agents are not permitted\n\n          -  Any evidence of interstitial lung disease"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

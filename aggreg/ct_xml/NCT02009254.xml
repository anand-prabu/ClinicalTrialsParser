  <brief_title>The Effect of Mashed Potato Composition on Food Intake, Satiety and Blood Glucose</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Toronto</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

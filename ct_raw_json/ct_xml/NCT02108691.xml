  <brief_title>Effect of Intake of Glycin Max (L.) Merr. Peel Extract on Body Fat and Body Weight</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hanyang University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

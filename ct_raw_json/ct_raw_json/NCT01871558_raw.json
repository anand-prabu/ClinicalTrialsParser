        "acronym": "ADDONIS", 
        "arm_group": [
            {
                "arm_group_label": "SU+metformin + Basal Insulin", 
                "arm_group_type": "Active Comparator", 
                "description": "Randomized patient will  remain on their previous dual therapy by SU+metformin, which will be kept unchanged, + start of Basal Insulin up-titrated as per usual algorithms primarily based on FPG"
            }, 
            {
                "arm_group_label": "Metformin/vildagliptin + Basal Insulin", 
                "arm_group_type": "Experimental", 
                "description": "Randomized patient will receive Vildagliptin 50 mg twice daily (b.i.d) + continued therapy with Metformin, + start of Basal Insulin up-titrated as per usual algorithms primarily based on FPG"
            }
        ], 
        "brief_summary": {
            "textblock": "To compare a therapeutic strategy combining vildagliptin+metformin + Basal Insulin versus\n      SU+metformin + Basal Insulin on the incidence of hypoglycemic events over 24 weeks."
        }, 
        "brief_title": "Vildagliptin/Metformin in T2DM Patients Starting Basal Insulin", 
        "completion_date": {
            "#text": "October 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Type 2 Diabetes Mellitus (T2DM)", 
        "condition_browse": {
            "mesh_term": [
                "Diabetes Mellitus", 
                "Diabetes Mellitus, Type 2"
            ]
        }, 
        "detailed_description": {
            "textblock": "This study, conducted  in T2DM patients failing a dual therapy with metformin/SU in whom the\n      decision to start basal insulin has been taken, will compare vildagliptin+metformin versus\n      the previously used SU+met combination (regimen kept unchanged) in association with basal\n      insulin, up-titrated as per usual algorithms primarily based on FPG to obtain  a similar\n      improvement in glycemic control  in both arms, on  the incidence of hypoglycemic episodes\n      over 24 weeks."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  T2DM patients inadequately controlled with metformin+ SU combination therapy at max\n             tolerated doses and in whom the decision to start basal insulin has been taken\n\n          -  with stable metformin+ SU combination therapy for at least 12 weeks prior to\n             randomization\n\n          -  with a glycemic target of HbA1c <= 7%\n\n          -  with HbA1c at inclusion >7% and <=9%\n\n          -  Patients willing and able to start basal insulin and perform appropriate self\n             monitoring of blood glucose (SMBG)\n\n        Exclusion Criteria:\n\n          -  contraindication for either SUs, metformin or insulin and history of hypersensitivity\n             to vildagliptin\n\n          -  acute or chronic diseases that interfere with efficacy/safety results of this trial\n             or put patient at risk\n\n        Other protocol defined inclusion/exclusion criteria may apply"
            }, 

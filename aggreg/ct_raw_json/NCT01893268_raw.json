{
    "clinical_study": {
        "@rank": "115994", 
        "arm_group": {
            "arm_group_label": "Cohort"
        }, 
        "brief_summary": {
            "textblock": "This prospective, observational study will assess the effectiveness and safety of Avastin\n      (bevacizumab) as 1st line therapy in elderly patients with advanced non-squamous non-small\n      cell lung cancer in clinical practice. Therapy with Avastin and standard chemotherapy will\n      be administered according to the treating physician. Data will be collected for\n      approximately 4 years."
        }, 
        "brief_title": "An Observational Study of Avastin (Bevacizumab) in Elderly Patients With Non-Squamous Non-Small Cell Lung Cancer in Clinical Practice", 
        "completion_date": {
            "#text": "January 2017", 
            "@type": "Anticipated"
        }, 
        "condition": "Non-Small Cell Lung Cancer", 
        "condition_browse": {
            "mesh_term": [
                "Carcinoma, Non-Small-Cell Lung", 
                "Lung Neoplasms"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Adult patients, >= 65 years of age\n\n          -  Diagnosis of advanced non squamous Non-Small Cell Lung Carcinoma (nsNSCLC, stage IIIB\n             or IV) for whom the physician has decided to initiate a treatment with Avastin\n             combined with chemotherapy as 1st line therapy\n\n          -  Having received oral and written information about the study, and having signed a\n             written consent form\n\n        Exclusion Criteria:\n\n          -  Participation in a cancer clinical trial with an investigational medicinal product\n             other than this study\n\n          -  Patient previously treated with Avastin\n\n          -  Patient under supervision or guardianship"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "65 Years", 
            "sampling_method": "Probability Sample", 
            "study_pop": {
                "textblock": "Patients aged over 65 years with non-squamous non-small cell lung cancer"
            }
        }, 

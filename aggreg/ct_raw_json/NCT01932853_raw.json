        "@rank": "90733", 
        "arm_group": {
            "arm_group_label": "Hemodialysis patients", 
            "description": "Patients > 18y hemodialysis for at least 6 months at any center of Spain Fresenius Medical Care (FMC) meeting the inclusion criteria."
        }, 
        "brief_summary": {
            "textblock": "The aim of this study is to assess whether patients receiving current recommendations of an\n      adequate dialysis dose by Kt adjusted for body surface area improved survival at 24 months\n      compared to those who do not get it, as well as assess whether patients receiving a dose\n      greater obtain more benefit."
        }, 
        "brief_title": "Association Between Dose Dialysis by Kt and Mortality", 
        "completion_date": {
            "#text": "December 2015", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "End Stage Renal Disease", 
            "Hemodialysis Patients"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Kidney Diseases", 
                "Kidney Failure, Chronic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  > 18 years old\n\n          -  > six months on hemodialysis\n\n          -  Receiving 3 sessions of dialysis per week\n\n          -  Having 5 or more valid measures of Kt during the month\n\n        Exclusion Criteria:\n\n          -  active chronic inflammatory diseases\n\n          -  Liver cirrhosis\n\n          -  Neoplasms\n\n          -  Chronic immunosuppression or use of anti-inflammatory"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Study subjects:\n\n        Patients> 18 years in hemodialysis treatment at any FMC center of Spain that meet the\n        inclusion criteria.\n\n        Data will be collected according to clinical practice and patients will be categorized\n        according to their dose of Kt, along the two years of the study. Will seek the association\n        of dialysis dose with mortality and hospitalization"
            }
        }, 

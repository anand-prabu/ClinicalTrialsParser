{
    "clinical_study": {
        "@rank": "124298", 
        "brief_summary": {
            "textblock": "This prospective, observational, multi-center, patient registry will follow patients who are\n      newly initiated on Tyvaso for the treatment of Pulmonary Arterial Hypertension (PAH). Once\n      enrolled, the patients' dose and titration will be followed for the first 6 months of\n      treatment with Tyvaso.\n\n      A call-center will contact the patients directly at weeks 1, 2, 3, 4, 8, 12, 16, 20, and 24\n      to review their dose and titration schedule. In addition to patient-reported dosing data,\n      some patient demographic information, will be collected by the investigative site at\n      Baseline."
        }, 
        "brief_title": "Tyvaso Dosing and Titration Evaluation: TyTRATE Registry", 
        "completion_date": {
            "#text": "December 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Pulmonary Arterial Hypertension", 

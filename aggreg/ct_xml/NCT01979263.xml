  <brief_title>Attention Bias Modification Treatment for Anxious Youth</brief_title>
  <acronym>ABMT</acronym>
  <sponsors>
    <lead_sponsor>
      <agency>Weill Medical College of Cornell University</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

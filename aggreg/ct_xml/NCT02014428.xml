  </secondary_outcome>
  <condition>Atrophic Vaginitis</condition>
  <arm_group>
    <arm_group_label>Hyaluronic acid</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>220 mg hyaluronic acid per tablet (two tablets/day for 10 days, and subsequently one tablet/day for three months)</description>
  </arm_group>
  <arm_group>
    <arm_group_label>Placebo</arm_group_label>

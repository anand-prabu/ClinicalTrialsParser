        "intervention": [
            {
                "arm_group_label": "Lanthanum Carbonate", 
                "description": "Subjects will receive Lanthanum carbonate orally at a total daily dose of 1500 mg to 3000 mg divided equally between three meals.", 
                "intervention_name": "Lanthanum carbonate", 
                "intervention_type": "Drug", 
                "other_name": "Fosrenol, SPD405"
            }, 
            {
                "arm_group_label": "Calcium Carbonate", 
                "description": "Subjects will receive Calcium carbonate orally at a total daily dose taken by the subject prior to starting the study or equivalent.  For new subjects, dosing will be based on standard clinical practice.  The total daily dose may be adjusted as appropriate, until the target serum phosphorus level is achieved or until a maximum daily dose of 6500mg is reached.", 
                "intervention_name": "Calcium carbonate", 
                "intervention_type": "Drug"
            }

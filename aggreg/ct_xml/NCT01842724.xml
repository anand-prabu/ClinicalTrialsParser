  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patients with painful non-rheumatoid wrist arthritis (posttraumatic arthritis and
             late stage Kienboeck`s disease)

          -  Age 18-75 years

          -  ASA class 1-3
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>75 Years</maximum_age>

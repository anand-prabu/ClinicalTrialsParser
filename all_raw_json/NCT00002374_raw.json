{
    "clinical_study": {
        "@rank": "124592", 
        "brief_summary": {
            "textblock": "To examine the efficacy of saquinavir SGC (soft gel capsules) in combination with other\n      antiretrovirals in HIV-1 infected patients currently treated with saquinavir HGC (hard gel\n      capsules) measured by the absolute change in plasma HIV-1 RNA during the 24 weeks of study\n      treatment."
        }, 
        "brief_title": "A Study of Saquinavir Soft Gelatin Capsules Combined With Other Anti-HIV Drugs in HIV-1 Infected Patients", 
        "condition": "HIV Infections", 
        "condition_browse": {
            "mesh_term": [
                "HIV Infections", 
                "Acquired Immunodeficiency Syndrome"
            ]
        }, 
        "detailed_description": {
            "textblock": "Eligible patients will be stratified by HIV-1 RNA level into 2 populations: 1) >=\n      5,000-30,000 HIV-1 RNA, 2) > 30,000 HIV-1 RNA. All patients will be randomized to 1 of 3\n      treatment arms: Arm A - Saquinavir soft gel capsules (SQV SGC) + 2 new nucleoside analogs\n      (reverse transcriptase inhibitors [RTIs]), Arm B - SQV SGC + nelfinavir + stavudine (d4T) or\n      a new RTI, or Arm C - SQV SGC + ritonavir + d4T or a new RTI. All patients will undergo\n      plasma HIV-1 RNA determinations, CD4 lymphocyte counts, hematology and chemistry blood work\n      at baseline, weeks 4, 8, 12, 16 and 24."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria\n\n        Patients must have:\n\n          -  Documented HIV infection.\n\n          -  CD4 > 100 cells/mm3.\n\n          -  Saquinavir HGC-experienced patients (>= 3 months cumulative therapy).\n\n        Required:\n\n        >= 3 months cumulative saquinavir HGC therapy."
            }, 
            "gender": "Both", 

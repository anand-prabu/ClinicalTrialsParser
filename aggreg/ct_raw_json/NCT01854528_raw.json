        "arm_group": [
            {
                "arm_group_label": "Arm A", 
                "arm_group_type": "Experimental", 
                "description": "ABT-450/r/ABT-267 150mg/100mg/25mg QD and ABT-333 250mg BID coadministered with weight-based ribavirin BID for 12 weeks"
            }, 
            {
                "arm_group_label": "Arm B", 
                "arm_group_type": "Active Comparator", 
                "description": "Telaprevir 750mg Q8H co-administered with pegIFN 180mcg SC weekly and weight-based ribavirin BID for 12 weeks followed by pegIFN 180mcg SC weekly and weight-based ribavirin BID for either 12 or 36 weeks, per local prescribing information."
            }
        ], 
        "brief_summary": {
            "textblock": "This is a study to evaluate the efficacy and safety of three experimental drugs compared\n      with Telaprevir (a licensed product) in people with hepatitis C virus infection who have\n      been previously treated."
        }, 
        "brief_title": "A Study to Evaluate the Efficacy and Safety of Three Experimental Drugs Compared With Telaprevir (a Licensed Product) for Treatment of Chronic Hepatitis C Infection in Treatment-experienced Adults", 
        "completion_date": {
            "#text": "July 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Chronic Hepatitis C Infection", 
        "condition_browse": {
            "mesh_term": [
                "Hepatitis", 
                "Hepatitis A", 
                "Hepatitis, Chronic", 
                "Hepatitis C", 
                "Hepatitis C, Chronic"
            ]
        }, 
        "detailed_description": {
            "textblock": "The primary purpose of this study is to assess the efficacy and safety of ABT- 450/r/ABT-267\n      and ABT-333 co-administered with ribavirin compared to treatment with telaprevir and\n      peg-interferon/ribavirin in treatment-experienced HCV genotype 1- infected adults."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Females must be post-menopausal for more than 2 years or surgically sterile or\n             practicing abstinence/specific forms of birth control\n\n          -  Chronic hepatitis C, genotype 1-infection\n\n          -  Subject must have documentation of adherence to a prior pegIFN/RBV combination\n             therapy and meet one of the protocol definitions for treatment failure: null\n             responder, partial responder, relapser\n\n        Exclusion Criteria:\n\n          -  Positive screen for drugs or alcohol\n\n          -  Significant sensitivity to any drug\n\n          -  Use of contraindicated medications within 2 weeks of dosing\n\n          -  Abnormal laboratory tests\n\n          -  Positive hepatitis B surface antigen and anti-Human Immunodeficiency Virus antibody"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "65 Years", 
            "minimum_age": "18 Years"
        }, 

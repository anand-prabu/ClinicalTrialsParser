        "arm_group": {
            "arm_group_label": "Perceval S Aortic Heart Valve", 
            "arm_group_type": "Experimental", 
            "description": "Treatment with the Perceval S Aortic Heart Valve"
        }, 
        "brief_summary": {
            "textblock": "To demonstrate the safety and effectiveness of the Perceval S heart valve when used to\n      replace a diseased or dysfunctional aortic valve or aortic valve prosthesis."
        }, 
        "brief_title": "Perceval S Aortic Heart Valve Study- North America", 
        "completion_date": {
            "#text": "April 2018", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Aortic Valve Stenosis", 
            "Aortic Valve Stenosis With Insufficiency", 
            "Regurgitation, Aortic Valve", 
            "Aortic Valve Incompetence"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Aortic Valve Insufficiency", 

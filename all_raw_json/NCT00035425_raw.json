            }, 
            {
                "arm_group_label": "B.", 
                "description": "1gm every 12 hours. The dose of vancomycin intravenously may be adjusted for renal function, but the patient must be able to receive a 300 mL volume of intravenous fluid at the assigned dosing times of every 12 hours.", 
                "intervention_name": "vancomycin", 
                "intervention_type": "Drug"
            }

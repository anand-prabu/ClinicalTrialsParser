{
    "clinical_study": {
        "@rank": "139418", 
        "arm_group": {
            "arm_group_label": "Nexium"
        }, 
        "brief_summary": {
            "textblock": "The purpose of this investigation is to collect following data in patients given Nexium\n      capsule (Nexium) for long term in usual-post-marketing therapy to prevention of recurrence\n      of gastric ulcer or duodenal ulcer with Low dose Aspirin (LDA)."
        }, 
        "brief_title": "Nexium Capsules LDA Specific Clinical Experience Investigation", 
        "completion_date": {
            "#text": "January 2017", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Gastric Ulcer", 
            "Duodenal Ulcer"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Duodenal Ulcer", 
                "Stomach Ulcer", 
                "Ulcer"
            ]
        }, 
        "detailed_description": {
            "textblock": "Nexium capsules Specific Clinical Experience Investigation for long-term use concerning\n      prevention of recurrence of gastric ulcer or duodenal ulcer with Low dose Aspirin (LDA)."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Exclusion Criteria:\n\n          -  Patients having gastric ulcer/duodenal ulcer when Nexium is started (Active phase\n             (A1, A2) or healing phase (H1, H2) of Sakita-Miwa classification on endoscopy).\n\n          -  Patients who had been given Nexium for suppression of recurrence of gastric\n             ulcer/duodenal ulcer."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "N/A", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "The patients who are continuously given Low dose Aspirin (LDA) to suppress\n        thrombus/embolism and who will be given Nexium for the first time to suppress recurrence\n        of gastric ulcer and duodenal ulcer.\n\n        (Patients who have previous experience of Nexium given in the treatment for gastric ulcer\n        or duodenal ulcer can be registered to this S-CEI.)"
            }
        }, 
        "enrollment": {
            "#text": "2000", 

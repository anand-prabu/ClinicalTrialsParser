{
    "clinical_study": {
        "@rank": "129387", 
        "arm_group": {
            "arm_group_label": "Cariprazine", 
            "arm_group_type": "Experimental", 
            "description": "Cariprazine, flexible dose, oral administration, once daily for 26 weeks"
        }, 
        "brief_summary": {
            "textblock": "The objective of this study is to evaluate the long-term safety and tolerability of\n      cariprazine as an adjunctive treatment to antidepressant therapy (ADT) in patients with\n      Major Depressive Disorder (MDD)."
        }, 
        "brief_title": "To Evaluate the Long-term Safety and Tolerability of Cariprazine as an Adjunctive Treatment to Antidepressant Therapy (ADT) in Patients With Major Depressive Disorder (MDD)", 
        "completion_date": {
            "#text": "April 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Major Depressive Disorder", 
        "condition_browse": {
            "mesh_term": [
                "Depressive Disorder", 
                "Depression", 
                "Depressive Disorder, Major"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Patients who have provided consent prior to any study specific procedures\n\n          -  Meet the DSM-IV-TR criteria for MDD\n\n          -  New patients must have ongoing inadequate response to protocol allowed ADTs as\n             reported in Antidepressant Treatment Response Questionnaire (ATRQ)\n\n          -  For rollover patients from RGH-MD-72, completion of Study RGH-MD-72 (either\n             double-blind or single-blind treatment periods) with continued ADT treatment.\n\n        Exclusion Criteria:\n\n          -  Patients who do not meet the DSM-IV-TR criteria for MDD"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "65 Years", 
            "minimum_age": "18 Years"
        }, 
        "enrollment": {
            "#text": "500", 
            "@type": "Anticipated"
        }, 

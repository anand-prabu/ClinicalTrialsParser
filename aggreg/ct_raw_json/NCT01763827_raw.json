    "clinical_study": {
        "@rank": "46811", 
        "acronym": "MENDEL-2", 
        "arm_group": [
            {
                "arm_group_label": "Arm 1", 
                "arm_group_type": "Experimental", 
                "description": "Dose 1 of subcutaneous Evolocumab (AMG 145) every 2 weeks and placebo orally/daily"
            }, 
            {
                "arm_group_label": "Arm 2", 
                "arm_group_type": "Experimental", 
                "description": "Dose 2 of subcutaneous Evolocumab (AMG 145) monthly and placebo orally/daily"
            }, 
            {

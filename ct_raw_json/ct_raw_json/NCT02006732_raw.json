        "brief_summary": {
            "textblock": "The objective of this study is to assess the efficacy and safety of 12 weeks once daily\n      treatment with orally inhaled tiotropium + olodaterol FDC (delivered by the Respimat\n      inhaler) compared with tiotropium and placebo in patients with COPD."
        }, 
        "brief_title": "Tiotropium+Olodaterol Fixed Dose Combination (FDC) in Chronic Obstructive Pulmonary Disease (OTEMTO 2)", 
        "completion_date": {
            "#text": "December 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Pulmonary Disease, Chronic Obstructive", 
        "condition_browse": {
            "mesh_term": [
                "Chronic Disease", 
                "Lung Diseases", 
                "Respiration Disorders", 
                "Pulmonary Disease, Chronic Obstructive", 
                "Lung Diseases, Obstructive"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion criteria:\n\n          -  Diagnosis chronic obstructive pulmonary disease\n\n          -  Relatively stable airway obstruction with post FEV1 >=30 and < 80% predicted normal\n             and post FEV1/ FVC < 70%\n\n          -  Male or female patients, 40 years of age or more\n\n          -  Smoking history more than 10 pack years\n\n        Exclusion criteria:\n\n          -  Significant diseases other than COPD\n\n          -  History of asthma\n\n          -  COPD exacerbation in previous 3 months\n\n          -  Completion of pulmonary rehabilitation program within previous 6 weeks or current\n             participation in pulmonary rehabilitation program.\n\n          -  Pregnant or nursing women\n\n          -  Patients unable to comply with pulmonary medication restrictions"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "40 Years"
        }, 

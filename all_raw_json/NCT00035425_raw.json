{
    "clinical_study": {
        "@rank": "42132", 
        "arm_group": [
            {
                "arm_group_label": "A.", 
                "arm_group_type": "Experimental", 
                "description": "Patients will be stratified according to the use of prophylactic antibiotics.    Both groups may receive open-label gram-negative coverage with either ceftazidime, aztreonam, and/or aminoglycosides (gentamicin, tobramycin, amikacin).  Subjects will receive study medication intravenously every 12 hours for 7 to 28 days."
            }, 
            {
                "arm_group_label": "B.", 
                "arm_group_type": "Experimental"
            }
        ], 
        "brief_summary": {
            "textblock": "This study will treat patients who have fever and neutropenia (after cancer chemotherapy)\n      that is possibly due to a specific bacteria (gram positive bacteria)."
        }, 
        "brief_title": "Treatment of Neutropenic Patients With Fever Who Are Suspected to Have A Gram Positive Infection", 
        "completion_date": {
            "#text": "November 2002", 
            "@type": "Actual"
        }, 
        "condition": "Febrile Neutropenia", 
        "condition_browse": {
            "mesh_term": [
                "Fever", 
                "Neutropenia"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n        Patients must have neutropenia (ANC less than 500) with fever (oral temp 38.3 C).\n\n        Patients must have a cancer with recent chemotherapy and risks factors for gram positive\n        infections.\n\n        Exclusion Criteria:\n\n        Patients with fever due to known causes. Patients with HIV. Patients with recent bone\n        marrow transplant. Patients with an infected indwelling catheter that cannot be removed.\n        Patients who have received more than one day of another antibiotic before entering the\n        trial.\n\n        Patients with endocarditis, osteomyelitis, meningitis, CNS infections."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "13 Years"
        }, 
        "enrollment": {
            "#text": "120", 
            "@type": "Actual"
        }, 
        "firstreceived_date": "May 3, 2002", 

        "brief_summary": {
            "textblock": "This study is an FDA post-approval study of the Nit-Occlud PDA Device to evaluate 5 year (60\n      months) safety and effectiveness of the device."
        }, 
        "brief_title": "Nit-Occlud PDA Post-Approval Study", 
        "completion_date": {
            "#text": "April 2022", 
            "@type": "Anticipated"
        }, 
        "condition": "Patent Ductus Arteriosus (PDA)", 
        "condition_browse": {
            "mesh_term": "Ductus Arteriosus, Patent"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Angiographically confirmed PDA with minimum diameter < 4 mm.\n\n          -  Weight is \u2265 5 kg.\n\n          -  Age 6 months to 21 years.\n\n        Exclusion Criteria:\n\n          -  Cardiac anomalies requiring surgery.\n\n          -  Known bleeding or coagulation disorder.\n\n          -  Febrile illness within 7 days of planned procedure.\n\n          -  Pregnancy.\n\n          -  Pulmonary hypertension with increased pulmonary vascular resistance (\u2265 5 Wood Units).\n\n          -  Hypersensitivity to contrast medium.\n\n          -  Known nickel allergy."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "21 Years", 
            "minimum_age": "6 Months", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Patients age 6 months to 21 years weighing > 5kg with an angiographically confirmed PDA\n        with a minimum diameter of <4 mm."
            }
        }, 

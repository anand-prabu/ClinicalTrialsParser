  <study_design>Observational Model: Case Control, Time Perspective: Prospective</study_design>
  <primary_outcome>
    <measure>Brain Structure</measure>
    <time_frame>Age 2 weeks</time_frame>
    <safety_issue>No</safety_issue>
    <description>We will use high resolution volumetric magnetic resonance imaging (MRI) and Diffusion tensor imaging (DTI) to measure the brain structure of 2 week old infants</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Brain Function</measure>
    <time_frame>Age 2 weeks</time_frame>
    <safety_issue>No</safety_issue>
    <description>We will use high resolution Resting state functional magnetic resonance imaging (rsfMRI) to measure the brain function of 2 week old infants</description>
  </secondary_outcome>
  <number_of_groups>2</number_of_groups>
  <enrollment type="Anticipated">60</enrollment>

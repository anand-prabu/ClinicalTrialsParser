    "clinical_study": {
        "@rank": "144513", 
        "arm_group": [
            {
                "arm_group_label": "Arm A: BMS-813160 150 mg & Placebo matching with BMS-813160", 
                "arm_group_type": "Experimental", 
                "description": "BMS-813160 150 mg capsules by mouth in AM and Placebo matching with BMS-813160 in PM for 12 weeks"
            }, 
            {
                "arm_group_label": "Arm B: BMS-813160 300 mg", 
                "arm_group_type": "Experimental", 
                "description": "BMS-813160 300 mg capsules by mouth twice daily for 12 weeks"
            }, 
            {
                "arm_group_label": "Arm C: Placebo matching with BMS-813160", 
                "arm_group_type": "Placebo Comparator", 
                "description": "Placebo matching with BMS-813160 0 mg capsules by mouth twice daily for 12 weeks"
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to determine whether BMS-813160 will reduce the amount of\n      protein loss in the urine of subjects with type 2 diabetes and diabetic kidney disease"
        }, 
        "brief_title": "Study to Evaluate the Effects of BMS-813160 on Protein Loss in the Urine of Subjects With Type 2 Diabetes and Diabetic Kidney Disease", 
        "completion_date": {
            "#text": "August 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Diabetic Kidney Disease", 
        "condition_browse": {
            "mesh_term": [
                "Diabetes Mellitus, Type 2", 
                "Diabetic Nephropathies", 
                "Kidney Diseases"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Clinical diagnosis of type 2 diabetes mellitus with macroalbuminuria (UACR between\n             200 and 3500 mg/g)\n\n          -  Clinical diagnosis of stable diabetic retinopathy\n\n          -  Background angiotensin converting enzyme inhibitor (ACEI) or angiotensin-receptor\n             blocker (ARB) therapy\n\n        Exclusion Criteria:\n\n          -  Clinical diagnosis of type 1 diabetes\n\n          -  Unstable cardiovascular, metabolic, or other chronic disease status\n\n          -  Estimated glomerular filtration rate (eGFR) <30 mL/min/1.73 m2\n\n          -  High risk of infection or  immune compromise\n\n          -  Clinically significant ECG conduction abnormalities\n\n          -  Drugs with significant potential to affect BMS-813160 exposure"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "85 Years", 
            "minimum_age": "30 Years"
        }, 

            "@type": "Anticipated"
        }, 
        "condition": "Congenital Atransferrinemia", 
        "condition_browse": {
            "mesh_term": "Metal Metabolism, Inborn Errors"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Established diagnosis of atransferrinemia, defined as serum levels of transferrin\n             below 40 mg/dl\n\n          -  Informed consent\n\n        Exclusion Criteria:\n\n          -  Known with allergic reactions against human plasma or plasma products\n\n          -  Having detectable anti-immunoglobulin A antibodies"
            }, 

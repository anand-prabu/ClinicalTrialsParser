        "arm_group": {
            "arm_group_label": "Brentuximab Vedotin + Bendamustine", 
            "arm_group_type": "Experimental", 
            "description": "Brentuximab vedotin 1.8mg/kg every 3 weeks and bendamustine"
        }, 
        "brief_summary": {
            "textblock": "The purpose of this study is to assess safety and efficacy of brentuximab vedotin in\n      combination with bendamustine in patients with relapsed or refractory Hodgkin lymphoma. It\n      is an open-label, 2-stage study designed to determine the recommended dose level of\n      bendamustine in combination with brentuximab vedotin. The study will assess the safety\n      profile of the combination treatment and determine what proportion of patients achieve a\n      complete remission."
        }, 
        "brief_title": "Study of Brentuximab Vedotin Combined With Bendamustine in Patients With Hodgkin Lymphoma", 
        "completion_date": {
            "#text": "December 2017", 
            "@type": "Anticipated"
        }, 
        "condition": "Hodgkin Disease", 
        "condition_browse": {
            "mesh_term": [
                "Hodgkin Disease", 
                "Lymphoma"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Histopathological diagnosis of classical Hodgkin lymphoma\n\n          -  Failed standard front-line therapy\n\n          -  Measurable disease of at least 1.5 cm as documented by radiographic technique\n\n          -  Eastern Cooperative Oncology Group performance status less than or equal to 2\n\n        Exclusion Criteria:\n\n          -  Received prior salvage therapy, including radiotherapy\n\n          -  Chemotherapy, radiotherapy, biologics, and/or other treatment with immunotherapy not\n             completed 4 weeks prior to first dose of study drug\n\n          -  Concurrent use of other investigational agents"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

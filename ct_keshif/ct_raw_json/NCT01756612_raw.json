{
    "clinical_study": {
        "@rank": "21587", 
        "arm_group": {
            "arm_group_label": "Cohort"
        }, 
        "brief_summary": {
            "textblock": "This prospective, multicenter, observational study will evaluate the impact of comorbidity\n      factors on the hemoglobin level in patients with chronic kidney disease not on dialysis who\n      are initiated on treatment with Mircera (methoxy polyethylene glycol-epoetin beta). Data\n      will be collected from patients for 9 months after initiation of Mircera treatment."
        }, 
        "brief_title": "COMETE Study: Comorbidity Mircera Evolution of Hemoglobin Rate; A Non-Interventional Study of Mircera (Methoxy Polyethylene Glycol-Epoetin Beta) in Patients With Chronic Kidney Disease", 
        "completion_date": {
            "#text": "September 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Kidney Disease, Chronic", 
        "condition_browse": {
            "mesh_term": [
                "Chronic Disease", 
                "Kidney Diseases", 
                "Renal Insufficiency, Chronic", 
                "Kidney Failure, Chronic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Adult patients, >/= 18 years of age\n\n          -  Patients with chronic kidney disease not on dialysis\n\n          -  Treatment-na\u00efve for ESAs (not having received ESAs in the 6 months prior to inclusion\n             in this study)\n\n          -  Hemoglobin level < 10 g/dL at inclusion\n\n          -  Patients for whom the treating physician has decided to initiate treatment with\n             Mircera for medical reasons prior to study start\n\n        Exclusion Criteria:\n\n          -  Functional renal transplant\n\n          -  Current participation in a clinical trial in anemia due to chronic kidney disease"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Probability Sample", 
            "study_pop": {
                "textblock": "Patients with chronic kidney disease not on dialysis and ESA na\u00efve"
            }
        }, 

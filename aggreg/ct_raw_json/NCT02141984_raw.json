    "clinical_study": {
        "@rank": "143105", 
        "arm_group": {
            "arm_group_label": "Polyarticular juvenile idiopathic arthritis patients", 
            "description": "Polyarticular JIA patients between 4-17 age whom the response to previous disease-modifying anti rheumatic drug therapy has been inadequate"
        }, 
        "brief_summary": {
            "textblock": "Approximately 600 pediatric patients prescribed Humira Injection in usual practice according\n      to the approved Korean product label will be registered into this observational study.\n      Baseline data will be obtained at enrollment including demographics, underlying diseases and\n      complications especially in regard to purified protein derivative (PPD) skin test, chest\n      X-ray. At routine visits for Humira Injection administration which will occur according to\n      usual medical practice, concomitant medication information and adverse events information\n      will be collected for up to 70 days after the last administration of Humira."
        }, 
        "brief_title": "Surveillance of Humira in Korean JIA Patients", 
        "completion_date": {
            "#text": "April 2016", 
            "@type": "Anticipated"
        }, 
        "condition": "Polyarticular Juvenile Idiopathic Arthritis", 
        "condition_browse": {
            "mesh_term": [
                "Arthritis", 
                "Arthritis, Juvenile Rheumatoid"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          1. Patients who were diagnosed with polyarticular juvenile idiopathic arthritis(JIA) and\n             were prescribed Humira by their physician according to the local label.\n\n          2. Polyarticular juvenile idiopathic arthritis (JIA) patients for whom the response to\n             previous disease-modifying anti rheumatic drug therapy has been inadequate\n\n          3. Patients who give written authorization form to use their personal and health data\n             from legal parents or representative.\n\n        Physician will refer to the product market authorization (label) for inclusion criteria.\n\n        Exclusion Criteria:\n\n          1. Patients with known hypersensitivity to Humira or any of its excipients.\n\n          2. Patients who is participating on other clinical trials. Physician will refer to the\n             product market authorization (label) for exclusion criteria."
            }, 

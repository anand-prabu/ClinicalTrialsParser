  <phase>N/A</phase>
  <study_type>Observational</study_type>
  <study_design>Observational Model: Cohort, Time Perspective: Prospective</study_design>
  <primary_outcome>
    <measure>Percentage of patients with a variation of Hb level of +/- 1 g/dL between the first Mircera injection and Month 6</measure>
    <time_frame>6 months</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <secondary_outcome>
    <measure>Percentage of patients with Hb value within the range of 10 - 12 g/dL</measure>
    <time_frame>6 months</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in Hb level</measure>
    <time_frame>from baseline to Month 6</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Monthly dose of Mircera</measure>

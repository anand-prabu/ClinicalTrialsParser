            }
        ], 
        "brief_summary": {
            "textblock": "A study to evaluate chronic hepatitis C infection."
        }, 
        "brief_title": "A Study to Evaluate Chronic Hepatitis C Infection in Treatment Experienced Adults", 
        "completion_date": {
            "#text": "October 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Chronic Hepatitis C Infection", 
        "condition_browse": {

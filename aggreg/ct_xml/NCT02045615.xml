  <brief_title>Removal Wichita Fusion Nail After Knee Arthrodesis</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Universitaire Ziekenhuizen Leuven</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

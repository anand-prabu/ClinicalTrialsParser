  <brief_title>Effects of Legume Intake on Cardiovascular Risk Factors Among Adults at Risk for Diabetes</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Isfahan University of Medical Sciences</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

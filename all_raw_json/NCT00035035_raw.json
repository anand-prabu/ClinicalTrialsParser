{
    "clinical_study": {
        "@rank": "58971", 
        "brief_summary": {
            "textblock": "Definition: This study will compare the drug GEMZAR to a combination of GEMZAR plus ALIMTA\n      for the treatment of cancer of the pancreas.  Patients may be able to participate in this\n      study if they have cancer in their pancreas that cannot be removed by surgery OR that has\n      spread to a new site in their body."
        }, 
        "brief_title": "A Phase 3 Trial of ALIMTA (LY231514, Pemetrexed) Plus GEMZAR Versus GEMZAR in Patients With Unresectable or Metastatic Cancer of the Pancreas.", 
        "condition": [
            "Pancreatic Neoplasms", 
            "Metastases, Neoplasm"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Neoplasms", 
                "Neoplasm Metastasis", 
                "Neoplasms, Second Primary", 
                "Pancreatic Neoplasms"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Pancreas cancer that cannot be removed by surgery OR that has spread to a new site in\n             your body.\n\n          -  You must have one tumor that can be physically measured or scanned by the doctor.\n\n          -  You have not had any chemotherapy, immunotherapy, biologic therapy, or hormonal\n             therapy for pancreas cancer, including 5-FU.\n\n          -  You cannot have any radiation for 4 weeks before starting this study, and you cannot\n             have radiation to the whole pelvis.\n\n        Exclusion Criteria:\n\n          -  Treatment with any medication or device that has not been approved by regulatory\n             agencies, like the FDA, at the time you enter the study.\n\n          -  Documentation that the cancer has spread to your brain.\n\n          -  Pregnant or breastfeeding.\n\n          -  A second primary cancer.\n\n          -  Unable to stop aspirin or non-steroidal anti-inflammatory agents (like ibuprofen) for\n             5-8 days."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"
        }, 

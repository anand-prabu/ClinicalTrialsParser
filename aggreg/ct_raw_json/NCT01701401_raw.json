        "primary_outcome": [
            {
                "description": "SVR12 is defined as HCV RNA level < the lower limit of quantification (LLOQ) 12 weeks after last dose of study drug.", 
                "measure": "Proportion of participants with sustained virologic response (SVR) 12 weeks after discontinuation of study drug", 
                "safety_issue": "No", 
                "time_frame": "Posttreatment Week 12"
            }, 
            {
                "measure": "Incidence of any AE leading to permanent discontinuation of study", 
                "safety_issue": "No", 
                "time_frame": "Up to 24 weeks"
            }
        ], 
        "required_header": {

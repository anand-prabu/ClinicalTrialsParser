  <brief_title>A Long-Term Safety and Efficacy Study of AGN-199201 in Patients With Persistent Erythema Associated With Rosacea</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>

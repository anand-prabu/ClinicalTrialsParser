        "acronym": "APPROACH-IS", 
        "arm_group": {
            "arm_group_label": "Adults with congenital heart disease"
        }, 
        "brief_summary": {
            "textblock": "The purpose of this study is to examine the differences in perceived health, psychosocial\n      functioning, behavioral outcomes and quality of life of adults with congenital heart disease\n      who are living in different areas of the world, and how these differences can be understood\n      (e.g., differences in sense of coherence or illness perceptions)."
        }, 
        "brief_title": "Patient-Reported Outcomes in Adults With Congenital Heart Disease", 
        "completion_date": {
            "#text": "December 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Heart Defects, Congenital", 
        "condition_browse": {
            "mesh_term": [
                "Congenital Abnormalities", 
                "Heart Defects, Congenital", 
                "Heart Diseases"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Diagnosed with congenital heart disease, defined as: a gross structural abnormality\n             of the heart or intra-thoracic great vessels that is actually or potentially of\n             functional significance (including mild, moderate, and severe heart defects)\n\n          -  18 years of age or older\n\n          -  Diagnosed under the age of ten, i.e. before adolescence\n\n          -  Continuing follow-up at an adult congenital heart disease center or included in a\n             national/regional registry\n\n          -  Physical, cognitive, and language abilities to complete self-report questionnaires\n\n        Exclusion Criteria:\n\n          -  Prior heart transplantation\n\n          -  Isolated pulmonary hypertension\n\n          -  Syndromes affecting cognitive abilities"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Adults with congenital heart disease who are in continuing follow-up at an adult\n        congenital heart disease center or included in a national/regional registry."
            }
        }, 

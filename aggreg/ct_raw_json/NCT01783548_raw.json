        "arm_group": [
            {
                "arm_group_label": "BDP Nasal Aerosol", 
                "arm_group_type": "Experimental", 
                "description": "BDP nasal aerosol: 80 mcg dose once daily. Participants/patients will administer 40 mcg BDP (one spray per nostril) during the 12-week Treatment Period."
            }, 
            {
                "arm_group_label": "Placebo Nasal Aerosol", 

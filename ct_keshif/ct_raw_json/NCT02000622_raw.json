            "secondary_id": "2013-005137-20"
        }, 
        "intervention": [
            {
                "arm_group_label": "Olaparib", 
                "description": "Patients will be administered olaparib orally twice daily (bid) at 300 mg. Two (2) x 150 mg olaparib tablets should be taken at the same times each morning and evening of each day, approximately 12 hours apart with approximately 240 mL of water.", 
                "intervention_name": "Olaparib", 
                "intervention_type": "Drug"
            }, 
            {
                "arm_group_label": "Physician's choice chemotherapy", 
                "description": "Investigators will declare one of the following regimens:\nCapecitabine 2500 mg/m2 po daily (divided in 2 doses) x 14 days, repeat every 21 days\nVinorelbine 30 mg/m2 IV Day 1 and Day 8, repeat every 21 days\nEribulin 1.4 mg/m2 IV Day 1 and Day 8, repeat every 21 days", 
                "intervention_name": "Physician's choice chemotherapy", 
                "intervention_type": "Drug"
            }
        ], 

  <brief_title>Eradication of Prostate Cancer Using an Adenosine-5'-Triphosphate (ATP) Inhibitor</brief_title>
  <official_title>Phase II Eradication of Prostate Cancer Using an Adenosine-5'-Triphosphate(ATP)Inhibitor.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Optimal Health Research</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>

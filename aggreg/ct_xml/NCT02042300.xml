  <eligibility>
    <study_pop>
      <textblock>
        patients with XIENCE XPEDITION™ stent
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Age 19 and more

          -  Intervention with XIENCE XPEDITION™ everolimus eluting coronary stent

          -  Agreed with written informed consent form

        Exclusion Criteria:

          -  Intervention with XIENCE XPEDITION™ everolimus eluting coronary stent and other drug
             eluting stent at the same time

          -  Life expectancy of 1year and under

          -  Cardiac shock
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>19 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>

    "clinical_study": {
        "@rank": "116064", 
        "arm_group": [
            {
                "arm_group_label": "Secukinumab (AIN457) 150 mg s.c.", 
                "arm_group_type": "Experimental", 
                "description": "secukinumab 150 mg (1.0 ml) plus placebo (1.0 ml) at Baseline, Weeks 1, 2, 3 and 4, followed by dosing every four weeks starting at Week 4."
            }, 
            {
                "arm_group_label": "Secukinumab (AIN457) 300 mg s.c.", 
                "arm_group_type": "Experimental", 
                "description": "secukinumab 300 mg (2 x 1.0 ml) at Baseline, Weeks 1, 2, 3, 4, followed by dosing every four weeks starting at Week 4."
            }, 
            {
                "arm_group_label": "Placebo", 
                "arm_group_type": "Placebo Comparator", 
                "description": "Placebo (2 x 1.0 ml) at Baseline, Weeks 1, 2, 3, 4, followed by dosing every four weeks starting at Week 4."
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to provide 24 - 52 week efficacy, safety and tolerability data,\n      as well as up to 3-year efficacy, safety and tolerability data in subjects with active\n      Psoriatic Arthritis despite current or previous nonsteroidal anti-inflammatory drug (NSAID),\n      disease-modifying antirheumatic drug (DMARD) therapy and/or previous anti-tumor necrosis\n      factor alpha (TNF\u03b1) therapy."
        }, 
        "brief_title": "24 Week Efficacy and 3-year Safety and Efficacy of Secukinumab in Active Psoriatic Arthritis", 
        "completion_date": {
            "#text": "January 2018", 
            "@type": "Anticipated"
        }, 
        "condition": "Psoriatic Arthritis", 
        "condition_browse": {
            "mesh_term": [
                "Arthritis", 
                "Arthritis, Psoriatic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Diagnosis of Psoriatic Arthritis (PsA) classified by ClASsification criteria for\n             Psoriatic ARthritis (CASPAR) criteria.\n\n          -  Rheumatoid factor and anti-cyclic citrullinated peptide (CCP) antibodies negative.\n\n          -  Diagnosis of active plaque psoriasis or nail changes consistent with psoriasis.\n\n          -  Inadequate control of symptoms with NSAID.\n\n        Exclusion Criteria:\n\n          -  Chest X-ray or chest magnetic resonance imaging (MRI) with evidence of ongoing\n             infectious or malignant process.\n\n          -  Subjects taking high potency opioid analgesics.\n\n          -  Previous exposure to secukinumab or other biologic drug directly targeting\n             interleukin-17 (IL-17) or IL-17 receptor.\n\n          -  Ongoing use of prohibited psoriasis treatments / medications.\n\n          -  Subjects who have ever received biologic immunomodulating agents except for those\n             targeting TNF\u03b1.\n\n          -  Previous treatment with any cell-depleting therapies."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

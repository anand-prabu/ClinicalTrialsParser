  <brief_title>The Effects on Growth and Tolerance of an Infant Formula Fed to Term Infants</brief_title>
  <official_title>The Effects on Growth and Tolerance of an Infant Formula Fed to Term Infants</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Mead Johnson Nutrition</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>

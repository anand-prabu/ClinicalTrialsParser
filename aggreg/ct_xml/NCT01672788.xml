  <primary_outcome>
    <measure>AUC0-infinity (area under the concentration-time curve of the analyte in plasma over the time interval from 0 extrapolated to infinity)</measure>
    <time_frame>up to 72 hours after drug intake</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <primary_outcome>
    <measure>Cmax (maximum measured concentration of the analyte in plasma)</measure>
    <time_frame>up to 72 hours after drug intake</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <secondary_outcome>
    <measure>AUC0-tz (area under the concentration-time curve of the analyte in plasma over the time interval from 0 to the last quantifiable data point)</measure>
    <time_frame>up to 72 hours after drug intake</time_frame>
    <safety_issue>No</safety_issue>
  </secondary_outcome>

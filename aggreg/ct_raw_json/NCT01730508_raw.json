    "clinical_study": {
        "@rank": "14823", 
        "arm_group": {
            "arm_group_label": "Cohort"
        }, 
        "brief_summary": {
            "textblock": "This multicenter, prospective, observational study will evaluate the use in clinical\n      practice and the efficacy and safety of Pegasys (peginterferon alfa-2a) in Chinese patients\n      with HBeAg negative chronic hepatitis B. Patients receiving Pegasys according to the local\n      label will be followed for the duration of their treatment and for one year after cessation\n      of treatment."
        }, 
        "brief_title": "An Observational Study of Pegasys (Peginterferon Alfa-2a) in Chinese Patients With HBeAg Negative Chronic Hepatitis B", 
        "completion_date": {
            "#text": "November 2016", 
            "@type": "Anticipated"
        }, 
        "condition": "Hepatitis B, Chronic", 
        "condition_browse": {
            "mesh_term": [
                "Hepatitis", 
                "Hepatitis A", 
                "Hepatitis B", 
                "Hepatitis, Chronic", 
                "Hepatitis B, Chronic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Adult patients, >/= 18 years of age\n\n          -  HBeAg negative serologically proven chronic hepatitis B with or without cirrhosis\n\n          -  Serum ALT > ULN (upper limit of normal) but </= 10 x ULN\n\n          -  HBV DNA >/= 2000 IU/mL\n\n        Exclusion Criteria:\n\n          -  Contraindications to Pegasys as detailed in the label\n\n          -  Co-infection with hepatitis A, hepatitis C or HIV\n\n          -  Pregnant or lactating women\n\n          -  Patients should not receive concomitant therapy with telbivudine"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

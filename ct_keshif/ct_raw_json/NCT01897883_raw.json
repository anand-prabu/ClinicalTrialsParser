{
    "clinical_study": {
        "@rank": "141225", 
        "arm_group": {
            "arm_group_label": "One Group", 
            "description": "Full Analysis Set (FAS) will be the primary analysis set.  All post ischemic stroke patients within  6-12 months from attack (i.e., with inclusion criterion No.2 fulfilled) except the screening failure patients, i.e., those who withdraw from the study once the informed consent is given, will be included in the FAS."
        }, 
        "brief_summary": {
            "textblock": "The purpose of this study is to provide current and reliable data of dyslipidimia management\n      together with control situation of blood pressure  and glucose for post-stroke patients\n      within 6-12 months from attack,  and also the association between patient characteristics\n      and control rate of lipids  in this population.\n\n      It is an non-interventional study, no study specified treatment is required.Approximately\n      5000 post ischemic stroke patients within 6-12 months from attack  will enter into the\n      study. Fifty  centres from different regions of  China will participate in this study."
        }, 
        "brief_title": "Dyslipidemia Management in Chinese Post Stroke Patients", 
        "completion_date": {
            "#text": "August 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Brain Ischemia,Stoke", 
        "condition_browse": {
            "mesh_term": [
                "Brain Ischemia", 
                "Ischemia", 
                "Dyslipidemias"
            ]
        }, 
        "detailed_description": {
            "textblock": "This is a multicentre, cross-sectional, observational study, aiming  to investigate the\n      prevalence  and control situation of dyslipidimia in China post ischemic stroke patients\n      within 6-12 months from attack. Patients will be screened consecutively, eligible subjects\n      will be interviewed by investigator and finish a questionire during the visit. Investigator\n      will collect relevant medical history, physical exam results and lab test results including\n      blood lipid (i.e. Triglyceride, Total Cholesterol, LDL-C, HDL-C) and glucose(HbA1c if\n      available).Approximately 5000 post ischemic stroke patients within 6-12 months from attack\n      will enter into the study. Fifty  centres from different regions of  China will participate\n      in this study."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          1. Male or female with age \u2265 18 years\n\n          2. Post ischemic stroke patients  within 6-12 months from attack\n\n          3. written informed consent is provided to participant in the study\n\n        Exclusion Criteria:\n\n          1. Significant medical or psychological condition that make patients can not finish the\n             questionnaire independently or with the aids of his/her legal representatives\n\n          2. The patient is or will be  in another clinical study\n\n          3. Previous enrolment in the present study"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Post -ischemic stroke patients within 6-12 months from attack, with age \u2265 18  years old.\n        Subject ( or his legal representative) must demonstrate his or her willingness to\n        participate in the survey and comply with its procedures by signing a written consent.\n        Subjects who are unwilling or unable to provide informed consent will be excluded"
            }
        }, 

    "clinical_study": {
        "@rank": "168013", 
        "arm_group": {
            "arm_group_label": "mavrilimumab Q2W + MTX", 
            "arm_group_type": "Experimental", 
            "description": "GM-CSFR alpha inhibitor"
        }, 
        "brief_summary": {
            "textblock": "A clinical study to investigate the safety of mavrilimumab, an antibody being developed for\n      the treatment of moderate to severe rheumatoid arthritis, an inflammatory condition that\n      affects the joints."
        }, 
        "brief_title": "A Long Term Safety Study of Mavrilimumab in Adult Subjects With Rheumatoid Arthritis", 
        "completion_date": {
            "#text": "January 2020", 
            "@type": "Anticipated"
        }, 
        "condition": "Rheumatoid Arthritis", 
        "condition_browse": {
            "mesh_term": [
                "Arthritis", 
                "Arthritis, Rheumatoid"
            ]
        }, 
        "detailed_description": {
            "textblock": "Despite the therapeutic improvements with recent biologic agents approved for rheumatoid\n      arthritis (RA), there is still significant unmet medical need for the treatment of subjects\n      with this chronic disease to achieve a faster, more complete response, and higher rates of\n      remission. This study is an open-label extension study for subjects who have participated in\n      one of the qualifying development program studies with mavrilimumab. Participation in this\n      study will allow these subjects to continue to receive long-term treatment with\n      mavrilimumab. The data from this study will provide an evaluation of the long-term safety of\n      mavrilimumab in adult subjects with RA. In addition, long-term exploratory efficacy outcomes\n      such as joint damage and disability will be evaluated."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Subjects who have completed the treatment period of the qualifying study or will have\n             failed to respond adequately to investigational product at a predefined time point in\n             the qualifying study regardless of their initial randomization.\n\n          -  No evidence of clinically uncontrolled respiratory disease to be confirmed by a local\n             pulmonologist\n\n        Exclusion Criteria:\n\n          -  Subjects who have been permanently discontinued from investigational product in\n             previous qualifying study.\n\n          -  Any new conditions or worsening of any pre-existing conditions as defined in the\n             protocol."
            }, 

{
    "clinical_study": {
        "@rank": "65411", 
        "arm_group": {
            "arm_group_label": "Ivacaftor Treated"
        }, 
        "brief_summary": {
            "textblock": "This study is designed to evaluate the risk of cataracts (lens opacities) and describe the\n      best corrected distance vision (with glasses/contacts for those who wear them) of pediatric\n      patients with Cystic Fibrosis who are 11 years of age or younger at the time of ivacaftor\n      treatment initiation and are receiving or planning to receive commercially-available\n      ivacaftor in the US."
        }, 
        "brief_title": "An Ocular Safety Study of Ivacaftor-Treated Pediatric Patients 11 Years of Age or Younger With Cystic Fibrosis", 
        "completion_date": {
            "#text": "May 2016", 
            "@type": "Anticipated"
        }, 
        "condition": "Cystic Fibrosis", 
        "condition_browse": {
            "mesh_term": [
                "Cystic Fibrosis", 
                "Fibrosis"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          1. Subject must be 11 years of age or younger at the time of treatment initiation with\n             ivacaftor (as part of clinical trial) or commercially-available ivacaftor.\n\n          2. Subject must reside in the US and be receiving or planning to receive\n             commercially-available ivacaftor.\n\n        Exclusion Criteria:\n\n          1. Subject is enrolled in an ivacaftor clinical study in which ivacaftor exposure is\n             mandated by the protocol.\n\n          2. Subject has received surgery for cataracts"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "11 Years", 
            "minimum_age": "6 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Pediatric patients with CF who are 11 years of age or younger at the time of initiation of\n        treatment with ivacaftor and are receiving or planning to receive commercially-available\n        ivacaftor in the United States (US)"
            }
        }, 

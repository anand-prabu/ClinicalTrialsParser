{
    "clinical_study": {
        "@rank": "83855", 
        "arm_group": [
            {
                "arm_group_label": "Group Bulgaria", 
                "description": "Subjects in this group include infants/children from Bulgaria, less than five years of age with home visits by the GP/paediatrician for the treatment of AGE or brought to the GP/paediatrician for the treatment of AGE."
            }, 
            {
                "arm_group_label": "Group Latvia", 
                "description": "Subjects in this group include infants/children from Latvia, less than five years of age with home visits by the GP/paediatrician for the treatment of AGE or brought to the GP/paediatrician for the treatment of AGE."
            }
        ], 
        "brief_summary": {
            "textblock": "This study aims to estimate the real situation of rotavirus gastroenteritis (RV GE) in the\n      primary care settings of both Bulgaria and Latvia in children less than 5 years of age."
        }, 
        "brief_title": "A Study to Estimate the Occurrence of Rotavirus Gastroenteritis in Children < 5 Years of Age, in Bulgaria and Latvia", 
        "completion_date": {
            "#text": "July 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Infections, Rotavirus", 
        "condition_browse": {
            "mesh_term": [
                "Gastroenteritis", 
                "Rotavirus Infections"
            ]
        }, 
        "detailed_description": {
            "textblock": "A Primary care setting will include independent general practitioners (GP) (specialised in\n      paediatrics) or independent paediatricians.\n\n      Stool samples will be collected from all enrolled subjects at Visit 1 (Day 0) and tested\n      immediately after collection. The stool sample will be stored for 24 hours only, in case the\n      test cannot be performed immediately after collection."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Infants/children with home visits by the GPGP/paediatrician for the treatment of AGE\n             or brought to the GP/paediatrician for AGE treatment during the study period.\n\n          -  A male or female infant/child less than five years of age at the time of the\n             GP/paediatrician visit.  A child becomes ineligible on the day of her/his fifth\n             birthday.\n\n          -  Written informed consent obtained from the parents/legally acceptable representative\n             (LARs) of the subject.\n\n          -  Subjects who the investigator believes that his/her, parents/LARs can and will comply\n             with the requirements of the protocol.\n\n        Exclusion Criteria:\n\n        \u2022  Child in care."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "5 Years", 
            "minimum_age": "N/A", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Infants/children less than five years of age with home visits by the GP/paediatrician for\n        the treatment of AGE or brought to the GP/paediatrician for the treatment of AGE."
            }
        }, 

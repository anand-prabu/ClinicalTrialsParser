  <brief_title>The Effect of Synching Prescription Refills on Adherence</brief_title>
  <official_title>A Randomized Controlled Trial to Test a Synchronized Prescription Refill Program to Improve Medication Adherence</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Pennsylvania</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

  <brief_title>Motec Versus Remotion Total Wrist Arthroplasty - A Prospective Study</brief_title>
  <official_title>Motec Versus Remotion Total Wrist Arthroplasty - A Prospective Randomized Study.</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Oslo University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>

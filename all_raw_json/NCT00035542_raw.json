    "clinical_study": {
        "@rank": "70767", 
        "brief_summary": {
            "textblock": "The purpose of this clinical research study is to see if Glucovance, a medication currently\n      approved for use in adults with type 2 diabetes, can control type 2 diabetes safely and\n      effectively in children 9 to 16 years of age."
        }, 
        "brief_title": "A Research Study to Determine the Safety and Efficacy of Glucovance Compared to Metformin and Glyburide in Children and Adolescents With Type 2 Diabetes.", 
        "completion_date": {
            "#text": "October 2003", 
            "@type": "Actual"
        }, 
        "condition": "Diabetes Mellitus, Non-Insulin-Dependent", 
        "condition_browse": {

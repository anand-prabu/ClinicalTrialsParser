    "clinical_study": {
        "@rank": "159859", 
        "acronym": "ONCOCIP", 
        "arm_group": {
            "arm_group_label": "PCVCs", 
            "description": "Cohort of cancer patients with indication PCVC under the usual practice"
        }, 
        "brief_summary": {
            "textblock": "The purpose of this study is to identify risk factors for thromboembolic and /or infectious\n      complications in 3,000  patients with solid tumor and receiving a Percutaneous Central\n      Venous Catheter (PCVC).  A better understanding of the risk factors for these complications\n      in this population would then allow interventional studies to be proposed to assess the\n      benefit of prophylactic procedures in a sub-group of patients at most risk."
        }, 
        "brief_title": "Risk Factors for Thromboembolic and Infectious Complications Related to Percutaneous Central Venous Catheters in Cancer - Prospective Multicenter Study", 
        "completion_date": {
            "#text": "March 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Cancer", 
        "condition_browse": {
            "mesh_term": [
                "Communicable Diseases", 
                "Infection"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Patient with a neoplastic solid tumour with an indication for insertion of a PCVC.\n\n        Exclusion Criteria:\n\n          -  Haematological neoplasm pathology, PCVC removal scheduled within 2 weeks of\n             insertion,\n\n          -  Therapeutic doses of anticoagulants required at inclusion,\n\n          -  PCVC inserted by the femoral vein"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 

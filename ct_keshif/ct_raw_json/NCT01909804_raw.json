        "primary_outcome": [
            {
                "description": "SVR12 is defined as HCV RNA < the lower limit of quantitation (LLOQ < 25 IU/mL) 12 weeks following the last dose of study drug.", 
                "measure": "Proportion of participants with sustained virologic response 12 weeks after discontinuation of therapy (SVR12)", 
                "safety_issue": "No", 
                "time_frame": "Posttreatment Week 12"
            }, 
            {
                "measure": "Incidence of adverse events leading to permanent discontinuation of study drug", 
                "safety_issue": "No", 
                "time_frame": "Baseline to Week 12"
            }
        ], 
        "required_header": {

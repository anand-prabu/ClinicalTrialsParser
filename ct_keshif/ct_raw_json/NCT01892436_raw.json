{
    "clinical_study": {
        "@rank": "62063", 
        "arm_group": [
            {
                "arm_group_label": "Secukinumab 75mg", 
                "arm_group_type": "Experimental", 
                "description": "Subjects will continue to receive secukinumab 75mg in PFS (same dose as that was received in core study CAIN457F2306) every 4 weeks up to Week 256."
            }, 
            {
                "arm_group_label": "Secukinumab 150mg", 
                "arm_group_type": "Experimental", 
                "description": "Subjects will continue to receive secukinumab 150mg in PFS (same dose as that was received in core study CAIN457F2306) every 4 weeks up to Week 256."
            }
        ], 
        "brief_summary": {
            "textblock": "This study is designed as a 3-year extension to the phase III core study CAIN457F2306. It\n      aims to provide continuous treatment with secukinumab in pre-filled syringes (PFS) for\n      subjects who completed the core study CAIN457F2306, to obtain further long term efficacy,\n      safety and tolerability information in subjects with active psoriatic arthritis receiving\n      secukinumab every 4 weeks. At Week 104 of the study CAIN457F2306, eligible subjects will\n      complete the assessments associated with the core study visit and will subsequently continue\n      in the extension study on the same dose that they were receiving during the core study. The\n      regular assessments of disease activity will ensure that subjects who are experiencing\n      worsening of disease in any of the treatment groups can exit the study upon their own wish\n      or based on the advice of the investigator at any time."
        }, 
        "brief_title": "Extension Study up to 3 Years for Secukinumab in Psoriatic Arthritis", 
        "completion_date": {
            "#text": "November 2017", 
            "@type": "Anticipated"
        }, 
        "condition": "Psoriatic Arthritis", 
        "condition_browse": {
            "mesh_term": [
                "Arthritis", 
                "Arthritis, Psoriatic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion criteria\n\n          -  Subjects must be able to understand and communicate with the investigator and comply\n             with the requirements of the study and must give a written, signed and dated informed\n             consent before any study assessment is performed\n\n          -  Subjects must have participated in core study CAIN457F2306, and must have completed\n             the entire treatment period\n\n          -  Subjects must be deemed by the investigator to benefit from continued secukinumab\n             therapy\n\n        Exclusion criteria\n\n          -  Any subject taking other concomitant biologic immunomodulating agent(s) except\n             secukinumab\n\n          -  Any subject who is deemed not to be benefiting from the study treatment based upon\n             lack of improvement or worsening of their symptoms\n\n          -  Pregnant or nursing (lactating) women\n\n          -  Women of child-bearing potential, unwilling to use effective contraception during the\n             study and for 16 weeks after stopping treatment\n\n        Other protocol-defined inclusion/exclusion criteria may apply"
            }, 
            "gender": "Both", 

{
    "clinical_study": {
        "@rank": "111215", 
        "arm_group": [
            {
                "arm_group_label": "Group 1", 
                "arm_group_type": "Experimental", 
                "description": "Adding natalizumab monthly infusion to Avonex weekly injection for up to 116 weeks."
            }, 
            {
                "arm_group_label": "Group 2", 
                "arm_group_type": "Placebo Comparator", 
                "description": "Adding placebo monthly infusion to Avonex weekly injection for up to 116 weeks."
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to determine if natalizumab in combination with AVONEX is safe\n      and effective in delaying progression of individuals diagnosed with relapsing remitting\n      Multiple Sclerosis (MS)."
        }, 
        "brief_title": "Safety and Efficacy of Natalizumab in Combination With Avonex in the Treatment of Multiple Sclerosis", 
        "completion_date": {
            "#text": "December 2005", 
            "@type": "Actual"
        }, 
        "condition": "Multiple Sclerosis, Relapsing-Remitting", 
        "condition_browse": {
            "mesh_term": [
                "Multiple Sclerosis", 
                "Sclerosis", 
                "Multiple Sclerosis, Relapsing-Remitting"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Diagnosis of MS as defined by McDonald et al, criteria, # 1- 4\n\n          -  Between the ages of 18 and 55, inclusive\n\n          -  Baseline EDSS score between 0.0 and 5.0, inclusive\n\n          -  Have been treated with Avonex for at least the 12 months prior to randomization\n\n          -  Have experienced at least one relapse (while on Avonex) within the 12 months prior to\n             randomization.\n\n          -  Cranial MRI scan demonstrating lesions consistent with MS.\n\n          -  Have given written informed consent to participate in the study.\n\n        Exclusion Criteria:\n\n          -  Primary progressive, secondary progressive, or progressive relapsing MS.\n\n          -  MS relapse has occurred within 50 days of randomization\n\n          -  A clinically significant infectious illness within 30 days prior to randomization\n\n          -  History of, or abnormal lab result, indicative of significant disease, that in the\n             opinion of the investigator, would preclude the administration of a recombinant\n             humanized antibody immunomodulating agent or Avonex for 116 weeks.\n\n          -  History of severe allergic or anaphylactic reactions or known drug hypersensitivity.\n\n          -  Unable to perform the Timed 25-Foot Walk, 9HPT and PASAT 3\n\n          -  Abnormal blood tests at Screening Visit"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "55 Years", 
            "minimum_age": "18 Years"
        }, 
        "enrollment": {

  <primary_outcome>
    <measure>Inhaled dose</measure>
    <time_frame>after 24 hours</time_frame>
    <safety_issue>No</safety_issue>
    <description>The inhaled dose assessed by residual gravimetric method</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Expiratory wasted dose</measure>
    <time_frame>after 24 hours</time_frame>
    <safety_issue>No</safety_issue>
    <description>The dose expelled in the ambient air through the exhalation port assessed by residual gravimetric method</description>
  </secondary_outcome>
  <other_outcome>
    <measure>Estimated lost dose</measure>
    <time_frame>after 24 hours</time_frame>
    <safety_issue>No</safety_issue>
    <description>The dose lost into the circuit (nebulizer included) assessed by residual gravimetric method</description>
  </other_outcome>

    "clinical_study": {
        "@rank": "9986", 
        "arm_group": {
            "arm_group_label": "Subcutaneous rituximab [MabThera/Rituxan]", 
            "arm_group_type": "Experimental"
        }, 
        "brief_summary": {
            "textblock": "This open-label, single-arm, phase IIIb study will evaluate the safety of switching from\n      intravenous (IV) to subcutaneous (SC) administration of MabThera/Rituxan during first-line\n      treatment for patients with CD20+ non-Hodgkin's follicular lymphoma (FL) or diffuse large\n      B-cell lymphoma (DLBCL) who have already received at least one full dose of MabThera/Rituxan\n      IV. Patients with FL will be given 1400 mg MabThera/Rituxan SC during induction therapy\n      (once monthly for 4-7 cycles) and maintenance therapy (once every 2 months for 6-12 cycles).\n      1400 mg SC of MabThera/Rituxan will be given to patients with DLBCL once monthly for 4-7\n      cycles. Treatment duration is expected to last up to 7 months for patients with DLBCL and up\n      to 32 months for patients with FL."
        }, 
        "brief_title": "MabRella Study: A Study to Evaluate the Safety of Switching From Intravenous to Subcutaneous Administration of Mabthera/Rituxan During First-Line Treatment for Lymphoma.", 
        "completion_date": {
            "#text": "May 2017", 
            "@type": "Anticipated"
        }, 
        "condition": "Lymphoma, Non Hodgkin", 
        "condition_browse": {

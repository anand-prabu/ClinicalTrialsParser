  <brief_title>Safety and Efficacy of Dexamethasone (OZURDEX®) Intravitreal Implant in Korea</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>

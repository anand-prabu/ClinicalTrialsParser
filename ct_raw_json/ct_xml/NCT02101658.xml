  <brief_title>Evaluating Expectancy Effects in Weight Loss Measurement</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Alabama at Birmingham</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>

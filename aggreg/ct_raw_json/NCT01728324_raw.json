{
    "clinical_study": {
        "@rank": "41738", 
        "arm_group": [
            {
                "arm_group_label": "Randomised 24-week arm", 
                "arm_group_type": "Experimental", 
                "description": "BI 207127 in combination with FDV and RBV for 24 weeks (randomised)"
            }, 
            {
                "arm_group_label": "Randomised 16-week arm", 
                "arm_group_type": "Experimental", 
                "description": "BI 207127-placebo, FDV-placebo and RBV-placebo for 8 weeks followed by BI 207127 in combination with FDV and RBV for 16 weeks (randomised)"
            }, 
            {
                "arm_group_label": "Allocated 24-week arm", 
                "arm_group_type": "Experimental", 
                "description": "BI 207127 in combination with FDV and RBV for 24 weeks (allocated to patients with compensated cirrhosis)"
            }
        ], 
        "brief_summary": {
            "textblock": "The aim of the study is to confirm efficacy and safety of treatment with 600 mg of BID BI\n      207127 in combination with 120 mg QD FDV and RBV for 16 or 24 weeks in target chronically\n      infected HCV GT1b treatment na\u00efve patients, including patients with compensated cirrhosis."
        }, 
        "brief_title": "Phase 3 Study of BI 207127 in Combination With Faldaprevir and Ribavirin for Treatment of Patients With Hepatitis C Infection, Including Patients Who Are Not Eligible to Receive Peginterferon: HCVerso2", 
        "completion_date": {
            "#text": "December 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Hepatitis C, Chronic", 
        "condition_browse": {
            "mesh_term": [
                "Hepatitis", 
                "Hepatitis A", 
                "Hepatitis C", 
                "Hepatitis C, Chronic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion criteria:\n\n          1. Chronic hepatitis C infection, diagnosed by positive anti-HCV antibodies and detected\n             HCV RNA at screening\n\n          2. HCV infection of sub-GT1b confirmed by genotypic testing at screening.\n\n          3. HCV viral load =1,000 IU/mL at randomisation.\n\n          4. Patients who have never been previously treated with any other HCV treatment regimen.\n\n        Exclusion criteria:\n\n          1. HCV infection of mixed GT (1/2, 1/3, and 1/4) diagnosed by genotypic testing at\n             screening.\n\n          2. HCV infection of sub-GT1a, mixed GT1a/1b, or undefined GT1.\n\n          3. Liver disease due to causes other than chronic HCV infection.\n\n          4. HIV infection.\n\n          5. Hepatitis B virus infection based on presence of HBs-Ag.\n\n          6. Confirmed or suspected active malignancy or history of malignancy within the last 5\n             years prior to screening.\n\n          7. History of illicit drug abuse other than cannabis or chronic alcohol abuse within 12\n             months prior to randomisation.\n\n          8. Subject is not willing to comply with the precautionary measures to prevent\n             photosensitivity (avoid excessive sun exposure and use sun block on a daily basis).\n\n          9. Decompensated liver disease, or history of decompensated liver disease.\n\n         10. Clinical evidence of unstable cardiovascular disease which may further decompensate\n             due to anemia.\n\n         11. Red blood cell disorders.\n\n         12. Body weight <40 kg or >125 kg."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "75 Years", 
            "minimum_age": "18 Years"
        }, 

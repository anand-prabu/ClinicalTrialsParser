        "secondary_outcome": [
            {
                "measure": "Change from baseline in the AS score of plantar flexors of the other treated body side at Day 29 of the 1st and 2nd injection cycle.", 
                "safety_issue": "No", 
                "time_frame": "Baseline up to week 16-40"
            }, 
            {
                "measure": "Change from baseline in the AS score of plantar flexors at Day 29 of the 2nd injection cycle.", 
                "safety_issue": "No", 
                "time_frame": "Week 12-36 to week 16-40"
            }, 
            {
                "measure": "Changes from baseline in AS score of plantar flexors at Day 57 and Day 85 of the 1st and of the 2nd injection cycle.", 
                "safety_issue": "No", 
                "time_frame": "Baseline up to week 24-48"
            }, 
            {
                "measure": "Changes from baseline in AS score of knee flexors or thigh adductors at Day 29 of the 1st and of the 2nd injection cycle.", 
                "safety_issue": "No", 
                "time_frame": "Baseline up to week 16-40"
            }, 
            {
                "description": "The Modified Tardieu Scale (MTS) assesses spastic muscle tone by subtraction of two angles measured at different conditions of passive muscle stretch. R2 is the angle of passive range of motion with a passive movement at slow speed. R1 is the angle where a \"catch-and-release\" or clonus can be triggered at the fastest possible speed.", 
                "measure": "Changes from baseline in modified Tardieu Scale [MTS] of plantar flexors at Day 29, Day 57, and Day 85 of the 1st and of the 2nd injection cycle.", 
                "safety_issue": "No", 
                "time_frame": "Baseline up to week 24-48"
            }, 
            {
                "description": "The Global Impression of Change Scales (GICS) are global outcomes to assess the impression of change due to treatment. GICS will be assessed by the investigator, by the subject (if feasible) and by parents'/caregiver (if applicable). GICS are 7-Point Likert Scales ranging from +3 (very much improved function) to -3 (very much worse function).", 
                "measure": "Investigator's, Child's/Adolescent's, and Parent's/Caregiver's Global Impression of Change Scale [GICS] at Day 29 of the 1st and 2nd injection cycle.", 
                "safety_issue": "No", 
                "time_frame": "Week 12 up to week 16-40"
            }, 
            {
                "measure": "Investigator's Global Impression of Change of GICS-PF at Day 29 of the 1st and 2nd injection cycle.", 
                "safety_issue": "No", 
                "time_frame": "Week 12 up to week 16-40"
            }, 
            {
                "description": "The GMFM-66 is a standardized observational 66-item instrument designed and validated to measure change in gross motor function over time in subjects with cerebral palsy.", 
                "measure": "Changes from baseline in Gross Motor Function Measure [GMFM]-66 score at the End of 1st Cycle and at the End of Study Visit.", 
                "safety_issue": "No", 
                "time_frame": "Baseline up to week 28-52"
            }, 
            {
                "description": "The QPS is a patient-reported outcome for children and adolescents (2-17 years) with cerebral palsy on spasticity-related pain. Pain intensity (from subjects) and pain frequency (from parent/caregiver) to be assessed with 'Questionnaire on Pain caused by Spasticity [QPS]'.", 
                "measure": "Change in scores of pain intensity (from subjects) and pain frequency (from parent/caregiver) to all post baseline visits of the 1st and of the 2nd injection cycle.", 
                "safety_issue": "No", 
                "time_frame": "Baseline up to week 72"
            }, 
            {
                "measure": "Time to reinjection for each of the three dose groups for the 1st and 2nd injection cycle", 
                "safety_issue": "No", 
                "time_frame": "Baseline up to week 24-72"
            }

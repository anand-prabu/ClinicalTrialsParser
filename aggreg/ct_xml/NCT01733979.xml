  <brief_title>Efficacy and Safety of Heme-Iron Polypeptide on Improvement of Anemia</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Chonbuk National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Chonbuk National University Hospital</source>

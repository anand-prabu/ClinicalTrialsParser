{
    "clinical_study": {
        "@rank": "90873", 
        "arm_group": {
            "arm_group_label": "LGX818", 
            "arm_group_type": "Experimental", 
            "description": "Adult patients, with confirmed diagnosis of BRAF V600E mutant advanced or metastatic NSCLC who have progressed on or after at least one prior systemic anticancer therapy."
        }, 
        "brief_summary": {
            "textblock": "This is an open-label, multi-center, single arm phase II study to evaluate the efficacy and\n      safety of novel BRAF (B-raf murine sarcoma viral oncogene homolog B1) inhibitor encorafenib\n      (LGX818) when used as single agent in patients with advanced or metastatic (stage IIIB or\n      IV) BRAF V600 mutant NSCLC. Patients must have progressed on or after at least one previous\n      systemic, anti-cancer therapy for locally advanced or metastatic NSCLC."
        }, 
        "brief_title": "Efficacy and Safety of LGX818 in Patients With Advanced or Metastatic BRAF V600 Mutant NSCLC", 
        "completion_date": {
            "#text": "September 2017", 
            "@type": "Anticipated"
        }, 
        "condition": "Non-Small Cell Lung Cancer", 
        "condition_browse": {
            "mesh_term": [
                "Carcinoma, Non-Small-Cell Lung", 
                "Lung Neoplasms"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Presence of BRAF V600E mutation in tumor tissue\n\n          -  Histologically or cytologically confirmed diagnosis of Stage IIIB or IV NSCLC\n\n          -  At least one measurable lesion as defined by RECIST v1.1\n\n          -  Patients must have progressed during or after at least one previous systemic,\n             anti-cancer treatment for locally advanced or metastatic NSCLC.\n\n          -  Eastern Cooperative Oncology Group (ECOG)/World Health Organization (WHO) performance\n             status 0-2\n\n        Exclusion Criteria:\n\n          -  Patients with symptomatic Central Nervous System (CNS) metastases\n\n          -  History of leptomeningeal metastases\n\n          -  Prior therapy with a BRAF inhibitor\n\n          -  Patients taking prohibited medication listed in the protocol\n\n          -  Presence or history of a malignant disease other than NSCLC that has been diagnosed\n             and/or required therapy within the past 3 years.\n\n          -  Impaired cardiovascular function or clinically significant cardiovascular diseases\n\n          -  Pregnant or lactating women or woman of childbearing potential"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"
        }, 
        "enrollment": {

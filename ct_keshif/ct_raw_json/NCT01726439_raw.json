{
    "clinical_study": {
        "@rank": "66995", 
        "acronym": "EVOLVE", 
        "arm_group": {
            "arm_group_label": "CHB patients who are naive to NUC treatment", 
            "description": "CHB patients who are naive to NUC at enrollment and be treated at hospitals at tier 2 cities in China"
        }, 
        "biospec_descr": {
            "textblock": "Serum"
        }, 
        "biospec_retention": "Samples With DNA", 
        "brief_summary": {
            "textblock": "To compare the effectiveness, in a real world practice setting in tier 2 cities of China, of\n      Entecavir (ETV) monotherapy and Lamivudine (LAM) based therapies (including LAM monotherapy,\n      de novo LAM + Adefovir [ADV] combination, and early add-on of ADV) among chronic hepatitis B\n      (CHB) patients who are naive to NUC at enrollment to this study"
        }, 
        "brief_title": "Effectiveness of Nucleos(t)Ide Analogs (NUC) Therapy Among Naive CHB Patients in China", 
        "completion_date": {
            "#text": "April 2016", 
            "@type": "Anticipated"
        }, 
        "condition": "Chronic Hepatitis B", 
        "condition_browse": {
            "mesh_term": [
                "Hepatitis", 
                "Hepatitis A", 
                "Hepatitis B", 
                "Hepatitis, Chronic", 
                "Hepatitis B, Chronic"
            ]
        }, 
        "detailed_description": {
            "textblock": "Sampling Method: Consecutive patient sampling\n\n      Biospecimen Retention: Blood samples  for HBV viral load testing along the treatment period\n      of this study"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  CHB patients, or CHB patients with compensated cirrhosis, as defined by the current\n             Chinese guidelines\n\n          -  Male or female\n\n          -  \u2265 18 years of age\n\n          -  Either Hepatitis B e antigen (HBeAg) positive or negative\n\n          -  Na\u00efve to NUC (defined as no previous exposure to NUC treatment as based on patient\n             self-report)\n\n          -  Has compensated liver disease\n\n          -  Patients with compensated cirrhosis\n\n          -  Patients who consent to participate in this study\n\n          -  Local residents with medical reimbursement coverage preferred\n\n        Exclusion Criteria:\n\n          -  Co-infected with hepatitis C virus (HCV)\n\n          -  CHB patients with decompensated cirrhosis, liver failure, hepatocellular carcinoma,\n             or any other types of malignancy at the screening phase\n\n          -  CHB patients who are being treated by interferon therapy within 6 months immediately\n             prior to the screening phase of this study\n\n          -  CHB patients with a confirmed pregnancy"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Hospitals in Chinese tier 2 cities. The definition of these hospitals is the following:\n\n          -  Hospitals where 300 or more CHB patients are treated monthly\n\n          -  Hospitals where PCR can be performed in the hospital's laboratory to measure HBV DNA\n             serum levels"
            }
        }, 

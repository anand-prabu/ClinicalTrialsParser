        "brief_summary": {
            "textblock": "The purpose of the long-terms study is to evaluate the safety, tolerability, and analgesic\n      efficacy of EN3409 in subjects with moderate to severe chronic pain requiring continuous\n      around-the-clock opioid analgesia for an extended period of time."
        }, 
        "brief_title": "Long-term Open-Label Safety Study to Evaluate EN3409", 
        "completion_date": {
            "#text": "June 2015", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Low Back Pain", 
            "Osteoarthritis", 
            "Neuropathic Pain"
        ], 

    "clinical_study": {
        "@rank": "23481", 
        "acronym": "SWITCH 1", 
        "arm_group": [
            {
                "arm_group_label": "IDeg OD + IAsp followed by IGlar OD + IAsp", 
                "arm_group_type": "Experimental", 
                "description": "Each treatment period consists of a 16-week wash-out period and a 16-week maintenance period"
            }, 
            {
                "arm_group_label": "IGlar OD + IAsp followed by IDeg OD + IAsp", 
                "arm_group_type": "Active Comparator", 
                "description": "Each treatment period consists of a 16-week wash-out period and a 16-week maintenance period"
            }
        ], 
        "brief_summary": {
            "textblock": "This trial is conducted in the United States of America (USA). The aim of the trial is to\n      compare the safety and efficacy of insulin degludec (IDeg) and insulin glargine (IGlar),\n      both with insulin aspart (IAsp) as mealtime insulin in subjects with type 1 diabetes."
        }, 
        "brief_title": "A Trial Comparing the Safety and Efficacy of Insulin Degludec and Insulin Glargine, Both With Insulin Aspart as Mealtime Insulin in Subjects With Type 1 Diabetes", 
        "completion_date": {
            "#text": "January 2016", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Diabetes", 
            "Diabetes Mellitus, Type 1"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Diabetes Mellitus", 
                "Diabetes Mellitus, Type 1"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Subjects fulfilling at least one of the below criteria: a) Experienced at least one\n             severe hypo episode within the last year (according to the ADA (American Diabetes\n             Association) definition, April 2013) b) Moderate chronic renal failure, defined as\n             glomerular filtration rate 30 - 59 mL/min/1.73 m^2 per CKD-Epi (chronic kidney\n             disease epidemiology collaboration) c) Hypoglycaemic symptom unawareness d) Diabetes\n             mellitus duration for more than 15 years e) Recent episode of hypoglycaemia (defined\n             by symptoms of hypoglycaemia and/or episode with low glucose measurement (below or\n             equal to 70 mg/dL [below or equal to 3.9 mmol/L])) within the last 12 weeks prior to\n             Visit 1 (screening)\n\n          -  Male or female, age at least 18 years at the time of signing informed consent\n\n          -  Type 1 diabetes mellitus (diagnosed clinically) for at least 52 weeks prior to Visit\n             1\n\n          -  Current treatment with a basal-bolus regimen consisting of neutral protamine Hagedorn\n             (NPH) insulin OD (once daily) / BID (twice daily) or insulin detemir (IDet) OD / BID\n             plus 2-4 daily injections of any rapid acting meal time insulin for at least 26 weeks\n             prior to Visit 1\n\n          -  HbA1c (glycosylated haemoglobin) below or equal to 9% by central laboratory analysis\n\n          -  BMI (body mass index) below or equal to 35 kg/m^2\n\n        Exclusion Criteria:\n\n          -  Treatment with IGlar or IDeg within the last 26 weeks prior to Visit 1\n\n          -  Use of any other anti-diabetic agent than those stated in the inclusion criteria\n             within the last 26 weeks prior to Visit 1"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"
        }, 

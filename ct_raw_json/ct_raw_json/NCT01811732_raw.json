        "arm_group": [
            {
                "arm_group_label": "Delafloxacin plus placebo", 
                "arm_group_type": "Experimental", 
                "description": "300mg iv every 12 hours for  up to 10 to 28 doses"
            }, 
            {
                "arm_group_label": "Vancomycin plus Aztreonam + placebo", 

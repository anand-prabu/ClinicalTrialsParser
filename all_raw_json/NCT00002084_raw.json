{
    "clinical_study": {
        "@rank": "141442", 
        "brief_summary": {
            "textblock": "To determine the safety and efficacy of oral valacyclovir hydrochloride ( 256U87 ) compared\n      to acyclovir in the treatment of recurrent anogenital herpes in HIV-infected patients with\n      CD4 counts = or > 100 cells/mm3."
        }, 
        "brief_title": "A Comparative Trial of Valacyclovir Hydrochloride ( 256U87 ) and Acyclovir for the Suppression of Anogenital Herpes Infections in HIV-Infected Patients", 
        "condition": [
            "Herpes Simplex", 
            "HIV Infections"
        ], 
        "condition_browse": {
            "mesh_term": [
                "HIV Infections", 
                "Acquired Immunodeficiency Syndrome", 
                "Herpes Simplex", 
                "Herpesviridae Infections"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria\n\n        Patients must have:\n\n          -  HIV infection with CD4 counts = or > 100 cells/mm3.\n\n          -  Documented culture of anogenital herpes simplex virus (HSV) within the last 5 years.\n\n          -  History of recurrent anogenital HSV infection, with a recurrence (with or without\n             culture) within 1 year prior to study.\n\n        Exclusion Criteria\n\n        Co-existing Condition:\n\n        Patients with the following symptoms and conditions are excluded:\n\n          -  Malabsorption or vomiting that would potentially limit the retention and absorption\n             of oral therapy.\n\n        Concurrent Medication:\n\n        Excluded:\n\n          -  Systemic antiherpes medication.\n\n          -  Interferon.\n\n        Patients with the following prior conditions are excluded:\n\n        History of hypersensitivity to acyclovir."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

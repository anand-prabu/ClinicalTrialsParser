  <study_design>Allocation: Randomized, Intervention Model: Parallel Assignment, Masking: Open Label, Primary Purpose: Prevention</study_design>
  <primary_outcome>
    <measure>Executive function accuracy</measure>
    <time_frame>Up to 8 months</time_frame>
    <safety_issue>No</safety_issue>
    <description>Executive function (part of cognitive function) measured by a modified computer-based flanker-test</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Academic achievement</measure>
    <time_frame>November 2013 -June 2014</time_frame>
    <safety_issue>No</safety_issue>
    <description>A standardized math test</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Physical activity</measure>
    <time_frame>November 2013-June 2014</time_frame>
    <safety_issue>No</safety_issue>
    <description>Measured with accelerometers for one week both pre- and post-intervention and supplemented with a questionnaire</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Aerobic fitness</measure>
    <time_frame>November 2013 - June 2014</time_frame>
    <safety_issue>No</safety_issue>
    <description>Measured with an intermittent running test (Andersen test)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Anthropometric measures</measure>
    <time_frame>November 2013 - June 2014</time_frame>
    <safety_issue>No</safety_issue>
    <description>Body height, body weight, waist circumference and puberty status (self-assessed)</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Questionnaire</measure>
    <time_frame>November 2013 - June 2014</time_frame>
    <safety_issue>No</safety_issue>
    <description>To students and parents. SES, family diseases, physical activity, sport participation</description>
  </secondary_outcome>
  <other_outcome>
    <measure>SMS tracking</measure>
    <time_frame>November 2013- June 2014</time_frame>
    <safety_issue>No</safety_issue>

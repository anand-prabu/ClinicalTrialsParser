{
    "clinical_study": {
        "@rank": "114429", 
        "acronym": "ABATE", 
        "arm_group": [
            {
                "arm_group_label": "Arm 1: Usual Care", 
                "arm_group_type": "No Intervention", 
                "description": "Routine policy for showering or bathing non-critical care patients"
            }, 
            {
                "arm_group_label": "Arm 2: Decolonization", 
                "arm_group_type": "Active Comparator", 
                "description": "Daily chlorhexidine (CHG) shower or CHG cloth bath for all non-critical care patients.\nTopical intranasal mupirocin ointment (bilateral nares, twice daily) x5 days if non-critical care patients are MRSA+ by history, culture, or screen."
            }
        ], 
        "brief_summary": {
            "textblock": "The ABATE Infection Project is a cluster randomized trial of hospitals to compare two\n      quality improvement strategies to reduce multi-drug resistant organisms and\n      healthcare-associated infections in non-critical care units. The two strategies to be\n      evaluated are:\n\n        -  Arm 1: Routine Care Routine policy for showering/bathing\n\n        -  Arm 2: Decolonization Use of chlorhexidine as routine soap for showering or bed bathing\n           for all patients Mupirocin  x 5 days if MRSA+ by history, culture, or screen\n\n      Note that enrolled \"subjects\" represents 53 individual HCA Hospitals (representing ~190\n      non-critical care units) that have been randomized."
        }, 
        "brief_title": "Active Bathing to Eliminate Infection (ABATE Infection) Trial", 
        "completion_date": {
            "#text": "December 2017", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Healthcare Associated Infections", 
            "Methicillin Resistant Staphylococcus Aureus", 
            "Multi Drug Resistant Organisms"
        ], 
        "condition_browse": {
            "mesh_term": "Staphylococcal Infections"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  All HCA hospitals that reside in the United States\n\n          -  Note: Unit of randomization is the hospital, but the participants are hospital units\n\n        Exclusion Criteria:\n\n          -  Non-critical care units where chlorhexidine bathing or decolonization for MRSA+\n             non-critical care patients is routine\n\n          -  Pediatric, peri-partum, rehabilitation, psychiatry, and BMT units\n\n          -  Units with >30% cardiac or hip/knee orthopedic surgeries\n\n          -  Unit average length of stay <2 days\n\n          -  Patients <12 years-old\n\n          -  Patients with known allergy to mupirocin or chlorhexidine"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

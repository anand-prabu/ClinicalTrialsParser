    "clinical_study": {
        "@rank": "28632", 
        "brief_summary": {
            "textblock": "To investigate the appropriate zalcitabine ( dideoxycytidine; ddC ) dose and zidovudine (\n      AZT ) schedule for use in combination therapy in patients with HIV infection."
        }, 
        "brief_title": "A Randomized, Open-Label Study of Alternative Treatment Combinations of Dideoxycytidine (HIVID; ddC) and Zidovudine (AZT) in Patients With HIV Infection", 
        "condition": "HIV Infections", 
        "condition_browse": {
            "mesh_term": [
                "HIV Infections", 
                "Acquired Immunodeficiency Syndrome"
            ]
        }, 
        "detailed_description": {
            "textblock": "Patients are randomized to one of four treatment arms.  ddC is administered at 1 of 2 doses\n      every 8 hours. AZT is administered at 1 of 2 doses (every 4 hours while awake or every 8\n      hours)."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria\n\n        Patients must have:\n\n          -  HIV infection.\n\n          -  CD4 count 100 - 500 cells/mm3."
            }, 

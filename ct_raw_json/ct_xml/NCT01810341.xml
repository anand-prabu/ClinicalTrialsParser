  <brief_title>SynapDx Autism Gene Expression Analysis Study (STORY)</brief_title>
  <acronym>STORY</acronym>
  <official_title>SynapDx Autism Spectrum Disorder Gene Expression Analysis Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>SynapDx</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>

{
    "clinical_study": {
        "@rank": "124335", 
        "acronym": "EVEREST II", 
        "arm_group": [
            {
                "arm_group_label": "Ranibizumab plus verteporfin PDT", 
                "arm_group_type": "Experimental", 
                "description": "After treatment initiation with combination therapy of ranibizumab and verteporfin PDT, re-treatment need with either ranibizumab alone or combined with verteporfin PDT will be determined at monthly visits based on defined retreatment criteria"
            }, 
            {
                "arm_group_label": "Ranibizumab monotherapy", 
                "arm_group_type": "Active Comparator", 
                "description": "After treatment initiation with combination therapy of ranibizumab and sham PDT, re-treatment need with either ranibizumab alone or combined with sham PDT will be determined at monthly visits based on defined retreatment criteria."
            }
        ], 
        "brief_summary": {
            "textblock": "This study will compare the effect of ranibizumab administered as monotherapy versus\n      ranibizumab administered in combination with verteporfin PDT on visual acuity in patients\n      with symptomatic macular PCV.  The results of this study will provide long-term safety and\n      efficacy data used to generate further guidance on the management of patients with PCV."
        }, 
        "brief_title": "Visual Outcome in Patients With Symptomatic Macular PCV Treated With Either Ranibizumab as Monotherapy or Combined With Verteporfin Photodynamic Therapy.", 
        "completion_date": {
            "#text": "September 2016", 
            "@type": "Anticipated"
        }, 
        "condition": "Age-related Macular Degeneration; Polypoidal Choroidal Vasculopathy (PCV)", 
        "condition_browse": {
            "mesh_term": "Macular Degeneration"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Confirmed diagnosis of symptomatic macular PCV in the study eye\n\n          -  A qualifying vision score at study entry;\n\n          -  A qualifying lesion size in the study eye at study entry\n\n        Exclusion Criteria:\n\n          -  Active inflammation or infection in the study eye\n\n          -  Uncontrolled intraocular pressure in the stuy eye\n\n          -  Ocular condition in the study eye which may impact vision and confound study outcomes\n\n          -  Prior treatment of the study eye with anti-VEGF therapy, verteporfin PDT, other laser\n             and surgical interventions, intraocular corticosteroids"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"
        }, 

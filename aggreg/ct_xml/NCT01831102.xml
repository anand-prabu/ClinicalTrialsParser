  <brief_title>Adiponectin and Ethnic Differences in Insulin Resistance</brief_title>
  <official_title>Adiponectin as an Independent Determinant of Ethnic Differences in Insulin Resistance</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Colorado, Denver</agency>
      <agency_class>Other</agency_class>

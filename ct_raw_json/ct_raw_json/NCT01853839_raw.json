{
    "clinical_study": {
        "@rank": "67120", 
        "brief_summary": {
            "textblock": "This is an out-patient based prospective, multi-centre, observational post-marketing\n      surveillance study amongst internists and cardiologists. In this study, patients with\n      essential hypertension and at least one additional risk factor will be included. Patients\n      may take any antihypertensive treatment which is approved for cardiovascular protection\n      including Micardis 80 mg / Micardis Plus. Patients will be followed over one year in four\n      visits from baseline to endpoint with an additional visit before and after the month of\n      Ramadan."
        }, 
        "brief_title": "Impact of Ramadan on Achieving Joint National Committee (JNC) 7 Treatment Goals in CV Risk Patients", 
        "completion_date": {
            "#text": "June 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Hypertension", 
        "condition_browse": {
            "mesh_term": "Hypertension"
        }, 
        "detailed_description": {
            "textblock": "Purpose:"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion criteria:\n\n          1. Male or female of age 18 years or older;\n\n          2. Newly diagnosed and untreated or previously treated and uncontrolled patients with\n             essential hypertension;\n\n          3. Seated blood pressure of >140/90 mmHg or >130/80 mmHg in patients with diabetes\n             mellitus or chronic kidney disease;\n\n          4. Patients with at least one cardiovascular (cv) risk factor;\n\n          5. Ability to provide written informed consent.\n\n        Exclusion criteria:\n\n          1. Patients with contraindications to the prescribed antihypertensive medications;\n\n          2. Pregnant or lactating women or women of childbearing potential not using an\n             acceptable method of contraception;\n\n          3. Patients who are participating in any other study protocol."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Hypertensive patients wih at least one CV risk factor"
            }
        }, 
        "enrollment": {

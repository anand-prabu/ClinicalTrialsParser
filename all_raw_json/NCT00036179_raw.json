    "clinical_study": {
        "@rank": "133073", 
        "brief_summary": {
            "textblock": "The purpose of the study is to evaluate the safety and efficacy of FK463 in the treatment of\n      patients with confirmed candidemia or invasive candidiasis."
        }, 
        "brief_title": "Study of FK463 in the Treatment of Candidemia or Invasive Candidiasis", 
        "completion_date": {
            "#text": "December 2002", 
            "@type": "Actual"
        }, 
        "condition": "Candidiasis", 
        "condition_browse": {
            "mesh_term": [
                "Candidiasis", 
                "Candidemia", 
                "Candidiasis, Invasive"
            ]
        }, 
        "detailed_description": {
            "textblock": "This will be an open-label, non-comparative study of intravenous FK463.  Enrollment will\n      include at least 100 patients evaluable for efficacy."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria\n\n          -  Has Candidemia or invasive candidiasis documented by typical clinical signs and\n             symptoms and confirmed by fungal culture or histologic confirmation\n\n        Exclusion Criteria\n\n          -  Has abnormal liver test parameters, e.g., AST or ALT > 10 times upper limit of normal\n\n          -  Has life expectancy judged to be less than 5 days\n\n          -  De novo patients who have received a systemic antifungal agent for the treatment of\n             this episode of candidemia or invasive candidiasis for more than 48 hours prior to\n             the first dose of FK463; or efficacy failure patients who have received =< 5 days of\n             prior systemic antifungal therapy for the treatment of this episode of candidemia or\n             invasive candidiasis\n\n          -  Require treatment with topical or systemic antifungal agents for conditions other\n             than candidemia or invasive candidiasis"
            }, 

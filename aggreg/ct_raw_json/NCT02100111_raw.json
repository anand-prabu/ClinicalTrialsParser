    "clinical_study": {
        "@rank": "20157", 
        "arm_group": {
            "arm_group_label": "Patients with Advanced or Metastatic BCC"
        }, 
        "brief_summary": {
            "textblock": "This multi-center, observational study involves reviewing the medical records of\n      approximately 100 adult patients diagnosed with advanced BCC from 01 January 2005 until 31\n      December 2010. All patients will be followed for a minimum of 1 year. Patient records will\n      be analyzed to identify patient characteristics, treatment patterns and clinical outcomes."
        }, 
        "brief_title": "An Analysis of Treatment Patterns and Outcomes for Basal Cell Carcinoma (BCC) Cancer Patients", 
        "completion_date": {
            "#text": "May 2013", 
            "@type": "Actual"
        }, 
        "condition": "Basal Cell Carcinoma", 
        "condition_browse": {
            "mesh_term": [
                "Carcinoma", 
                "Carcinoma, Basal Cell"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Age >/= 18 years\n\n          -  New diagnosis of advanced BCC, defined as (1) locally advanced BCC (inoperable as\n             determined by the site investigator or surgery contraindicated) with receipt of\n             radiation therapy (unless radiation was contraindicated), or (2) metastatic BCC, from\n             01 January 2005 through 31 December 2010\n\n        Exclusion Criteria:\n\n          -  Participation in an interventional trial that dictated treatment during the study\n             time period (01 January 2005 - 31 December 2010), or within 90 days prior to\n             individual patients' study enrollment dates\n\n          -  Treatment with a Hedgehog pathway inhibitor"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Patients with locally advanced or metastatic basal cell carcinoma"
            }

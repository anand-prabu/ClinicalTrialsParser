  <brief_title>Intraarticular Xylitol Injections for Knee Osteoarthritis</brief_title>
  <official_title>Intraarticular Xylitol Injections for Knee Osteoarthritis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Hadassah Medical Organization</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

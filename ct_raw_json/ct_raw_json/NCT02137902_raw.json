    "clinical_study": {
        "@rank": "5041", 
        "arm_group": [
            {
                "arm_group_label": "Tobacco/Physical Activity Intervention", 
                "arm_group_type": "Experimental", 
                "description": "Includes a psychosocial component that utilizes a counselor-facing computer-assisted intervention with tailored counseling feedback focused on increasing intrinsic motivation, goal setting for tobacco and physical activity, adherence with nicotine replacement therapy, and self-monitoring with a pedometer-based walking program. The intervention will provide 12 weeks of nicotine replacement therapy for participants."
            }, 
            {
                "arm_group_label": "Hypertension and Hypercholesterolemia", 
                "arm_group_type": "Active Comparator", 
                "description": "Consists of a psychosocial component that includes counselor-facing computer-assisted intervention with tailored counseling feedback focused on in creasing intrinsic motivation, goal setting for managing hypertension and hypercholesterolemia, and adherence with antihypertensives and statins with supportive dietary changes."
            }
        ], 
        "brief_summary": {
            "textblock": "This study aims to identify effective and cost-effective interventions for tobacco use and\n      other risk behaviors for cardiovascular disease among Alaska Native people in rural\n      villages. In a randomized controlled trial, the study will compare interventions using\n      telemedicine to promote the American Heart Association's identified ideal health behaviors\n      (nonsmoking and physical activity) relative to ideal health factors (managing cholesterol\n      and blood pressure)."
        }, 
        "brief_title": "Technology Innovations for Supporting Health in Alaska Native (AN) People", 

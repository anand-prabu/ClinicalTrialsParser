{
    "clinical_study": {
        "@rank": "26851", 
        "arm_group": [
            {
                "arm_group_label": "IPI-145", 
                "arm_group_type": "Experimental", 
                "description": "IPI-145 is administered orally and supplied as 5 mg and 25 mg formulated capsules."
            }, 
            {
                "arm_group_label": "Ofatumumab", 
                "arm_group_type": "Active Comparator", 
                "description": "Ofatumumab is administered as an intravenous (IV) infusion and is supplied in single-use vials at two strengths, 100 mg/5mL and 1000 mg/50 mL"
            }
        ], 
        "brief_summary": {
            "textblock": "A Phase 3 (extension) clinical trial to examine the efficacy of IPI 145 monotherapy or\n      ofatumumab monotherapy in subjects with Chronic Lymphocytic Leukemia (CLL) or Small\n      Lymphocytic Lymphoma (SLL) who experienced disease progression after treatment with IPI-145\n      or Ofatumumab in Study IPI-145-07."
        }, 
        "brief_title": "A Study of IPI-145 and Ofatumumab in Patients With CLL/SLL Previously Enrolled in Study IPI-145-07", 
        "completion_date": {
            "#text": "December 2019", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Chronic Lymphocytic Leukemia", 
            "Small Lymphocytic Lymphoma"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Leukemia", 
                "Leukemia, Lymphocytic, Chronic, B-Cell", 
                "Leukemia, Lymphoid", 
                "Lymphoma"
            ]
        }, 
        "detailed_description": {
            "textblock": "This is an open-label, two- arm, extension study designed to enable subjects who experience\n      radiologic disease progression in Study IPI-145-07 to receive the alternative treatment\n      (either IPI-145 or ofatumumab) than what was received during study IPI-145-07.\n\n      Subjects who have previously received ofatumumab in study IPI-145-07 will receive a starting\n      dose of 25 mg IPI-145 BID continuously in a 21 day cycle for Cycle 1 followed by 28- day\n      treatment cycles thereafter for up to 12 cycles.\n\n      Subjects who have previously received IPI-145 in study IPI-145-07 will receive a starting\n      dose of 300 mg ofatumumab on Day 1 followed by seven weekly doses of 2000 mg. Thereafter,\n      subjects will receive 2000 mg ofatumumab once every month for four months unless disease\n      progression or unacceptable toxicity occurs. Administration of ofatumumab will not exceed\n      the 12 doses (within 7 cycles)."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Received either IPI-145 or ofatumumab while participating in study IPI-145-07 and\n             experienced radiologic disease progression\n\n          -  Diagnosis of active CLL or SLL that meets at least 1 of the International Workshop on\n             Chronic Lymphocytic Leukemia (IWCLL) 2008 criteria for requiring treatment\n\n          -  Measurable nodal disease by CT\n\n          -  Eastern Cooperative Oncology Group (ECOG) performance status of 0-2\n\n        Exclusion Criteria:\n\n          -  Discontinued participation in Infinity- sponsored IPI-145-07 study for reasons other\n             than disease progression\n\n          -  History of Richter's transformation or prolymphocytic leukemia\n\n          -  Uncontrolled autoimmune hemolytic anemia (AIHA) or idiopathic thrombocytopenia (ITP)\n\n          -  Prior allogeneic transplant\n\n          -  Known central nervous system lymphoma or leukemia\n\n          -  Use of any anticancer medication other than IPI-145 or ofatumumab within 1 week of\n             enrollment\n\n          -  Ongoing systemic bacterial, fungal, or viral infections\n\n          -  Prior, current, or chronic hepatitis B or hepatitis C infection"
            }, 

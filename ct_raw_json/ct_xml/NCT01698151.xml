  <primary_outcome>
    <measure>Hospitalization</measure>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">2000</enrollment>
  <condition>Chronic Obstructive Pulmonary Disease (COPD)</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Patients in primary care on the Fyn island, Denmark
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  40 years old or more,

          -  diagnosis of COPD in primary care

        Exclusion Criteria:

          -  Major psychiatric illness,

          -  inability to visit primary care doctor,

          -  inability to understand danish language,
      </textblock>
    </criteria>
    <gender>Both</gender>
    <minimum_age>40 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>

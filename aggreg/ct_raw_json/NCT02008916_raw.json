        "@rank": "28514", 
        "arm_group": [
            {
                "arm_group_label": "Secukinumab 10 mg/kg i.v. / 300 mg s.c.", 
                "arm_group_type": "Experimental", 
                "description": "Three i.v. infusions: at Baseline and weeks 2 and 4, followed by one s.c. injection every four weeks until the end of the study."
            }, 
            {
                "arm_group_label": "Secukinumab 10 mg/kg i.v. / 150 mg s.c.", 

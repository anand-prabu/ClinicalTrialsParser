    "clinical_study": {
        "@rank": "38762", 
        "brief_summary": {
            "textblock": "The purpose of this study is to find out how well an experimental drug called DepoCyt works\n      for neoplastic meningitis (cancer that has spread to the tissues around the brain and spinal\n      cord). DepoCyt is a new slow-release form of the cancer drug called ara-C (cytarabine).\n      Cytarabine has been used for many years to treat cancer."
        }, 
        "brief_title": "DepoCyt Therapy in Patients With Neoplastic Meningitis From Lymphoma or a Solid Tumor", 
        "completion_date": "November 2004", 
        "condition": "Meningeal Neoplasms", 

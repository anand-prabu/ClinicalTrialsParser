    "clinical_study": {
        "@rank": "62266", 
        "arm_group": {
            "arm_group_label": "Lubiprostone", 
            "arm_group_type": "Experimental", 
            "description": "Lubiprostone 12 or 24 mcg twice daily (BID)"
        }, 
        "brief_summary": {
            "textblock": "The purpose of this study is to assess the long-term safety, efficacy, and pharmacokinetics\n      of oral lubiprostone 12 or 24 mcg capsules dosed twice daily (BID) when administered orally\n      for 36 weeks in paediatric subjects with functional constipation."
        }, 
        "brief_title": "Long-term Safety and Efficacy of Lubiprostone in Paediatric Subjects Aged \u2265 6 Years to < 18 Years With Functional Constipation", 
        "completion_date": {
            "#text": "December 2016", 
            "@type": "Anticipated"
        }, 
        "condition": "Constipation - Functional", 
        "condition_browse": {
            "mesh_term": "Constipation"
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Completed 12 weeks of blinded treatment in the SAG/0211PFC-1131 study.\n\n          -  Continue to abstain from taking concomitant medication (prescribed or\n             over-the-counter) that affects gastrointestinal motility.\n\n        Exclusion Criteria:\n\n          -  Untreated faecal impaction at the time of rolling over into study.\n\n          -  Significant change in medical status, newly diagnosed and uncontrolled\n             cardiovascular, liver or lung disease, neurologic or psychiatric disorder, or other\n             systemic disease.\n\n          -  Demonstrated non-compliance with study protocol during the SAG/0211PFC-1131 study."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "17 Years", 
            "minimum_age": "6 Years"
        }, 

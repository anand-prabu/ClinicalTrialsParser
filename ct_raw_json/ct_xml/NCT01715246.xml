  <brief_title>Clinical Safety of a New Infant Starter Formula Containing 2 Human Milk Oligosaccharides (HMOs)</brief_title>
  <official_title>Clinical Safety of a New Infant Starter Formula Containing 2 Human Milk Oligosaccharides (HMOs)</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nestlé</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Nestlé</source>
  <oversight_info>

{
    "clinical_study": {
        "@rank": "83910", 
        "arm_group": [
            {
                "arm_group_label": "Treatment  Arm 1", 
                "arm_group_type": "Experimental", 
                "description": "Subjects randomized to study drug will take 600 mg of lumacaftor once daily (qd) and 250 mg of ivacaftor every 12 hours (q12h)"
            }, 
            {
                "arm_group_label": "Treatment Arm 2", 
                "arm_group_type": "Experimental", 
                "description": "Subjects randomized to study drug will take 400 mg of lumacaftor every 12 hours (q12h) and 250 mg of ivacaftor every 12 hours (q12h)"
            }, 
            {
                "arm_group_label": "Treatment Arm 3", 
                "arm_group_type": "Experimental", 
                "description": "Subjects randomized to placebo will remain on placebo through 24 weeks. Subjects will be given tablets that match both lumacaftor and ivacaftor and will follow the same dosing regimen as subjects receiving the study drug"
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to evaluate the efficacy and safety of lumacaftor in\n      combination with ivacaftor in persons 12 years and older with Cystic Fibrosis who are\n      homozygous for the F508del mutation."
        }, 
        "brief_title": "A Study of Lumacaftor in Combination With Ivacaftor in Cystic Fibrosis Subjects Aged 12 Years and Older Who Are Homozygous for the F508del-CFTR Mutation (TRANSPORT)", 
        "condition": "Cystic Fibrosis, Homozygous for the F508del CFTR Mutation", 
        "condition_browse": {
            "mesh_term": [
                "Cystic Fibrosis", 
                "Fibrosis"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Males and females, aged 12 years or older on the date of informed consent or, where\n             appropriate, date of assent\n\n          -  Confirmed diagnosis of CF\n\n          -  Homozygous for the F508del CFTR mutation\n\n          -  FEV1 \u226540% and \u226490% of predicted normal for age, sex, and height\n\n          -  Willing to remain on a stable CF medication regimen through Week 24 or, if\n             applicable, the Safety Follow up Visit\n\n        Exclusion Criteria:\n\n          -  An acute upper or lower respiratory infection, pulmonary exacerbation, or changes in\n             therapy (including antibiotics) for pulmonary disease within 4 weeks before first\n             dose of study drug\n\n          -  History of solid organ or hematological transplantation\n\n          -  History of alcohol or drug abuse in the past year\n\n          -  Ongoing or prior participation in an investigational drug study (including studies\n             investigating lumacaftor and/or ivacaftor) within 30 days of screening.\n\n          -  Use of strong inhibitors, moderate inducers, or strong inducers of CYP3A , including\n             consumption of certain herbal medications (e.g., St. John's Wort) and certain fruit\n             and fruit juices within 14 days before Day 1 of dosing"
            }, 

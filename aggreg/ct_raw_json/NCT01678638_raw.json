        "arm_group": [
            {
                "arm_group_label": "Early inguinal hernia (IH) repair", 
                "arm_group_type": "Active Comparator", 
                "description": "IH repair before NICU discharge"
            }, 
            {
                "arm_group_label": "Late inguinal hernia (IH) repair", 
                "arm_group_type": "Active Comparator", 
                "description": "IH repair at 55-60 weeks post-menstrual age"
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to determine whether early (before NICU discharge) or late\n      (55-60 weeks post-menstrual age) inguinal hernia repair is safer for premature infants who\n      have an inguinal hernia."
        }, 
        "brief_title": "Timing of Inguinal Hernia Repair in Premature Infants", 
        "completion_date": {
            "#text": "June 2018", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Inguinal Hernia", 
            "Prematurity"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Hernia", 
                "Hernia, Inguinal"
            ]
        }, 
        "detailed_description": {
            "textblock": "This is a randomized clinical trial comparing early versus late repair in premature infants\n      with an inguinal hernia (IH) to determine which approach may be safer. Safety in this trial\n      is defined as the freedom from significant adverse events, a reduction in hospital days\n      during the study period, and normal neurodevelopmental testing at 2 years. Costs of each\n      treatment strategy are also important and are being evaluated."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Infant with estimated gestational age < 37 weeks, 0 days\n\n          -  In a NICU at participating site\n\n          -  Diagnosed with an IH per the pediatric surgery team\n\n          -  Parents and providers willing to randomize the infant\n\n        Exclusion Criteria:\n\n          -  Associated factor that impacts timing of IH repair  (e.g. clinical factors might\n             preclude early IH repair)\n\n          -  Infant is undergoing another operative procedure and IH repair is planned as a\n             secondary procedure (e.g. fundoplication or G tube is planned, and IH repair is\n             considered a secondary procedure)\n\n          -  Known major congenital anomaly that impact neurodevelopmental outcome or chromosomal\n             abnormality\n\n          -  Family unable / unwilling to return for follow up and later IH repair; or likely\n             unable to monitor IH as outpatient"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

  <brief_title>Effect of Folic Acid, Vitamin B6 and Vitamin B12 in Diabetic Retinopathy</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Catania</agency>
      <agency_class>Other</agency_class>

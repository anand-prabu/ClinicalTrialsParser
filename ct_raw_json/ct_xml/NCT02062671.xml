  <brief_title>Renal Sympathetic Denervation for Reduction of Pain and Improvement of Insulin Sensitivity in Adult Polycystic Kidney Disease</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Odense University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

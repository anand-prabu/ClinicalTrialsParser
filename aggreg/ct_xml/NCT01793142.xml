  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Viviant</intervention_name>
    <description>Viviant (Bazedoxifene) 20mg once daily</description>
    <arm_group_label>Viviant treatment group</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Postmenopausal osteoporosis and osteopenia patients
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Postmenopausal osteoporosis and osteopenia patients

        Exclusion Criteria:

          -  Patients with active or past history of venous thromboembolic events including deep
             vein thrombosis,

          -  Patients with pulmonary embolism and retinal vein thrombosis
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>19 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>

        "brief_summary": {
            "textblock": "To examine the safety and efficacy of tofacitinib in subjects with active psoriatic\n      arthritis who have previously had an inadequate response to at least one TNF inhibitor\n      either due to lack of efficacy or an adverse event."
        }, 
        "brief_title": "Tofacitinib In Psoriatic Arthritis Subjects With Inadequate Response to TNF Inhibitors", 
        "completion_date": {
            "#text": "August 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Psoriatic Arthritis", 
        "condition_browse": {
            "mesh_term": [
                "Arthritis", 
                "Arthritis, Psoriatic"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Active arthritis at screening/baseline as indicated by >/= 3 tender/painful and 3\n             swollen joints\n\n          -  Active plaque psoriasis at screening\n\n          -  Inadequate efficacy or lack of toleration to previously administered TNF inhibitor\n\n        Exclusion Criteria:\n\n          -  Non-plaque forms of psoriasis (with exception of nail psoriasis)\n\n          -  History of autoimmune rheumatic disease other than PsA; also prior history of or\n             current, rheumatic inflammatory disease other than PsA"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 

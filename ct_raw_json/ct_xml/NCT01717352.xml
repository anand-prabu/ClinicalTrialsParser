  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  age 21 to 65

          -  BMI 25 to 45

          -  sleep 7 hours or less most nights

        Exclusion Criteria:

          -  use of medications affecting sleep

          -  sleep apnea

          -  shift work
      </textblock>

  <brief_title>Median Study for Biochemical Serum Markers in First Trimester of Pregnancy</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>PerkinElmer, Wallac Oy</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>

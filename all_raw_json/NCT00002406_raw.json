{
    "clinical_study": {
        "@rank": "10139", 
        "brief_summary": {
            "textblock": "The purpose of this study is to determine if two dose levels of indinavir combined with two\n      nucleoside analogue reverse transcriptase inhibitors (NRTIs) have the same effect on plasma\n      viral load (level of HIV in the blood)."
        }, 
        "brief_title": "A Comparison of Two Dose Levels of Indinavir Combined With Two Nucleoside Analogue Reverse Transcriptase Inhibitors (NRTIs) in HIV-Infected Patients", 
        "condition": "HIV Infections", 
        "condition_browse": {
            "mesh_term": [
                "HIV Infections", 
                "Acquired Immunodeficiency Syndrome"
            ]
        }, 
        "detailed_description": {
            "textblock": "In this open-label study 350 seropositive HIV-1 men and women are first stratified according\n      to baseline plasma viral RNA (less than 400 copies/mL vs negative plasma viral RNA result)\n      then randomized into one of two arms:\n\n      Arm 1: Indinavir (800 mg, q8h) plus two pre-existing NRTIs. Arm 2: Indinavir (1,200 mg,\n      q12h) plus two pre-existing NRTIs. Plasma viral RNA will be measured every 4 weeks for the\n      duration of the 24-week study."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria\n\n        Patients must have:\n\n          -  Documented HIV-1 seropositive status.\n\n          -  CD4 count greater than 100 cells/mm3.\n\n          -  Parental consent for patients under 18."
            }, 
            "gender": "Both", 

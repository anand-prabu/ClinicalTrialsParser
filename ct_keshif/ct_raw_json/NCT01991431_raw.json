        "@rank": "53729", 
        "acronym": "ROUTE", 
        "arm_group": {
            "arm_group_label": "TAVI", 
            "description": "All Patients undergoing transaortic transcatheter valve implantation with commercially available Edwards SAPIEN XT Transcatheter Heart Valve with the Ascendra+ Delivery-System in participating sites"
        }, 
        "brief_summary": {
            "textblock": "This is an international multi-center, prospective, observational registry with consecutive\n      patient enrollment intended to determine outcome Parameters within 30 days after transaortic\n      transcatheter aortic valve replacement."
        }, 
        "brief_title": "ROUTE Registry Of the Utilisation of the Transaortic (Tao) TAVI Approach Using the Edwards Sapien XT Valve", 
        "completion_date": {
            "#text": "September 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Aortic Stenosis", 
        "condition_browse": {
            "mesh_term": "Aortic Valve Stenosis"
        }, 
        "detailed_description": {
            "textblock": "The purpose of this registry is to expand upon existing data sets, to identify patient\n      characteristics and indicators related to complications and clinical benefits for patients\n      with symptomatic severe calcific degenerative aortic stenosis that are undergoing\n      transaortic transcatheter procedure with the commercially available Edwards SAPIEN XT\n      Transcatheter Heart Valve with the Ascendra+ System."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Intended transaortic (Tao) TAVI using Edwards SAPIEN XT Transcatheter Heart Valve\n             with the Ascendra+ Delivery-System\n\n          -  Compliance with the indications according to the Instructions for Use\n\n          -  Written informed consent\n\n        Exclusion Criteria:\n\n          -  Presence of contraindications as to the Instructions for Use\n\n          -  TAo with concomitant procedure (e.g. Tao + CABG)\n\n          -  Participation in the SOURCE XT registry"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "All Patients undergoing transaortic transcatheter valve implantation with commercially\n        available Edwards SAPIEN XT Transcatheter Heart Valve with the Ascendra+ Delivery-System\n        in participating sites"
            }
        }, 

  <brief_title>Influence of Erythropoietin on Bone Marrow Microenvironment</brief_title>
  <official_title>Influence of Erythropoietin on Bone Marrow Microenvironment: A Pilot Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Kantonsspital Baselland Bruderholz</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

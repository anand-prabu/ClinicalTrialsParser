{
    "clinical_study": {
        "@rank": "60219", 
        "acronym": "PSR-REVERSE", 
        "arm_group": {
            "arm_group_label": "Medtronic CRT-D w/Expanded Indications", 
            "description": "Patients implanted with a Medtronic CRT-D device who meet the expanded indication criteria: ICD indicated patients which are New York Heart Association (NYHA) Class II, with a Left Ventricular Ejection Fraction (LVEF) \u2264 30%, 130 ms \u2264 QRS \u2264 150 ms, and Left Bundle Branch Block (LBBB) morphology."
        }, 
        "brief_summary": {
            "textblock": "The purpose of the REVERSE Post Approval Study (PAS) is to confirm the benefit observed in\n      the REVERSE and RAFT pivotal studies in \"real-world\" clinical practice."
        }, 
        "brief_title": "The Product Surveillance Registry REVERSE Post Approval Study", 
        "condition": "Heart Failure", 
        "condition_browse": {
            "mesh_term": [
                "Heart Failure", 
                "Ventricular Dysfunction, Left", 
                "Ventricular Dysfunction"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Patient or legally authorized representative provides written authorization and/or\n             consent per institution and geographical requirements\n\n          -  Patient has or is intended to receive or be treated with a Medtronic CRT-D who meets\n             the expanded CRT-D indication criteria with a QRS \u2264 150ms\n\n          -  Patient within 30 days of implant\n\n        Exclusion Criteria:\n\n          -  Patient who is, or will be, inaccessible for follow-up\n\n          -  Patient with exclusion criteria required by local law\n\n          -  Patient is currently enrolled in or plans to enroll in any concurrent drug and/or\n             device study that may confound results"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "N/A", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Heart failure patients that are implanted with a Medtronic CRT-D device and meet the\n        expanded indication criteria."
            }
        }, 
        "enrollment": {
            "#text": "500", 
            "@type": "Anticipated"
        }, 
        "firstreceived_date": "August 6, 2012", 
        "has_expanded_access": "No", 
        "id_info": {
            "nct_id": "NCT01660035", 
            "org_study_id": "PSR-REVERSE"
        }, 
        "intervention": {
            "arm_group_label": "Medtronic CRT-D w/Expanded Indications", 
            "description": "Medtronic CRT-D devices are multi-programmable defibrillators with biventricular pacing features for cardiac resynchronization. These systems automatically detect and treat episodes of ventricular fibrillation (VF), ventricular tachycardia (VT), fast ventricular tachycardia (FVT), and bradyarrhythmia. When a cardiac arrhythmia is detected, the implantable devices deliver fibrillation, cardioversion, anti-tachycardia pacing or standard pacing therapy. They also provide biventricular pacing for cardiac resynchronization to improve hemodynamics for patients with heart failure who have ventricular conduction disturbances such as interventricular conduction delays.", 
            "intervention_name": "Medtronic Cardiac Resynchronization Therapy (CRT-D)", 
            "intervention_type": "Device"
        }, 
        "is_fda_regulated": "Yes", 
        "is_section_801": "Yes", 
        "lastchanged_date": "May 20, 2014", 

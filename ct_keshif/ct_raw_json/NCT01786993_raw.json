        "acronym": "MPP IDE", 
        "arm_group": [
            {
                "arm_group_label": "Multi-point pacing arm", 
                "arm_group_type": "Experimental", 
                "description": "MultiPoint Pacing"
            }, 
            {
                "arm_group_label": "Biventricular arm", 

{
    "clinical_study": {
        "@rank": "168097", 
        "acronym": "NUTRIREA2", 
        "arm_group": [
            {
                "arm_group_label": "Parenteral nutrition", 
                "arm_group_type": "Other", 
                "description": "Patients will receive parenteral nutrition during the first week of mechanical ventilation. After Day 3, the parenteral route may be switched to the enteral route if shock resolve (vasoactive drug stopped since 24 hours and serum lactate level < 2 mmol/l). After Day 7, all patients will be fed via the enteral route."
            }, 
            {
                "arm_group_label": "Enteral nutrition", 
                "arm_group_type": "Other", 
                "description": "Patients will receive nutrition only via the enteral route during the firs week of invasive mechanical ventilation."
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to assess the hypothesis that, as compared to early intravenous\n      feeding, early nutrition via the enteral route is associated with reduced Day 28-mortality\n      in critically ill patients treated with mechanical ventilation and vasoactive drug."
        }, 
        "brief_title": "Impact of Early Enteral vs. Parenteral Nutrition on Mortality in Patients Requiring Mechanical Ventilation and Catecholamines", 
        "completion_date": {
            "#text": "June 2016", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Acute Respiratory Failure", 
            "Shock"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Shock", 
                "Respiratory Distress Syndrome, Adult", 
                "Respiratory Insufficiency"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Invasive mechanical ventilation expected to be required more than 48 hours\n\n          -  Nutrition started within 24 hours after initiation of endotracheal mechanical\n             ventilation\n\n          -  Treatment with vasoactive drug administered via a central venous catheter\n\n          -  Age over 18 years\n\n          -  Signed information\n\n        Exclusion Criteria:\n\n          -  Abdominal surgery within 1 month before inclusion\n\n          -  History of esophageal, gastric, duodenal or pancreatic surgery\n\n          -  Bleeding from the esophagus, stomach or bowel\n\n          -  enteral nutrition via gastrostomy or jejunostomy\n\n          -  pregnancy\n\n          -  Treatment-limitation decisions\n\n          -  Current inclusion in a trial on comparison between enteral and parenteral nutrition"
            }, 
            "gender": "Both", 

{
    "clinical_study": {
        "@rank": "166372", 
        "arm_group": [
            {
                "arm_group_label": "Retro-articular drilling", 
                "arm_group_type": "Active Comparator", 
                "description": "Retro-articular drilling goes through the cortical margin of the affected condyle, thereby sparing the articular surface and physes."
            }, 
            {
                "arm_group_label": "Trans-articular drilling", 
                "arm_group_type": "Active Comparator", 
                "description": "Trans-articular drilling penetrates the articular cartilage through multiple sites to create subchondral penetrations."
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to compare the functional, clinical and radiographic outcomes\n      associated with trans-articular drilling versus retro-articular drilling, two commonly\n      employed techniques of operative treatment for stable forms of juvenile osteochondritis\n      dissecans (JOCD) lesions. This study also aims to better define the natural history of this\n      condition in its most commonly identified pathological state (as a stable lesion) following\n      surgical intervention by determining the rate of radiographic healing and any need for\n      secondary surgery."
        }, 
        "brief_title": "Study of Two Surgical Drilling Techniques to Treat Juvenile Osteochondritis Dissecans of the Knee", 
        "completion_date": {
            "#text": "November 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Juvenile Osteochondritis Dissecans", 
        "condition_browse": {
            "mesh_term": [
                "Osteochondritis", 
                "Osteochondritis Dissecans", 
                "Osteochondrosis"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Diagnosis of JOCD,\n\n          -  Lesion located on the lateral aspect of the medial femoral condyle,\n\n          -  Lesion considered stable based on MRI,\n\n          -  Patient deemed skeletally immature based on MRI,\n\n          -  Completed a course of conservative therapy.\n\n        Exclusion Criteria:\n\n          -  Lesion healed sufficiently and surgery is not recommended,\n\n          -  Prior surgery on the affected knee,\n\n          -  Diagnosis of metabolic bone disorder (e.g. osteogenesis imperfecta),\n\n          -  Diagnosis of sickle cell disease,\n\n          -  History of prolonged corticosteroid or chemotherapy treatment,"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "18 Years", 
            "minimum_age": "8 Years"
        }, 
        "enrollment": {

            "mesh_term": [
                "Hypertension, Pulmonary", 
                "Hypertension"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  The patient should have been diagnosed with Pulmonary Arterial Hypertension\n\n          -  The patient has received or will receive treatment with the implanted pump LenusPro\n\n        Exclusion Criteria:\n\n          -  The patient is participating in another study which means treatment of the patient no\n             longer follows the normal clinical practice"
            }, 

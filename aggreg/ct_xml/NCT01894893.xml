  <brief_title>Atlas of Human Milk Nutrients</brief_title>
  <official_title>Atlas of Human Milk Nutrients</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Nestlé</agency>
      <agency_class>Industry</agency_class>

{
    "clinical_study": {
        "@rank": "36630", 
        "arm_group": [
            {
                "arm_group_label": "Arm A: Idelalisib+ofatumumab", 
                "arm_group_type": "Experimental", 
                "description": "Participants will receive idelalisib plus ofatumumab for 24 weeks."
            }, 
            {
                "arm_group_label": "Arm B: Ofatumumab", 
                "arm_group_type": "Active Comparator", 
                "description": "Participants will receive ofatumumab for 24 weeks."
            }
        ], 
        "brief_summary": {
            "textblock": "This study is to evaluate the effect of the addition of idelalisib (GS-1101) to ofatumumab\n      on progression-free survival (PFS) in participants with previously treated chronic\n      lymphocytic leukemia (CLL)."
        }, 
        "brief_title": "Efficacy and Safety of Idelalisib (GS-1101; CAL-101) in Combination With Ofatumumab for Previously Treated Chronic Lymphocytic Leukemia", 
        "completion_date": {
            "#text": "November 2016", 
            "@type": "Anticipated"
        }, 
        "condition": "Chronic Lymphocytic Leukemia", 
        "condition_browse": {
            "mesh_term": [
                "Leukemia", 
                "Leukemia, Lymphocytic, Chronic, B-Cell", 
                "Leukemia, Lymphoid"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Adult subjects with previously treated recurrent CLL who have measurable\n             lymphadenopathy\n\n          -  Require therapy for CLL\n\n          -  Have experienced CLL progression <24 months since the completion of the last prior\n             therapy\n\n          -  Have disease that is not refractory to ofatumumab"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"
        }, 

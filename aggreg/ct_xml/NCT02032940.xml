  <brief_title>Non-Enhanced Magnetic Resonance Angiography With the t-SLIP Technique</brief_title>
  <acronym>MRA tSLIP</acronym>
  <official_title>Non-Enhanced Magnetic Resonance Angiography With the t-SLIP Technique</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Miami Cardiac and Vascular Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>

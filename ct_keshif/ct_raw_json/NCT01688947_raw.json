        "brief_summary": {
            "textblock": "The primary objective of this study is to evaluate the analgesic efficacy of 2 dose levels\n      of V116517 versus placebo."
        }, 
        "brief_title": "Analgesic Efficacy and Safety of V116517 in Subjects With Moderate to Severe Chronic Pain Due to Postherpetic Neuralgia (PHN)", 
        "completion_date": {
            "#text": "April 2014", 
            "@type": "Actual"
        }, 
        "condition": [
            "Postherpetic Neuralgia", 
            "Pain"
        ], 

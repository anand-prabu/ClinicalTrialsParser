{
    "clinical_study": {
        "@rank": "104481", 
        "brief_summary": {
            "textblock": "The purpose of this study is to determine whether sivelestat will reduce the amount of time\n      a patient must spend on a ventilator and/or increase the chance of survival of patients with\n      acute lung injury."
        }, 
        "brief_title": "A Phase II Study to Determine the Efficacy and Safety of Sivelestat in Subjects With Acute Lung Injury", 
        "completion_date": "December 2002", 
        "condition": [
            "Respiratory Distress Syndrome, Adult", 
            "Acute Lung Injury"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Respiratory Distress Syndrome, Newborn", 
                "Respiratory Distress Syndrome, Adult", 
                "Acute Lung Injury", 
                "Lung Injury", 
                "Wounds and Injuries"
            ]
        }, 
        "detailed_description": {
            "textblock": "Acute lung injury often develops as a result of severe infection, sepsis, severe injuries,\n      inhalation of foreign substances into the lungs, or pneumonia.  The tissues and blood\n      vessels in the lungs become inflamed, and the body does not receive enough oxygen.  Patients\n      with this condition are placed on a ventilator to assist with breathing.\n\n      Patients entered into this study will be randomly assigned to one of two treatment groups:\n      a sivelestat group or a placebo group.  Once entered into the study, patients are monitored\n      for up to 6 months."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria\n\n          -  show evidence of acute lung injury\n\n          -  be on mechanical ventilation\n\n        Exclusion Criteria\n\n          -  have undergone certain organ transplants\n\n          -  have severe underlying medical problems\n\n          -  be unlikely to survive\n\n          -  be pregnant or breast-feeding"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "18 Years"

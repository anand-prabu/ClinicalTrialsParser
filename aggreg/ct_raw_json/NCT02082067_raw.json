    "clinical_study": {
        "@rank": "114445", 
        "arm_group": [
            {
                "arm_group_label": "Adductor canal", 
                "arm_group_type": "Experimental", 
                "description": "All patients in the arm will receive continuous adductor canal block under ultrasound guidance. 10ml of Ropivacaine 0,75%, BBraun, Germany, will be injected to dilate adductor canal. After catheter insertion 10ml of Ropivacaine 0,75% will be injected."
            }, 
            {
                "arm_group_label": "Femoral", 
                "arm_group_type": "Active Comparator", 
                "description": "All patients in the arm will receive continuous femoral nerve block under ultrasound guidance. Correct position of catheter will be check with injection of 20ml Ropivacaine 0,75% BBraun, Germany medial to femoral nerve."
            }, 
            {
                "arm_group_label": "Controls", 
                "arm_group_type": "Placebo Comparator", 
                "description": "No patients of this arm receive continuous nerve block. Intravenous opioids analgesia will be administered."
            }
        ], 
        "brief_summary": {
            "textblock": "Aim of this study is to evaluate the effects of the adductor canal block on the early and\n      medium term ( 1 month) rehabilitation compared to the continuous femoral block commonly used\n      in the knee prosthesis."
        }, 
        "brief_title": "Total Knee Prosthesis With Continuous Block of Adductor Canal and Outcome", 
        "completion_date": {

    "clinical_study": {
        "@rank": "151243", 
        "brief_summary": {
            "textblock": "Seventeen hospitals in China will participate in the study, which aims to provide data on\n      serum bilirubin levels in the first 168 hr after birth in term and late-preterm neonates,\n      and estimate the incidence of severe neonatal hyperbilirubinemia and the underlying causes.\n\n      We hypothesize that the study can be value in identifying and implementing strategies for\n      risk reduction."
        }, 
        "brief_title": "Multicenter Prospective Study on the Incidence of Neonatal Hyperbilirubinemia in Term and Late-preterm Chinese Infants", 
        "completion_date": {
            "#text": "January 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Hyperbilirubinemia, Neonatal", 
        "condition_browse": {
            "mesh_term": [
                "Hyperbilirubinemia", 
                "Hyperbilirubinemia, Neonatal"
            ]
        }, 
        "detailed_description": {
            "textblock": "Neonatal hyperbilirubinemia resulting in clinical jaundice is a common problem among\n      infants, particularly during the first weeks of life.\n\n      Information about the incidence of neonatal jaundice is not available from China where is\n      one of the highest countries of the incidence of severe neonatal hyperbilirubinemia."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n        Term or late-preterm newborn infants with GAs of \u226535 weeks and birth weights of \u22652,000 g\n        were included.\n\n        Exclusion Criteria:\n\n        All sick newborn infants who were admitted to the intensive care unit."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "Accepts Healthy Volunteers", 
            "maximum_age": "28 Days", 

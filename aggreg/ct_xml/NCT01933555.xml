  <brief_title>Impact of Women's Empowerment Program for Abused Pregnant Women</brief_title>
  <official_title>Impact of Women's Empowerment Program for Abused Pregnant Women: A Randomized Controlled Trial</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Macmillan Research Group UK</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>

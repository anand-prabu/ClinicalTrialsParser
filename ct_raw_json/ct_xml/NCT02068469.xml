  <primary_outcome>
    <measure>Evaluate sensitivity and specificity of the Liat Strep A assay when compared to culture</measure>
    <time_frame>Results based on samples collected at initial visit</time_frame>
    <safety_issue>No</safety_issue>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>

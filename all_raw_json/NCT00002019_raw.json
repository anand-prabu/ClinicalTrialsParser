    "clinical_study": {
        "@rank": "31991", 
        "brief_summary": {
            "textblock": "To evaluate the safety, tolerance and efficacy of three different dosage regimens of\n      Amphotericin B Lipid Complex (ABLC) compared to Fungizone (Amphotericin B) in patients with\n      AIDS and cryptococcal meningitis."
        }, 
        "brief_title": "Safety and Efficacy of Amphotericin B Lipid Complex in the Treatment of Cryptococcal Meningitis in Patients With the Acquired Immunodeficiency Syndrome", 
        "condition": [
            "Meningitis, Cryptococcal", 
            "HIV Infections"
        ], 
        "condition_browse": {
            "mesh_term": [

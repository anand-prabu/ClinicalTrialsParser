  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Spinal cord injury

          -  Minimum time since injury: 3 months

        Exclusion Criteria:

          -  Participation in resistance training programmes in the previous 6 months

          -  Creatine supplementation in the previous 6 months
      </textblock>

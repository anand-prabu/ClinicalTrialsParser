{
    "clinical_study": {
        "@rank": "46837", 
        "arm_group": [
            {
                "arm_group_label": "Canagliflozin 100 mg", 
                "arm_group_type": "Experimental", 
                "description": "Participants will receive one 100 mg canagliflozin capsule before the morning meal and one matching placebo capsule with the evening meal plus placebo tablets with the evening meal (to match the metformin XR tablets administered in other treatment arms) for 26 weeks."
            }, 
            {
                "arm_group_label": "Canagliflozin 300 mg", 
                "arm_group_type": "Experimental", 
                "description": "Participants will receive one 300 mg canagliflozin capsule before the morning meal and one matching placebo capsule with the evening meal plus placebo tablets with the evening meal (to match the metformin XR tablets administered in other treatment arms) for 26 weeks."
            }, 
            {
                "arm_group_label": "Metformin XR", 
                "arm_group_type": "Experimental", 
                "description": "Participants will receive metformin XR tablets (in doses titrated over 9 weeks) once daily with the evening meal, plus one placebo capsule before the morning meal and one placebo capsule with the evening meal (to match the canagliflozin capsules administered in other treatment arms) for 26 weeks."
            }, 
            {
                "arm_group_label": "Canagliflozin 100 mg + Metformin XR", 
                "arm_group_type": "Experimental", 
                "description": "Participants will receive one 100 mg canagliflozin capsule with the evening meal and one matching placebo capsule before the morning meal plus metformin XR tablets (in doses titrated over 9 weeks) once daily with the evening meal for 26 weeks."
            }, 
            {
                "arm_group_label": "Canagliflozin 300 mg + Metformin XR", 
                "arm_group_type": "Experimental", 
                "description": "Participants will receive one 300 mg canagliflozin capsule with the evening meal and one matching placebo capsule before the morning meal plus metformin XR tablets (in doses titrated over 9 weeks) once daily with the evening meal for 26 weeks."
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to assess the effectiveness of the co-administration of\n      canagliflozin and metformin extended release (XR) compared with canagliflozin alone, and\n      metformin XR alone in patients with type 2 diabetes mellitus with inadequate control despite\n      treatment with diet and exercise. The safety and tolerability of canagliflozin will also be\n      assessed."
        }, 
        "brief_title": "A Study to Evaluate the Effectiveness, Safety, and Tolerability of Canagliflozin in Combination With Metformin in the Treatment of Patients With Type 2 Diabetes Mellitus With Inadequate Glycemic Control With Diet and Exercise", 
        "completion_date": {
            "#text": "December 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Diabetes Mellitus, Type 2", 
        "condition_browse": {
            "mesh_term": [
                "Diabetes Mellitus", 
                "Diabetes Mellitus, Type 2"
            ]
        }, 
        "detailed_description": {
            "textblock": "This study will be a randomized (the study medication is assigned by chance), double-blind\n      (neither physician nor participant knows the identity of the assigned treatment),\n      active-controlled (one of the treatments is an established effective treatment for type 2\n      diabetes mellitus), parallel-group (each group of participants will be treated at the same\n      time), 5-arm (groups), multicenter study. Approximately 1,200 participants will be randomly\n      assigned to the 5 treatment arms in a 1:1:1:1:1 ratio for 26 weeks."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Must have type 2 diabetes mellitus with inadequate glycemic control on diet and\n             exercise\n\n          -  Not on antihyperglycemic agent therapy (at least 12 weeks before screening) and have\n             a screening visit fingerstick glycated hemoglobin (HbA1c) of more than or equal to 7\n             percent and less than or equal to 12.5 percent\n\n          -  Have a screening visit HbA1c of more than or equal to 7.5 percent and less than or\n             equal to 12 percent as determined by the central laboratory\n\n          -  Must have a fasting plasma glucose of less than or equal to 300 mg/dL (16.7 mmol/L)\n             prior to randomization\n\n          -  Must have a fasting fingerstick glucose of greater than 120 mg/dL (6.7 mmol/L)\n             performed at home or at the study center prior to randomization\n\n        Exclusion Criteria:\n\n          -  History of diabetic ketoacidosis, type 1 diabetes mellitus (T1DM), pancreas or\n             beta-cell transplantation, or diabetes secondary to pancreatitis or pancreatectomy\n\n          -  Fasting C-peptide less than 0.70 ng/mL (0.23 nmol/L) in participants for whom the\n             investigator cannot reasonably exclude T1DM based upon clinical evaluation\n\n          -  Repeated (2 or more over a 1 week period) fasting self-monitored blood glucose\n             measurements more than 300 mg/dL (16.7 mmol/L) prior to randomization, despite\n             reinforcement of diet and exercise counseling\n\n          -  History of hereditary glucose-galactose malabsorption or primary renal glucosuria\n\n          -  Has history of, or currently active, illness considered to be clinically significant\n             by the Investigator or any other illness that the Investigator considers should\n             exclude the patient from the study or that could interfere with the interpretation of\n             the study results"
            }, 

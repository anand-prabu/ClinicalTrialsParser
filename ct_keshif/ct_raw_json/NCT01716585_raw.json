            }
        ], 
        "brief_summary": {
            "textblock": "A study to evaluate chronic hepatitis C infection."
        }, 
        "brief_title": "A Study to Evaluate Chronic Hepatitis C Infection", 
        "completion_date": {
            "#text": "September 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Chronic Hepatitis C Infection", 
        "condition_browse": {
            "mesh_term": [
                "Hepatitis", 
                "Hepatitis A", 
                "Hepatitis, Chronic", 
                "Hepatitis C", 
                "Hepatitis C, Chronic"
            ]
        }, 
        "detailed_description": {
            "textblock": "The purpose of this study is to evaluate the safety and efficacy of ABT-450, ritonavir and\n      ABT-267 (ABT-450/r/ABT-267)  and ABT-333 co-administered with ribavirin in hepatitis C virus\n      genotype 1 infected treatment-na\u00efve adults (SAPPHIRE-I)."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Females must be post-menopausal for more than 2 years or surgically sterile or\n             practicing specific forms of birth control\n\n          -  Chronic hepatitis C, genotype 1-infection (HCV RNA level greater than 10,000 IU/mL at\n             screening)\n\n          -  Subject has never received antiviral treatment for hepatitis C infection\n\n          -  No evidence of liver cirrhosis\n\n        Exclusion Criteria:\n\n          -  Positive screen for drugs or alcohol\n\n          -  Significant sensitivity to any drug\n\n          -  Use of contraindicated medications within 2 weeks of dosing\n\n          -  Abnormal laboratory tests\n\n          -  Positive hepatitis B surface antigen and anti-Human Immunodeficiency Virus Antibody"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "70 Years", 
            "minimum_age": "18 Years"
        }, 

        "arm_group": [
            {
                "arm_group_label": "Adjuvant Chemotherapy", 
                "arm_group_type": "Experimental", 
                "description": "Paclitaxel: 175 mg/m(2) intravenously (IV); followed by Paraplatin (Carboplatin Injection) AUC=5 IV. 3-6 cycles as necessary."
            }, 
            {
                "arm_group_label": "Adjuvant Radiotherapy", 
                "arm_group_type": "Active Comparator", 
                "description": "Histopathological grade G3 and <50% myometrial invasion: Vaginal brachytherapy 5Gy, 3 times;\nHistopathological grade G3 and vascular space involvement: Pelvic radiation 45-50 Gy;\n\u226550% myometrial invasion: Pelvic radiation 50 Gy + Vaginal brachytherapy 5Gy, 2-4 times."
            }
        ], 
        "brief_summary": {
            "textblock": "This randomized trial is studying the efficacy and safety of the chemotherapy compared with\n      radiation therapy alone as adjuvant treatment after operation in Patients with high risk and\n      Stage I endometrial carcinoma."
        }, 
        "brief_title": "The Efficacy and Safety of the Postoperative Adjuvant Treatment in Patients With High-risk Stage I Endometrial Carcinoma", 
        "condition": "Endometrial Neoplasms", 
        "condition_browse": {

        "brief_summary": {
            "textblock": "The purpose of the S-ICD Post Approval Study is to document long term safety and\n      effectiveness outcomes associated with the implantation of the SQ-RX pulse generator and\n      Q-TRAK electrode in a commercial clinical setting."
        }, 
        "brief_title": "S-ICD\u00ae System Post Approval Study", 
        "completion_date": {
            "#text": "October 2020", 
            "@type": "Anticipated"
        }, 
        "condition": [
            "Primary Prevention of Sudden Cardiac Arrest", 
            "Secondary Prevention of Sudden Cardiac Arrest"
        ], 
        "condition_browse": {

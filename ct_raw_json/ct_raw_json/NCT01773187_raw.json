        }, 
        "responsible_party": {
            "responsible_party_type": "Sponsor"
        }, 
        "secondary_outcome": {
            "description": "To compare pacritinib with best available therapy with respect to the proportion of patients with >= 50% reduction in total score from baseline to week 24 on the Myeloproliferative Neoplasm Symptom Assessment Form (MPN-SAF TSS)", 
            "measure": "Symptomatic Efficacy", 
            "safety_issue": "No", 
            "time_frame": "Baseline to week 24"
        }, 

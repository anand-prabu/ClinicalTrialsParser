    "clinical_study": {
        "@rank": "38760", 
        "brief_summary": {
            "textblock": "A multi-center, double-blind, placebo-controlled out-patient, safety, tolerability, and\n      pilot efficacy study of intramuscular AN-1792 in patients with mild to moderate Alzheimer's\n      disease."
        }, 
        "brief_title": "Randomized Safety, Tolerability and Pilot Efficacy of AN-1792 in Alzheimer's Disease", 
        "completion_date": "September 2003", 
        "condition": "Alzheimer's Disease", 
        "condition_browse": {
            "mesh_term": "Alzheimer Disease"
        }, 
        "detailed_description": {
            "textblock": "The study will enroll approximately 375 patients with mild to moderate Alzheimer's disease\n      at investigational sites in the United States and Europe.  Patients will receive either\n      AN-1792 or placebo, and they will be evaluated using standard clinical assessments of\n      cognition and memory as well as experimental surrogate markers of Alzheimer's disease\n      pathology.  The goal of the study is to evaluate the clinical impact of eliciting an immune\n      response (formation of antibodies) to the A-beta peptide in patients with Alzheimer's\n      disease."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Mild to moderate Alzheimer's disease. Ability to cooperate with MRI scanning and\n        neuropsychological testing. Live at home or in the community and a caregiver capable of\n        accompanying the patient on all clinic visits and visiting the patient at least 5 times\n        per week."
            }, 

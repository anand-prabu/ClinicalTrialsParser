  <brief_title>Attain Performa(TM) Quadripolar Lead Study</brief_title>
  <official_title>Attain Performa(TM) Quadripolar Lead Study</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Medtronic Cardiac Rhythm Disease Management</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>

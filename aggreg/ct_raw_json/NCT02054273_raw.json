    "clinical_study": {
        "@rank": "124497", 
        "brief_summary": {
            "textblock": "The objectives of this study is to assess the characteristics and frequency of adenovirus\n      conjunctivitis in a population of male and female patients from one year of age who present\n      signs and symptoms of acute conjunctivitis."
        }, 
        "brief_title": "ADenoVirus Initiative Study in Epidemiology in Germany", 
        "completion_date": {
            "#text": "October 2014", 
            "@type": "Anticipated"
        }, 
        "condition": "Viral Conjunctivitis", 
        "condition_browse": {
            "mesh_term": [
                "Conjunctivitis", 
                "Conjunctivitis, Viral", 
                "Conjunctivitis, Inclusion"
            ]
        }, 
        "detailed_description": {
            "textblock": "Epidemiological, prospective, open, multicentric trial in patients suffering from acute\n      conjunctivitis who will undergo a rapid diagnostic test for adenoviral conjunctivitis at the\n      time of their visit to the ophthalmologist office, at the emergency room or during\n      hospitalization. This epidemiological study will include approximately 500 patients in\n      Germany. The planned duration of the study is one year from the first patient visit to the\n      last patient visit."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Male or Female patients of at least one year of age presenting acute (for less than 7\n             days) conjunctivitis signs and symptoms.\n\n          -  Patient or / and legal representative (if patient below 18 years of age) must be able\n             to provide oral consent for the collection and treatment of the questionnaire data.\n\n          -  No history of hypersensitivity to corn starch, talcum powder or Dacron (sampling\n             fleece components).\n\n        Exclusion Criteria:\n\n          -  Current use or having received (within the last 7 days) local antiviral therapies\n             such as povidone iodine or ganciclovir, or topical steroids or immunomodulators such\n             as cyclosporine which may interfere with the test result.\n\n          -  Previous enrolment in the present study.\n\n          -  Any direct involvement with the study conduct at site or any family link with study\n             site staff."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "N/A", 
            "minimum_age": "1 Year", 
            "sampling_method": "Non-Probability Sample", 
            "study_pop": {
                "textblock": "Any female and male patients from one year of age with signs and symptoms of acute\n        conjunctivitis either seen during ophthalmology consultations or who are in the emergency\n        room or hospitalized, can be enrolled in the study"
            }
        }, 

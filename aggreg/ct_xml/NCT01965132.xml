  <brief_title>Registry of Korean Patients With Rheumatic Disease Using Biologics</brief_title>
  <acronym>KOBIO</acronym>
  <official_title>Prospective Cohort Study of Korean Patients With Rheumatic Disease Using Biologic Agents</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Seoul National University Hospital</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Seoul National University Hospital</source>

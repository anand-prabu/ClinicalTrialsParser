    "clinical_study": {
        "@rank": "153128", 
        "arm_group": {
            "arm_group_label": "Synagis Immuno Compromised Children", 
            "description": "Patients receiving palivizumab for the prevention of RSV lower respiratory tract infection on proper use after the approval of the indication are to be enrolled."
        }, 
        "brief_summary": {
            "textblock": "This is a post marketing observational study (PMOS) conducted in Japan in patients receiving\n      palivizumab for the prevention of Respiratory Syncytial Virus (RSV) lower respiratory tract\n      infection at the age of 24 months and under, who have immunocompromised medical condition\n      (e.g., combined immunodeficiency disease, antibody deficiency, or other types of\n      immunodeficiency; HIV infection; recovering from organ or bone marrow transplantation; on\n      chemotherapy; on high-dose corticosteroid therapy; on immunosuppressants) or who have Downs\n      syndrome."
        }, 
        "brief_title": "Synagis\u00ae Liquid 50mg, 100mg for Intramuscular Injection Special Investigation in Immuno Compromised Children With Synagis", 
        "completion_date": {
            "#text": "September 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Respiratory Syncytial Virus Infection", 
        "condition_browse": {
            "mesh_term": [
                "Respiratory Syncytial Virus Infections", 
                "Virus Diseases"
            ]
        }, 
        "detailed_description": {
            "textblock": "Palivizumab will be prescribed according to the local label and independently of the\n      decision to enroll the subject in the study. Palivizumab will be administered monthly (30\n      days) throughout the Respiratory Syncytial Virus (RSV) infection season. Survey forms will\n      be collected after the observation period. The number of adverse events and the frequency of\n      hospitalizations due to RSV infections in surveyed patients will be counted to evaluate the\n      safety and efficacy of palivizumab."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          1. Patients receiving palivizumab for prevention of RSV lower respiratory tract\n             infection on proper use after the approval of the indication are to be enrolled.\n\n          2. Children, at the age of 24 months and under, who have an immunocompromised medical\n             condition.\n\n          3. Children, at the age of 24 months and under, who have Down's syndrome.\n\n        Exclusion Criteria:\n\n          1. Patients included in \"Contraindication\" in the package insert.\n\n          2. Patients with known hypersensitivity to the ingredients of palivizumab.\n\n          3. Patients with known positive RSV infection before hospitalization.\n\n          4. A life expectancy less than 1 year."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

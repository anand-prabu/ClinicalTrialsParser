    "clinical_study": {
        "@rank": "106045", 
        "arm_group": [
            {
                "arm_group_label": "RBP-7000 90 mg", 
                "arm_group_type": "Experimental", 
                "description": "RBP-7000 administered as a 90 mg subcutaneous injection on Days 1 and 29 for a total of two injections."
            }, 
            {
                "arm_group_label": "RBP-7000 120 mg", 
                "arm_group_type": "Experimental", 
                "description": "RBP-7000 administered as a 120 mg subcutaneous injection on Days 1 and 29 for a total of two injections."
            }, 
            {
                "arm_group_label": "Placebo", 
                "arm_group_type": "Placebo Comparator", 
                "description": "Placebo administered by subcutaneous injection on Days 1 and 29 for a total of two injections."
            }
        ], 
        "brief_summary": {
            "textblock": "The purpose of this study is to evaluate the efficacy and safety of RBP-7000 compared with\n      placebo in the treatment of patients with schizophrenia."
        }, 
        "brief_title": "Randomized, Double-blind, Placebo Controlled, Multi-center Trial of Efficacy, Safety and Tolerability of RBP-7000 in Schizophrenia Patients", 
        "completion_date": {
            "#text": "July 2015", 
            "@type": "Anticipated"
        }, 
        "condition": "Schizophrenia", 
        "condition_browse": {
            "mesh_term": "Schizophrenia"
        }, 
        "detailed_description": {
            "textblock": "This will be a double-blind, placebo-controlled, Phase III study with 90 mg and 120 mg doses\n      of RBP-7000 compared with placebo over an 8-week treatment period."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Males and females between the ages of 18 to 55 years, inclusive\n\n          -  Diagnosis of schizophrenia as defined by Diagnostic and Statistical Manual,\n             Edition 4, text revision (DSM-IV-TR) criteria\n\n          -  Subjects who are deemed \"valid\" by the State, Assessability, Face, Ecological, and\n             Rule (SAFER) interview\n\n          -  Subjects who are otherwise healthy on the basis of their physical examination\n\n        Exclusion Criteria:\n\n          -  Subjects who have an improvement in their total Positive and Negative Syndrome Scale\n             (PANSS) score of 20% or greater between the initial screening visit and the first day\n             of treatment.\n\n          -  Subjects taking daily oral risperidone at a dose  \u2265 6 mg/day\n\n          -  Subjects who have received a depot antipsychotic within 120 days of screen\n\n          -  Subjects with treatment resistant schizophrenia, as judged by the investigator, who\n             have been treated with antipsychotics for adequate durations and with adequate\n             dosages."
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "55 Years", 
            "minimum_age": "18 Years"
        }, 
        "enrollment": {
            "#text": "339", 
            "@type": "Anticipated"
        }, 
        "firstreceived_date": "March 19, 2014", 
        "has_expanded_access": "No", 

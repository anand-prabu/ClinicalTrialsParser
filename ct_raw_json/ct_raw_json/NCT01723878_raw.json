    "clinical_study": {
        "@rank": "132666", 
        "arm_group": {
            "arm_group_label": "Cohort"
        }, 
        "brief_summary": {
            "textblock": "This observational study will evaluate the patterns of disease progression in patients with\n      locally advanced or metastatic non-small cell lung cancer (NSCLC) harbouring EGFR activating\n      mutations receiving Tarceva (erlotinib) as first-line treatment. Patients will be followed\n      for up to 12 months after progression of disease."
        }, 
        "brief_title": "ASPET Study: An Observational Study of Tarceva (Erlotinib) in First Line in Patients With Advanced Non-Small Cell Lung Cancer Harbouring EGFR Activating Mutations", 
        "completion_date": {
            "#text": "January 2017", 
            "@type": "Anticipated"
        }, 
        "condition": "Non-Small Cell Lung Cancer", 
        "condition_browse": {
            "mesh_term": [
                "Carcinoma, Non-Small-Cell Lung", 
                "Lung Neoplasms"
            ]
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Adult patients, >/= 18 years of age\n\n          -  Histologically or cytologically confirmed locally advanced or metastatic (IIIB/IV)\n             non-squamous non-small cell lung cancer (NSCLC) which is routinely evaluated in\n             clinical practice according to RECIST criteria\n\n          -  EGFR mutation-positive disease according to local laboratory testing\n\n          -  Patients on treatment with Tarceva monotherapy as first-line treatment according to\n             the physician's usual practice\n\n        Exclusion Criteria:\n\n          -  Prior systemic therapy for advanced NSCLC. The administration of neoadjuvant or\n             adjuvant chemotherapy is allowed as long as it was finalized >6 months prior to\n             receiving Tarceva\n\n          -  Participation in another clinical study\n\n          -  Patients could have received radiotherapy as long as the irradiated lesion was not\n             the only lesion for evaluating response and a long as the radiotherapy was completed\n             before initiating Tarceva treatment"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 

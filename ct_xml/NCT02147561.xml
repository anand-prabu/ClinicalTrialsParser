  <brief_title>A Safety and Efficacy Study of BOTOX® in Korean Adults With Chronic Migraine</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>Allergan</source>

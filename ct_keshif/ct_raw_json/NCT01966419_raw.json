        "brief_summary": {
            "textblock": "The purpose of this study is to determine whether ornithine phenylacetate can speed recovery\n      from an acute hepatic encephalopathy episode requiring hospitalization in cirrhotic\n      patients."
        }, 
        "brief_title": "Phase 2B Efficacy/Safety of Ornithine Phenylacetate in Hospitalized Cirrhotic Patients With Hepatic Encephalopathy (STOP-HE)", 
        "condition": [
            "Acute Episode of Overt Hepatic Encephalopathy", 
            "Hepatic Encephalopathy"
        ], 
        "condition_browse": {
            "mesh_term": [
                "Hepatic Encephalopathy", 
                "Brain Damage, Chronic", 
                "Delirium", 
                "Encephalitis", 
                "Neurotoxicity Syndromes", 
                "Hyperammonemia"
            ]
        }, 
        "detailed_description": {
            "textblock": "This is a multicenter, randomized, double-blind, placebo-controlled study of ornithine\n      phenylacetate (OCR-002) administered intravenously to hospitalized patients with cirrhosis,\n      hyperammonemia, and an acute episode of hepatic encephalopathy administered on top of\n      standard of care treatment."
        }, 
        "eligibility": {
            "criteria": {
                "textblock": "Inclusion Criteria:\n\n          -  Hospitalized with an acute episode of hepatic encephalopathy as complication of\n             cirrhosis\n\n          -  Elevated venous ammonia\n\n        Exclusion Criteria:\n\n          -  Renal failure with serum creatinine > 3 mg/dL or need for dialysis\n\n          -  Molecular Adsorbent Recirculation System utilized\n\n          -  Pregnancy or breastfeeding"
            }, 
            "gender": "Both", 
            "healthy_volunteers": "No", 
            "maximum_age": "75 Years", 
            "minimum_age": "18 Years"
